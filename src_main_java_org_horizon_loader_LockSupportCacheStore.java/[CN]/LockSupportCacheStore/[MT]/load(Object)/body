{
  if (log.isTraceEnabled())   log.trace("load (" + key + ")");
  String lockingKey=getLockFromKey(key);
  lockForReading(lockingKey);
  try {
    return loadLockSafe(key,lockingKey);
  }
  finally {
    unlock(lockingKey);
    if (log.isTraceEnabled())     log.trace("Exit load (" + key + ")");
  }
}
