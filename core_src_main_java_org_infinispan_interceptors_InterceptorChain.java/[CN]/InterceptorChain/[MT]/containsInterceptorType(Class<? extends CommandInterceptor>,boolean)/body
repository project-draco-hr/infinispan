{
  CommandInterceptor it=firstInChain;
  while (it != null) {
    if (alsoMatchSubClasses) {
      if (interceptorType.isAssignableFrom(it.getClass()))       return true;
    }
 else {
      if (it.getClass().equals(interceptorType))       return true;
    }
    it=it.getNext();
  }
  return false;
}
