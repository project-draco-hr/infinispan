{
  try {
    return command.acceptVisitor(ctx,firstInChain);
  }
 catch (  CacheException e) {
    if (e.getCause() instanceof InterruptedException)     Thread.currentThread().interrupt();
    throw e;
  }
catch (  RuntimeException e) {
    throw e;
  }
catch (  Throwable t) {
    throw new CacheException(t);
  }
}
