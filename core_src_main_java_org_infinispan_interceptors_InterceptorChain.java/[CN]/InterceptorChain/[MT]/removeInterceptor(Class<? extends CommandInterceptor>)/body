{
  if (firstInChain.getClass() == clazz) {
    firstInChain=firstInChain.getNext();
  }
  CommandInterceptor it=firstInChain.getNext();
  CommandInterceptor prevIt=firstInChain;
  while (it != null) {
    if (it.getClass() == clazz) {
      prevIt.setNext(it.getNext());
    }
    prevIt=it;
    it=it.getNext();
  }
}
