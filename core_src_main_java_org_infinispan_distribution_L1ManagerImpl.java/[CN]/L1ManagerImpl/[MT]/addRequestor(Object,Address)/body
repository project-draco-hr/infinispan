{
  Collection<Address> as=requestors.get(key);
  if (as == null) {
    as=new ConcurrentHashSet<Address>();
    as.add(origin);
    Collection<Address> previousAs=requestors.putIfAbsent(key,as);
    if (previousAs != null) {
      previousAs.add(origin);
    }
  }
 else {
    as.add(origin);
  }
}
