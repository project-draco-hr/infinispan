{
  Collection<Address> addresses=new HashSet<Address>();
  for (  Object key : keys) {
    Collection<Address> as=requestors.remove(key);
    if (as != null) {
      addresses.addAll(as);
      if (origin != null && as.contains(origin))       addRequestor(key,origin);
    }
  }
  if (origin != null)   addresses.remove(origin);
  return addresses;
}
