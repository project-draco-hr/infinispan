{
  File[] filesInRoot=rootDirectory.listFiles();
  for (  File maybeDirectory : filesInRoot) {
    if (maybeDirectory.isDirectory()) {
      String name=maybeDirectory.getName();
      try {
        getDirectory(name);
      }
 catch (      PersistenceException e) {
        log.couldNotWalkDirectory(name,e);
      }
    }
  }
}
