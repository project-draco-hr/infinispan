{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  TransportConfiguration other=(TransportConfiguration)obj;
  if (attributes == null) {
    if (other.attributes != null)     return false;
  }
 else   if (!attributes.equals(other.attributes))   return false;
  if (remoteCommandThreadPool == null) {
    if (other.remoteCommandThreadPool != null)     return false;
  }
 else   if (!remoteCommandThreadPool.equals(other.remoteCommandThreadPool))   return false;
  if (totalOrderThreadPool == null) {
    if (other.totalOrderThreadPool != null)     return false;
  }
 else   if (!totalOrderThreadPool.equals(other.totalOrderThreadPool))   return false;
  if (transportThreadPool == null) {
    if (other.transportThreadPool != null)     return false;
  }
 else   if (!transportThreadPool.equals(other.transportThreadPool))   return false;
  return true;
}
