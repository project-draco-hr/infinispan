{
  servers=new LinkedList<Address>();
  int numServers=5;
  for (int i=0; i < numServers; i++) {
    servers.add(new TestAddress(i));
  }
  ch=(DefaultConsistentHash)BaseDistFunctionalTest.createNewConsistentHash(servers);
