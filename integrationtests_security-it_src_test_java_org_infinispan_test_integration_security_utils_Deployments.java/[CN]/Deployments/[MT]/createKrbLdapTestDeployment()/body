{
  WebArchive war=ShrinkWrap.create(WebArchive.class).addAsLibraries(new File("target/test-libs/infinispan-core.jar"),new File("target/test-libs/infinispan-commons.jar"),new File("target/test-libs/jboss-marshalling.jar"),new File("target/test-libs/jboss-marshalling-river.jar")).addAsWebInfResource(new File("target/test-classes/jboss-deployment-structure.xml")).addPackage(Deployments.class.getPackage()).addPackage(AbstractAuthentication.class.getPackage());
  return war;
}
