{
  sdi=new SlowDownInterceptor();
  Configuration config=new Configuration().fluent().loaders().addCacheLoader(new DummyInMemoryCacheStore.Cfg()).customInterceptors().add(sdi).after(InvocationContextInterceptor.class).transaction().transactionMode(TransactionMode.NON_TRANSACTIONAL).build();
  return TestCacheManagerFactory.createCacheManager(config);
}
