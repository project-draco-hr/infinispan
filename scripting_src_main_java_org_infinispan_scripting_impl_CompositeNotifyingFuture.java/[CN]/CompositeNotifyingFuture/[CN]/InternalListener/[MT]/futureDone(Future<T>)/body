{
synchronized (this) {
    Throwable error=null;
    System.err.println("Waiting on future " + future.toString());
    try {
      results.add(future.get());
    }
 catch (    Throwable e) {
      error=e;
    }
    remaining.countDown();
    if (remaining.getCount() == 0) {
      if (error != null) {
        notifyException(error);
      }
 else {
        notifyDone(results);
      }
      setFuture(new NoOpFuture<>(results));
    }
  }
}
