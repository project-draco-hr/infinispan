{
  Directory dir=DirectoryBuilder.newDirectoryInstance(cache,cache,cache,INDEX_NAME).chunkSize(BUFFER_SIZE).overrideWriteLocker(new LockFactory(){
    @Override public Lock makeLock(    String lockName){
      return null;
    }
    @Override public void clearLock(    String lockName) throws IOException {
    }
  }
).create();
  Assert.assertEquals(0,dir.listAll().length);
  dir.close();
}
