{
  if (topologyId < this.topologyId) {
    throw new IllegalStateException("Cannot set a topology id (" + topologyId + ") that is lower that the current one ("+ this.topologyId+ ")");
  }
  if (trace) {
    log.tracef("Signalling topology %d is installed",topologyId);
  }
  this.topologyId=topologyId;
synchronized (topologyLock) {
    topologyLock.notifyAll();
  }
}
