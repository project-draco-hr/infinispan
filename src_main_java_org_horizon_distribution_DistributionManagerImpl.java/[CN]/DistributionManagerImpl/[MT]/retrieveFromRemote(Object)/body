{
  ClusteredGetCommand command=new ClusteredGetCommand(key,cache.getName());
  List<Object> responses=rpcManager.invokeRemotely(consistentHash.locate(key,replCount),command,ResponseMode.SYNCHRONOUS,configuration.getSyncReplTimeout(),false);
  for (  Object r : responses) {
    if (!(r instanceof Throwable))     return r;
  }
  return null;
}
