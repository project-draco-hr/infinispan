{
  assertEmpty(KEY_1);
  cache(1).put(KEY_1,VALUE_1);
  assertCacheValue(1,KEY_1,VALUE_1);
  cache(0).remove(KEY_1);
  assertCacheValue(0,KEY_1,null);
  cache(0).put(KEY_1,VALUE_1);
  assertCacheValue(0,KEY_1,VALUE_1);
  cache(0).remove(KEY_1);
  assertCacheValue(0,KEY_1,null);
  assertNoTransactions();
}
