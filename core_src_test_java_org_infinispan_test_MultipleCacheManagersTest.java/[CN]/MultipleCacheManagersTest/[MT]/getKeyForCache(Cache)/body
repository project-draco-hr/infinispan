{
  ExecutorService ex=Executors.newSingleThreadExecutor();
  KeyAffinityService<Object> kas=KeyAffinityServiceFactory.newKeyAffinityService(cache,ex,new RndKeyGenerator(),5,true);
  try {
    return kas.getKeyForAddress(cache.getAdvancedCache().getAdvancedCache().getRpcManager().getAddress());
  }
  finally {
    ex.shutdown();
  }
}
