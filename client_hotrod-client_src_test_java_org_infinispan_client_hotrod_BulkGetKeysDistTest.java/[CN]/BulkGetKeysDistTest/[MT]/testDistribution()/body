{
  for (int i=0; i < 100; i++) {
    remoteCache.put(i,i);
  }
  for (int i=0; i < numberOfHotRodServers(); i++) {
    assertTrue(cache(i).size() < 100);
  }
  Set<Object> set=remoteCache.keySet();
  assertEquals(100,set.size());
  for (int i=0; i < 100; i++) {
    assertTrue(set.contains(i));
  }
}
