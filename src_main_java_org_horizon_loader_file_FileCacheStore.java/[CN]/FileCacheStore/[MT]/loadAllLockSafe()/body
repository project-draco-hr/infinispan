{
  Set<StoredEntry> result=new HashSet<StoredEntry>();
  for (  File bucketFile : root.listFiles()) {
    Bucket bucket=loadBucket(bucketFile);
    if (bucket != null) {
      if (bucket.removeExpiredEntries()) {
        saveBucket(bucket);
      }
      result.addAll(bucket.getStoredEntries());
    }
  }
  return result;
}
