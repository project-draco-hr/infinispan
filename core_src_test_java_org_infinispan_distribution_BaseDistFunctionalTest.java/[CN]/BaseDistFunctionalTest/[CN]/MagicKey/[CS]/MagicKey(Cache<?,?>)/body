{
  address=addressOf(toMapTo).toString();
  Random r=new Random();
  for (; ; ) {
    final int hc=r.nextInt(DefaultConsistentHash.HASH_SPACE);
    Object dummy=new Object(){
      @Override public int hashCode(){
        return hc;
      }
    }
;
    if (BaseDistFunctionalTest.isFirstOwner(toMapTo,dummy)) {
      hashcode=hc;
      break;
    }
  }
}
