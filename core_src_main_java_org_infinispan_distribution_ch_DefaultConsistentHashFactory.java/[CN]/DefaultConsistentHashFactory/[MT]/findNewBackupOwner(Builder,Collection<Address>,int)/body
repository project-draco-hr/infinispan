{
  Address best=null;
  int foundOwned=maxSegments;
  for (  Address candidate : builder.getMembers()) {
    if (builder.getOwned(candidate) >= foundOwned)     continue;
    if (excludes == null || !excludes.contains(candidate)) {
      best=candidate;
      foundOwned=builder.getOwned(candidate);
    }
  }
  return best;
}
