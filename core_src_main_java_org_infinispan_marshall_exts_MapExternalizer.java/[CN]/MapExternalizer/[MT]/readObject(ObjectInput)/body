{
  int magicNumber=input.readUnsignedByte();
  Map subject=null;
switch (magicNumber) {
case HASHMAP:
    subject=new HashMap();
  break;
case TREEMAP:
subject=new TreeMap();
break;
case FASTCOPYHASHMAP:
subject=new FastCopyHashMap();
break;
}
MarshallUtil.unmarshallMap(subject,input);
return subject;
}
