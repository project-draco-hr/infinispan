{
  StringBuilder sb=new StringBuilder();
  sb.append("(");
  sb.append(compositeCondition.getLeftCondition().accept(this));
  sb.append(") ");
  sb.append(compositeCondition.isConjunction() ? "AND" : "OR");
  sb.append(" (");
  sb.append(compositeCondition.getRightCondition().accept(this));
  sb.append(")");
  return sb.toString();
}
