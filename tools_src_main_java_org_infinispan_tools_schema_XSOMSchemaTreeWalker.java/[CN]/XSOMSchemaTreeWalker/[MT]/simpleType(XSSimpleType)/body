{
  simpleType.visit(new XSSimpleTypeVisitor(){
    @Override public void listSimpleType(    XSListSimpleType type){
    }
    @Override public void restrictionSimpleType(    XSRestrictionSimpleType type){
      Iterator<?> itr=type.iterateDeclaredFacets();
      while (itr.hasNext()) {
        facet((XSFacet)itr.next());
      }
    }
    @Override public void unionSimpleType(    XSUnionSimpleType type){
    }
  }
);
}
