{
  XSComplexType type=schema.getComplexType(decl.getType().getName());
  if (!decl.isAbstract()) {
    TreeNode n=new TreeNode(decl.getName(),currentNode);
    currentNode.getChildren().add(n);
    currentNode=n;
    currentNode.setType(type);
    complexType(type);
    currentNode=n.getParent();
  }
}
