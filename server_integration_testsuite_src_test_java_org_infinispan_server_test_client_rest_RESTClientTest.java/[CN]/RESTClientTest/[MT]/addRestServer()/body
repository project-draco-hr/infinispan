{
  if (server1.getRESTEndpoint().getInetAddress() instanceof Inet6Address) {
    RESTHelper.addServer("[" + server1.getRESTEndpoint().getInetAddress().getHostName() + "]",server1.getRESTEndpoint().getContextPath());
  }
 else {
    RESTHelper.addServer(server1.getRESTEndpoint().getInetAddress().getHostName(),server1.getRESTEndpoint().getContextPath());
  }
}
