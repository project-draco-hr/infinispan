{
  Set<Object> keys=loadAllKeys(null);
  Set<InternalCacheEntry> result=new HashSet<InternalCacheEntry>();
  for (  Object key : keys) {
    InternalCacheEntry ice=load(key);
    if (ice != null) {
      result.add(ice);
      if (result.size() >= numEntries)       return result;
    }
  }
  return result;
}
