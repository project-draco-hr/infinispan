{
  try {
    boolean parents_exist=checkParentDirs(getPath(),true);
    if (!parents_exist)     return false;
    metadataCache.withFlags(FORCE_SYNCHRONOUS).put(getPath(),new Metadata(0,System.currentTimeMillis(),chunk_size,Metadata.DIR));
    return true;
  }
 catch (  IOException e) {
    return false;
  }
}
