{
  try {
    if (trace)     log.trace("{0} current sleepycat transaction {1}",commit ? "committing" : "aborting",currentTransaction.getTransaction());
    if (commit)     currentTransaction.commitTransaction();
 else     currentTransaction.abortTransaction();
  }
 catch (  Exception caught) {
    throw convertToCacheLoaderException("Problem completing transaction",caught);
  }
}
