{
  if (acl == null)   return Collections.emptySet();
  final Set<K> set=new HashSet<K>();
  acl.process(filter,new AdvancedCacheLoader.CacheLoaderTask<K,Object>(){
    @Override public void processEntry(    MarshalledEntry<K,Object> marshalledEntry,    AdvancedCacheLoader.TaskContext taskContext) throws InterruptedException {
      set.add(marshalledEntry.getKey());
    }
  }
,new WithinThreadExecutor(),false,false);
  return set;
}
