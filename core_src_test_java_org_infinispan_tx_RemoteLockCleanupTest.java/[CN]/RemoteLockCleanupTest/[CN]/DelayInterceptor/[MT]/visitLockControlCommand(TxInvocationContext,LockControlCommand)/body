{
  if (!ctx.isOriginLocal()) {
    receivedReplRequest=true;
    Thread.sleep(5000);
    try {
      System.out.println("RemoteLockCleanupTest$DelayInterceptor.visitLockControlCommand");
      return super.visitLockControlCommand(ctx,command);
    }
  finally {
      lockAcquired=true;
    }
  }
 else {
    return super.visitLockControlCommand(ctx,command);
  }
}
