{
  FileMetadata m=new FileMetadata();
  m.setBufferSize(10);
  m.setSize(10);
  Assert.assertEquals(1,m.getNumberOfChunks());
  m.setSize(11);
  Assert.assertEquals(2,m.getNumberOfChunks());
  m.setBufferSize(11);
  Assert.assertEquals(1,m.getNumberOfChunks());
  m.setSize(22);
  Assert.assertEquals(2,m.getNumberOfChunks());
}
