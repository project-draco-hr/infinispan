{
  if (millis > 0 && millis < 1000) {
    if (log.isTraceEnabled()) {
      log.tracef("Adjusting %s time for (k,v): (%s, %s) from %d millis to 1 sec, as milliseconds are not supported by HotRod",desc,entry.getKey(),entry.getValue(),millis);
    }
    return 1;
  }
  return TimeUnit.MILLISECONDS.toSeconds(millis);
}
