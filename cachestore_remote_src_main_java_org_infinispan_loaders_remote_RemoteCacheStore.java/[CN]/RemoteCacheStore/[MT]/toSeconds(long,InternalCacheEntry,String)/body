{
  if (millis > 0 && millis < 1000) {
    if (log.isTraceEnabled()) {
      log.trace("Adjusting " + desc + " time for (k,v): ("+ entry.getKey()+ ", "+ entry.getValue()+ ") from "+ millis+ " millis to 1 sec, as milliseconds are not supported by HotRod");
    }
    return 1;
  }
  return TimeUnit.MILLISECONDS.toSeconds(millis);
}
