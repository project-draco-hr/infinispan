{
  CacheEntry ce=lookupEntry(key);
  if (ce == null || ce.isNull() || ce.getValue() == null) {
    if (ce != null) {
      ce.setValue(cacheEntry.getValue());
      ce.setMetadata(cacheEntry.getMetadata());
      onEntryValueReplaced(key,cacheEntry);
    }
 else {
      return false;
    }
  }
  return true;
}
