{
  boolean result=false;
  if (log.isDebugEnabled())   log.debug("Invoking set on attribute " + attribute.getName() + " with value "+ attribute.getValue());
  AttributeEntry entry=atts.get(attribute.getName());
  if (entry != null) {
    try {
      entry.invoke(attribute);
      result=true;
    }
 catch (    Exception e) {
      log.warn("Exception while writing value for attribute " + attribute.getName(),e);
    }
  }
 else {
    log.warn("Could not invoke set on attribute " + attribute.getName() + " with value "+ attribute.getValue());
  }
  return result;
}
