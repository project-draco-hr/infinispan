def validateVersion(version):
    versionPattern = re.compile('^[4-9]\\.[0-9]\\.[0-9]\\.(Final|(ALPHA|BETA|CR)[1-9][0-9]?)$', re.IGNORECASE)
    if versionPattern.match(version):
        return version.strip().upper()
    else:
        print (("Invalid version '" + version) + "'!\n")
        helpAndExit()
