{
  int hash=(key == null) ? 0 : hash(key);
  int i=indexFor(hash,table.length);
  for (LinkedEntry<K,V> e=table[i]; e != null; e=e.next) {
    Object k;
    if (e.hash == hash && ((k=e.key) == key || (key != null && key.equals(k)))) {
      e.value=value;
      return;
    }
  }
  createEntry(hash,key,value,i);
}
