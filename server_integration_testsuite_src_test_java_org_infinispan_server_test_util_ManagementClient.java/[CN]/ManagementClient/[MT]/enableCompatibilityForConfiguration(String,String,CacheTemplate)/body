{
  ops.add(Address.subsystem("datagrid-infinispan").and("cache-container",containerName).and("configurations","CONFIGURATIONS").and(template.getType(),configurationName).and("compatibility","COMPATIBILITY"));
  ops.writeAttribute(Address.subsystem("datagrid-infinispan").and("cache-container",containerName).and("configurations","CONFIGURATIONS").and(template.getType(),configurationName).and("compatibility","COMPATIBILITY"),"enabled",true);
}
