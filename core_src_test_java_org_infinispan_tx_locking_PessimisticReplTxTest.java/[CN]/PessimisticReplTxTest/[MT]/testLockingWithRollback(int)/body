{
  tm(executeOn).begin();
  advancedCache(executeOn).lock(k);
  assertLockingOnRollback();
  assertNoTransactions();
  assertNull(cache(0).get(k));
  assertNull(cache(1).get(k));
  tm(executeOn).begin();
  advancedCache(executeOn).lock(k);
  cache(executeOn).put(k,"v");
  assertLockingOnRollback();
  assertNoTransactions();
  assertNull(cache(0).get(k));
  assertNull(cache(1).get(k));
}
