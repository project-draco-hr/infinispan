{
  if (bucket != null) {
    if (bucket.removeExpiredEntries()) {
      upgradeLock(bucket.getBucketId());
      try {
        updateBucket(bucket);
      }
  finally {
        downgradeLock(bucket.getBucketId());
      }
    }
    boolean enoughLooping=consider(bucket.getStoredEntries());
    if (enoughLooping) {
      return true;
    }
  }
  return false;
}
