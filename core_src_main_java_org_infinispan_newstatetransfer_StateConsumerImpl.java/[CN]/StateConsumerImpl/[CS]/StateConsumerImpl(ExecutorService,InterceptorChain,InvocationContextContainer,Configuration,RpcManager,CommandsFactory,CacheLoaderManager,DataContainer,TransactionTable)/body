{
  this.executorService=executorService;
  this.interceptorChain=interceptorChain;
  this.icc=icc;
  this.configuration=configuration;
  this.rpcManager=rpcManager;
  this.commandsFactory=commandsFactory;
  this.cacheLoaderManager=cacheLoaderManager;
  this.dataContainer=dataContainer;
  this.transactionTable=transactionTable;
  useVersionedPut=configuration.transaction().transactionMode().isTransactional() && configuration.versioning().enabled() && configuration.locking().writeSkewCheck()&& configuration.transaction().lockingMode() == LockingMode.OPTIMISTIC && configuration.clustering().cacheMode().isClustered();
  timeout=configuration.clustering().stateTransfer().timeout();
}
