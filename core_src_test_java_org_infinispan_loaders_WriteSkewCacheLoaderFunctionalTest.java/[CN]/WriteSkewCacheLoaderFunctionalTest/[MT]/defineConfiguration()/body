{
  ConfigurationBuilder builder=new ConfigurationBuilder();
  builder.transaction().transactionMode(TransactionMode.TRANSACTIONAL).versioning().enable().scheme(VersioningScheme.SIMPLE).locking().isolationLevel(IsolationLevel.REPEATABLE_READ).writeSkewCheck(true).clustering().cacheMode(CacheMode.REPL_SYNC).sync().loaders().preload(true).addLoader(DummyInMemoryCacheStoreConfigurationBuilder.class).storeName(this.getClass().getName());
  return builder;
}
