{
synchronized (servers) {
    servers.add(server);
  }
synchronized (filterFactories) {
    for (    Map.Entry<String,CacheEventFilterFactory> entry : filterFactories.entrySet())     server.addCacheEventFilterFactory(entry.getKey(),entry.getValue(),marshaller);
  }
synchronized (converterFactories) {
    for (    Map.Entry<String,CacheEventConverterFactory> entry : converterFactories.entrySet())     server.addCacheEventConverterFactory(entry.getKey(),entry.getValue(),marshaller);
  }
}
