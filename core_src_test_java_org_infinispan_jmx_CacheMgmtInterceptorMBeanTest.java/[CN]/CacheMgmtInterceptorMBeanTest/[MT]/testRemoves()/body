{
  assertStores(0);
  assertRemoveHits(0);
  assertRemoveMisses(0);
  cache.put("key","value");
  cache.put("key2","value2");
  cache.put("key3","value3");
  assertStores(3);
  assertRemoveHits(0);
  assertRemoveMisses(0);
  cache.remove("key");
  cache.remove("key3");
  cache.remove("key4");
  assertRemoveHits(2);
  assertRemoveMisses(1);
  cache.remove("key2");
  assertRemoveHits(3);
  assertRemoveMisses(1);
}
