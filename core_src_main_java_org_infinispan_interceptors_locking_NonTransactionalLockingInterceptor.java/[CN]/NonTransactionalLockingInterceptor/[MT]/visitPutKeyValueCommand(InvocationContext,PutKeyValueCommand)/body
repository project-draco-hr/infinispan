{
  try {
    if (cll.localNodeIsOwner(command.getKey()))     lockForPut(ctx,command,!command.isPutIfAbsent());
 else     wrapForRead(ctx,command.getKey());
    return invokeNextInterceptor(ctx,command);
  }
 catch (  Throwable te) {
    return cleanLocksAndRethrow(ctx,te);
  }
 finally {
    commit(ctx);
  }
}
