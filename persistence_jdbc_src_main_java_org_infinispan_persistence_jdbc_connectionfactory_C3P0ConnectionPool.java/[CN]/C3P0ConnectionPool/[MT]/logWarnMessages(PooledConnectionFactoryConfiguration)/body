{
  log.warn("The c3p0 connection factory has been deprecated and will be removed in future releases");
  if (poolConfig.propertyFile() != null) {
    log.warn(String.format("Ignoring properties file '%s'. In order to configure additional c3p0 properies, " + "you must place a '%s' or '%s' file on the classpath.",poolConfig.propertyFile(),C3P0_CONFIG,C3P0_PROPERTIES));
  }
}
