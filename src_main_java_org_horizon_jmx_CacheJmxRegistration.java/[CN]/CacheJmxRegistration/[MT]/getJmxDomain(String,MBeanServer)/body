{
  String[] registeredDomains=mBeanServer.getDomains();
  int index=2;
  String finalName=jmxDomain;
  boolean done=false;
  while (!done) {
    done=true;
    for (    String domain : registeredDomains) {
      if (domain.equals(finalName)) {
        finalName=jmxDomain + index++;
        done=false;
        break;
      }
    }
  }
  return finalName;
}
