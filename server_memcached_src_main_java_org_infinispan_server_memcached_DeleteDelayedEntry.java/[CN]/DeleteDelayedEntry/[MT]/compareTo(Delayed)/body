{
  if (o == this)   return 0;
  if (o instanceof DeleteDelayedEntry) {
    DeleteDelayedEntry x=(DeleteDelayedEntry)o;
    long diff=time - x.time;
    if (diff < 0)     return -1;
 else     if (diff > 0)     return 1;
 else     return 0;
  }
 else {
    throw new ClassCastException(o.getClass() + " is not of type " + DeleteDelayedEntry.class);
  }
}
