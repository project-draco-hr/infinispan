{
  long now=System.currentTimeMillis();
synchronized (entries) {
    for (Iterator<FileEntry> it=entries.values().iterator(); it.hasNext(); ) {
      FileEntry fe=it.next();
      if (fe.isExpired(now)) {
        it.remove();
        try {
          free(fe);
        }
 catch (        Exception e) {
          throw new CacheLoaderException(e);
        }
      }
    }
  }
}
