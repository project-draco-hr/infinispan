{
  super(map);
  this.parent=parent;
  this.batch=batch;
  if (parent != null && map != null) {
    Node[] t;
    if ((t=parent.tab) == null && (t=parent.tab=map.table) != null)     parent.baseLimit=parent.baseSize=t.length;
    this.tab=t;
    this.baseSize=parent.baseSize;
    int hi=this.baseLimit=parent.baseLimit;
    parent.baseLimit=this.index=this.baseIndex=(hi + parent.baseIndex + 1) >>> 1;
  }
}
