{
  KOut value=null;
  CacheLoader cl=resolveCacheLoader();
  if (cl != null) {
    try {
      InternalCacheEntry entry=cl.load(key);
      if (entry != null) {
        value=(KOut)entry.getValue();
      }
    }
 catch (    CacheLoaderException e) {
      throw new CacheException("Could not load key/value entries from cacheloader",e);
    }
  }
  return value;
}
