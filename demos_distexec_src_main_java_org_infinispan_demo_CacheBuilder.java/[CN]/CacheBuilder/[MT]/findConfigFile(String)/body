{
  FileLookup fl=new FileLookup();
  if (configFile != null) {
    InputStream inputStream=fl.lookupFile(configFile,Thread.currentThread().getContextClassLoader());
    try {
      if (inputStream != null)       return configFile;
    }
  finally {
      Util.close(inputStream);
    }
  }
  return null;
}
