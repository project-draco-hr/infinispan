{
  if (maxResults == null) {
    return null;
  }
 else {
    long tmpMaxResult=(long)getFirstResultIndex() + maxResults;
    if (tmpMaxResult >= Integer.MAX_VALUE) {
      return Integer.MAX_VALUE - 1;
    }
    if (tmpMaxResult == 0) {
      return 1;
    }
 else {
      return (int)tmpMaxResult;
    }
  }
}
