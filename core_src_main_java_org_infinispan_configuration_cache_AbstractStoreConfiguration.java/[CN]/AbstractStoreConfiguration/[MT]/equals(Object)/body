{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  AbstractStoreConfiguration other=(AbstractStoreConfiguration)obj;
  if (async == null) {
    if (other.async != null)     return false;
  }
 else   if (!async.equals(other.async))   return false;
  if (attributes == null) {
    if (other.attributes != null)     return false;
  }
 else   if (!attributes.equals(other.attributes))   return false;
  if (singletonStore == null) {
    if (other.singletonStore != null)     return false;
  }
 else   if (!singletonStore.equals(other.singletonStore))   return false;
  return true;
}
