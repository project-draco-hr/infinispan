{
  for (  Synchronization s : participants) {
    if (log.isDebugEnabled()) {
      log.debug("processing afterCompletion for " + s);
    }
    try {
      s.afterCompletion(status);
    }
 catch (    Throwable t) {
      log.error("afterCompletion() failed for " + s,t);
    }
  }
  participants.clear();
}
