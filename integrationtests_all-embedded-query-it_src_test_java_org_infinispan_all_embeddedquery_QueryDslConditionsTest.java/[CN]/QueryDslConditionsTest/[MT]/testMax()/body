{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getTransactionImplClass()).select(Expression.property("accountId"),Expression.max("amount")).groupBy("accountId").orderBy("accountId").build();
  List<Object[]> list=q.list();
  assertEquals(2,list.size());
  assertEquals(2,list.get(0).length);
  assertEquals(2,list.get(1).length);
  assertEquals(1800d,list.get(0)[1]);
  assertEquals(149d,list.get(1)[1]);
}
