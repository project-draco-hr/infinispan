{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getAccountImplClass()).having("creationDate").eq(Expression.param("param1")).toBuilder().build().setParameter("param1",makeDate("2013-01-03"));
  List<Account> list=q.list();
  assertEquals(1,list.size());
  assertEquals(1,list.get(0).getId());
}
