{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getUserImplClass()).select(Expression.property("surname"),Expression.count("addresses.street")).groupBy("surname").orderBy("surname").build();
  List<Object[]> list=q.list();
  assertEquals(3,list.size());
  assertEquals(2,list.get(0).length);
  assertEquals(2,list.get(1).length);
  assertEquals(2,list.get(2).length);
  assertEquals(1L,list.get(0)[1]);
  assertEquals(2L,list.get(1)[1]);
  assertEquals(0L,list.get(2)[1]);
}
