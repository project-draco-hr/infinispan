{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getTransactionImplClass()).select(Expression.property("accountId"),Expression.avg("amount")).groupBy("accountId").orderBy("accountId").build();
  List<Object[]> list=q.list();
  assertEquals(2,list.size());
  assertEquals(2,list.get(0).length);
  assertEquals(2,list.get(1).length);
  assertEquals(1107.6666d,(Double)list.get(0)[1],0.0001d);
  assertEquals(120.18867d,(Double)list.get(1)[1],0.0001d);
}
