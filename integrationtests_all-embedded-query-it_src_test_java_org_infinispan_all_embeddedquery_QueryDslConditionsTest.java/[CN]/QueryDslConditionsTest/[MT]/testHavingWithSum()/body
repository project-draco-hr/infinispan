{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getTransactionImplClass()).select(Expression.property("accountId"),Expression.sum("amount")).groupBy("accountId").having(Expression.sum("amount")).gt(3324).toBuilder().orderBy("accountId").build();
  List<Object[]> list=q.list();
  assertEquals(1,list.size());
  assertEquals(2,list.get(0)[0]);
  assertEquals(6370.0d,(Double)list.get(0)[1],0.0001d);
}
