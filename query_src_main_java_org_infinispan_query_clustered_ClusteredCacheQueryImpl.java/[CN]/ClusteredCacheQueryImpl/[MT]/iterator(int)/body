{
  ClusteredQueryCommand command=ClusteredQueryCommand.createEagerIterator(hSearchQuery,cache);
  HashMap<UUID,ClusteredTopDocs> topDocsResponses=treatIteratorResponses(command);
  DistributedIterator it=new DistributedIterator(sort,fetchSize,this.resultSize,topDocsResponses,cache);
  return it;
}
