{
  if (retval instanceof NotifyingFuture) {
    return (NotifyingFuture<X>)retval;
  }
 else {
    return new AbstractInProcessNotifyingFuture<X>(){
      @Override @SuppressWarnings("unchecked") public X get() throws InterruptedException, ExecutionException {
        return (X)retval;
      }
    }
;
  }
}
