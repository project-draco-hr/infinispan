{
  while (true) {
    Lock lock=getLock(key);
    boolean locked=false;
    try {
      locked=lock.tryLock(timeout,unit);
    }
 catch (    InterruptedException ie) {
      safeRelease(lock);
      throw ie;
    }
catch (    Throwable th) {
      safeRelease(lock);
      locked=false;
    }
    if (locked) {
      Lock existingLock=locks.putIfAbsent(key,lock);
      if (existingLock != null && existingLock != lock) {
        safeRelease(lock);
      }
 else {
        return lock;
      }
    }
 else {
      return null;
    }
  }
}
