{
  if (instance == null) {
    try {
      ByteArrayInputStream bais=new ByteArrayInputStream(raw);
      ObjectInputStream ois=new MarshalledValueInputStream(bais);
      instance=ois.readObject();
      ois.close();
      bais.close();
    }
 catch (    Exception e) {
      throw new CacheException("Unable to unmarshall value",e);
    }
  }
}
