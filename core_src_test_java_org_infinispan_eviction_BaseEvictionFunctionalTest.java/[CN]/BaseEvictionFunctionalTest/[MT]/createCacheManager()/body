{
  ConfigurationBuilder builder=TestCacheManagerFactory.getDefaultCacheConfiguration(false);
  EmbeddedCacheManager cm=new DefaultCacheManager(builder.eviction().maxEntries(CACHE_SIZE).strategy(getEvictionStrategy()).expiration().wakeUpInterval(100L).locking().useLockStriping(false).invocationBatching().build());
  cache=cm.getCache();
  cache.addListener(new EvictionListener());
  return cm;
}
