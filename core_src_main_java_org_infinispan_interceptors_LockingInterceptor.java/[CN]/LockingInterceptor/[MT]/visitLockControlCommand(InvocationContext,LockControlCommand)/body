{
  try {
    if (c.isLock()) {
      for (      Object key : c.getKeys())       entryFactory.acquireLock(ctx,key);
    }
 else     if (c.isUnlock()) {
      for (      Object key : c.getKeys())       entryFactory.releaseLock(key);
    }
    return invokeNextInterceptor(ctx,c);
  }
  finally {
    doAfterCall(ctx);
  }
}
