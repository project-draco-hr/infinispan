{
  try {
    for (    InternalCacheEntry entry : dataContainer.entrySet())     entryFactory.wrapEntryForWriting(ctx,entry,false,false,false,false,false);
    return invokeNextInterceptor(ctx,command);
  }
 catch (  Throwable te) {
    return cleanLocksAndRethrow(ctx,te);
  }
 finally {
    doAfterCall(ctx);
  }
}
