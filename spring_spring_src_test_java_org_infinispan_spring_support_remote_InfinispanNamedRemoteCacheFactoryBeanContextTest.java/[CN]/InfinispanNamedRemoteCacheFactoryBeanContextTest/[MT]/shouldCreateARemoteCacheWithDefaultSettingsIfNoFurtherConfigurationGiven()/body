{
  final Cache<Object,Object> infinispanNamedRemoteCacheWithoutFurtherConfiguration=this.applicationContext.getBean(INFINISPAN_NAMED_REMOTE_CACHE_WITHOUT_FURTHER_CONFIGURATION_BEAN_NAME,Cache.class);
  assertNotNull("Spring application context should contain a named Infinispan cache having bean name = \"" + INFINISPAN_NAMED_REMOTE_CACHE_WITHOUT_FURTHER_CONFIGURATION_BEAN_NAME + "\". However, it doesn't.",infinispanNamedRemoteCacheWithoutFurtherConfiguration);
}
