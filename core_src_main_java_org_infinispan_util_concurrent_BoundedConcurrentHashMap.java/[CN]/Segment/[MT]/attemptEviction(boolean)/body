{
  Set<HashEntry<K,V>> evicted=null;
  boolean obtainedLock=!lockedAlready ? tryLock() : true;
  if (!obtainedLock && eviction.thresholdExpired()) {
    lock();
    obtainedLock=true;
  }
  if (obtainedLock) {
    try {
      if (eviction.thresholdExpired()) {
        evicted=eviction.execute();
      }
    }
  finally {
      if (!lockedAlready) {
        unlock();
      }
    }
  }
  return evicted;
}
