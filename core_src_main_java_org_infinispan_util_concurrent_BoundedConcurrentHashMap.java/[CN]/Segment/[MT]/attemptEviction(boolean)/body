{
  boolean shouldAttemptEvict=lockedAlready || tryLock();
  Set<HashEntry<K,V>> evicted=null;
  if (shouldAttemptEvict) {
    try {
      if (eviction.thresholdExpired()) {
        evicted=eviction.execute();
      }
    }
  finally {
      if (!lockedAlready) {
        unlock();
      }
    }
  }
  return evicted;
}
