{
  TreeNode result=null;
  if (tn.getName().equals(name) && tn.getParent() != null && tn.getParent().getName().equals(parent)) {
    result=tn;
  }
 else {
    for (    TreeNode child : tn.getChildren()) {
      result=findNode(child,name,parent);
      if (result != null)       break;
    }
  }
  return result;
}
