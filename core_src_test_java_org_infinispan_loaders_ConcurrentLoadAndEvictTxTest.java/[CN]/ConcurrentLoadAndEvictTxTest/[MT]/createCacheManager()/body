{
  Configuration config=getDefaultStandaloneConfig(true).fluent().eviction().strategy(EvictionStrategy.LRU).maxEntries(10).expiration().wakeUpInterval(10L).loaders().addCacheLoader(new DummyInMemoryCacheStore.Cfg()).build();
  EmbeddedCacheManager cm=TestCacheManagerFactory.createCacheManager(config);
  cache=cm.getCache();
  tm=getTransactionManager(cache);
  return cm;
}
