{
  Configuration config=new Configuration().fluent().eviction().strategy(EvictionStrategy.FIFO).maxEntries(10).expiration().wakeUpInterval(10L).loaders().addCacheLoader(new DummyInMemoryCacheStore.Cfg()).build();
  EmbeddedCacheManager cm=TestCacheManagerFactory.createCacheManager(config,true);
  cache=cm.getCache();
  tm=getTransactionManager(cache);
  return cm;
}
