{
  if (attributes != null) {
    final OperationStepHandler restartCacheWriteHandler=new RestartCacheWriteAttributeHandler(cacheResource.getPathElement().getKey(),cacheResource.getCacheAddHandler(),attributes);
    for (    AttributeDefinition attr : attributes) {
      resourceRegistration.registerReadWriteAttribute(attr,CacheReadAttributeHandler.INSTANCE,restartCacheWriteHandler);
    }
  }
}
