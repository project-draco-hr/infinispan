{
  byte[] key={1,2,3};
  byte[] value={4,5,6};
  map.put(key,value);
  byte[] computeKey={1,2,3};
  byte[] newValue=map.computeIfAbsent(computeKey,new EquivalentConcurrentHashMapV8.Fun<byte[],byte[]>(){
    @Override public byte[] apply(    byte[] bytes){
      return new byte[]{7,8,9};
    }
  }
);
  assertTrue(String.format("Expected value=%s to be returned by operation, instead returned value=%s",str(value),str(newValue)),Arrays.equals(newValue,value));
}
