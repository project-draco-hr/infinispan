{
  cache.put(1,"v1");
  cache.put(2,"v2");
  tm().begin();
  try {
    assertEquals("v1",cache.get(1));
    Transaction suspended=tm().suspend();
    tm().begin();
    try {
      cache.remove(1);
    }
  finally {
      tm().commit();
      tm().resume(suspended);
      assertEquals(2,cache.size());
      assertEquals("v1",cache.get(1));
      assertEquals("v2",cache.get(2));
    }
  }
  finally {
    tm().commit();
  }
}
