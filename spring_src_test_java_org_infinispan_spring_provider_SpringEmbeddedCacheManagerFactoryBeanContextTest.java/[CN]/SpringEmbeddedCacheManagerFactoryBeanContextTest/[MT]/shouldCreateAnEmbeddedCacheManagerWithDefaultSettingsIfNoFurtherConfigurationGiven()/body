{
  final SpringEmbeddedCacheManager springEmbeddedCacheManagerWithDefaultConfiguration=this.applicationContext.getBean(SPRING_EMBEDDED_CACHE_MANAGER_WITH_DEFAULT_CONFIGURATION_BEAN_NAME,SpringEmbeddedCacheManager.class);
  AssertJUnit.assertNotNull("Spring application context should contain a SpringEmbeddedCacheManager with default settings having bean name = \"" + SPRING_EMBEDDED_CACHE_MANAGER_WITH_DEFAULT_CONFIGURATION_BEAN_NAME + "\". However, it doesn't.",springEmbeddedCacheManagerWithDefaultConfiguration);
  springEmbeddedCacheManagerWithDefaultConfiguration.stop();
}
