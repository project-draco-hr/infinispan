{
  final SpringEmbeddedCacheManager springEmbeddedCacheManagerConfiguredFromConfigurationFile=this.applicationContext.getBean(SPRING_EMBEDDED_CACHE_MANAGER_CONFIGURED_FROM_CONFIGURATION_FILE_BEAN_NAME,SpringEmbeddedCacheManager.class);
  AssertJUnit.assertNotNull("Spring application context should contain a SpringEmbeddedCacheManager configured from configuration file having bean name = \"" + SPRING_EMBEDDED_CACHE_MANAGER_CONFIGURED_FROM_CONFIGURATION_FILE_BEAN_NAME + "\". However, it doesn't.",springEmbeddedCacheManagerConfiguredFromConfigurationFile);
  springEmbeddedCacheManagerConfiguredFromConfigurationFile.stop();
}
