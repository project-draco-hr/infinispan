{
  final SpringEmbeddedCacheManager springEmbeddedCacheManagerConfiguredUsingProperties=this.applicationContext.getBean(SPRING_EMBEDDED_CACHE_MANAGER_CONFIGURED_USING_SETTERS_BEAN_NAME,SpringEmbeddedCacheManager.class);
  AssertJUnit.assertNotNull("Spring application context should contain a SpringEmbeddedCacheManager configured using properties having bean name = \"" + SPRING_EMBEDDED_CACHE_MANAGER_CONFIGURED_USING_SETTERS_BEAN_NAME + "\". However, it doesn't.",springEmbeddedCacheManagerConfiguredUsingProperties);
  springEmbeddedCacheManagerConfiguredUsingProperties.stop();
}
