{
  if (config.getMaxEntries() > -1) {
    int currentSize=evictionQueue.size();
    if (config.getMaxEntries() < currentSize)     return true;
    if (config.getMaxEntries() < originalQueueSize && config.getMinEntries() > -1 && config.getMinEntries() < currentSize)     return true;
  }
  return false;
}
