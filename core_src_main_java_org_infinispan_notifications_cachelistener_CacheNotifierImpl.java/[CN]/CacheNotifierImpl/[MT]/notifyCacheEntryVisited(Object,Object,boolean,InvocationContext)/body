{
  if (!cacheEntryVisitedListeners.isEmpty()) {
    EventImpl<Object,Object> e=EventImpl.createEvent(cache,CACHE_ENTRY_VISITED);
    e.setPre(pre);
    e.setKey(key);
    e.setValue(value);
    setTx(ctx,e);
    for (    ListenerInvocation listener : cacheEntryVisitedListeners)     listener.invoke(e);
  }
}
