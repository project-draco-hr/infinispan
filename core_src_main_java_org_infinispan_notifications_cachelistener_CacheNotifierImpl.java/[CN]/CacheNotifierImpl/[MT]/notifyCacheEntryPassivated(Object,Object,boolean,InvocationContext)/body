{
  if (!cacheEntryPassivatedListeners.isEmpty()) {
    InvocationContext contexts=icc.suspend();
    try {
      EventImpl e=new EventImpl();
      e.setCache(cache);
      e.setPre(pre);
      e.setKey(key);
      e.setValue(value);
      setTx(ctx,e);
      e.setType(CACHE_ENTRY_PASSIVATED);
      for (      ListenerInvocation listener : cacheEntryPassivatedListeners) {
        listener.invoke(e);
      }
    }
  finally {
      icc.resume(contexts);
    }
  }
}
