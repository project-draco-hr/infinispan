{
  if (!cacheEntryPassivatedListeners.isEmpty()) {
    EventImpl<Object,Object> e=EventImpl.createEvent(cache,CACHE_ENTRY_PASSIVATED);
    e.setPre(pre);
    e.setKey(key);
    e.setValue(value);
    for (    ListenerInvocation listener : cacheEntryPassivatedListeners)     listener.invoke(e);
  }
}
