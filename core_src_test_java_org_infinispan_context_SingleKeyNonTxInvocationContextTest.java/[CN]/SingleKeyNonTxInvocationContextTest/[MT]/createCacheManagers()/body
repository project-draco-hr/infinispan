{
  final Configuration c=getDefaultClusteredConfig(Configuration.CacheMode.DIST_SYNC,false);
  c.fluent().hash().numOwners(1);
  createCluster(c,2);
  waitForClusterToForm();
  ci0=new CheckInterceptor();
