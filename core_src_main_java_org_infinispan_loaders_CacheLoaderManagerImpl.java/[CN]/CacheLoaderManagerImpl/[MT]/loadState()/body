{
  int ne=-1;
  if (configuration.eviction().strategy().isEnabled())   ne=configuration.eviction().maxEntries();
  Set<InternalCacheEntry> state;
switch (ne) {
case -1:
    state=loader.loadAll();
  break;
case 0:
state=Collections.emptySet();
break;
default :
state=loader.load(ne);
break;
}
return state;
}
