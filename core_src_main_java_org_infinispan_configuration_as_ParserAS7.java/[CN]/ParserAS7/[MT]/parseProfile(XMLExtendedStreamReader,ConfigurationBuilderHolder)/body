{
  while (reader.hasNext() && (reader.nextTag() != XMLStreamConstants.END_ELEMENT)) {
    Element element=Element.forName(reader.getLocalName());
switch (element) {
case SUBSYSTEM:
{
        String ns=reader.getNamespaceURI();
        if (ns.contains("jgroups"))         parseJGroupsSubsystem(reader,holder);
 else         if (ns.contains("threads"))         parseThreadsSubsystem(reader,holder);
 else         if (ns.contains("infinispan"))         parseInfinispanSubsystem(reader,holder);
 else         reader.discardRemainder();
        break;
      }
default :
{
      reader.discardRemainder();
    }
}
}
}
