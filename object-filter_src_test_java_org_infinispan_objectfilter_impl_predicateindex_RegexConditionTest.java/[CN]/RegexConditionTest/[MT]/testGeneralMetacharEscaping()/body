{
  assertTrue(new RegexCondition("a%(b").match("aaa(b"));
  assertTrue(new RegexCondition("a%)b").match("aaa)b"));
  assertTrue(new RegexCondition("a%[b").match("aaa[b"));
  assertTrue(new RegexCondition("a%]b").match("aaa]b"));
  assertTrue(new RegexCondition("a%{b").match("aaa{b"));
  assertTrue(new RegexCondition("a%}b").match("aaa}b"));
  assertTrue(new RegexCondition("a%$b").match("aaa$b"));
  assertTrue(new RegexCondition("a%^b").match("aaa^b"));
  assertTrue(new RegexCondition("a%.b").match("aaa.b"));
  assertTrue(new RegexCondition("a%|b").match("aaa|b"));
  assertTrue(new RegexCondition("a%\\b").match("aaa\\b"));
}
