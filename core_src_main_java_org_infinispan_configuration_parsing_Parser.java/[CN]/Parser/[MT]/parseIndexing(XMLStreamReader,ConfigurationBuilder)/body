{
  for (int i=0; i < reader.getAttributeCount(); i++) {
    ParseUtils.requireNoNamespaceAttribute(reader,i);
    String value=replaceSystemProperties(reader.getAttributeValue(i));
    Attribute attribute=Attribute.forName(reader.getAttributeLocalName(i));
switch (attribute) {
case ENABLED:
      if (Boolean.valueOf(value).booleanValue())       builder.indexing().enable();
 else       builder.indexing().disable();
    break;
default :
  throw ParseUtils.unexpectedAttribute(reader,i);
}
}
builder.indexing().withProperties(parseProperties(reader));
}
