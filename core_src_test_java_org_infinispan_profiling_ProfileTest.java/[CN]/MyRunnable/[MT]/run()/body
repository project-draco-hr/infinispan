{
  try {
    Object key=Generator.getRandomElement(keys);
    long d=0, st=0;
switch (mode) {
case PUT:
      Object value=Generator.getRandomString();
    st=System.nanoTime();
  cache.put(key,value);
d=System.nanoTime() - st;
break;
case GET:
st=System.nanoTime();
cache.get(key);
d=System.nanoTime() - st;
break;
case REMOVE:
st=System.nanoTime();
cache.remove(key);
d=System.nanoTime() - st;
break;
}
duration.getAndAdd(d);
}
 catch (Exception e) {
log.error("Caught ",e);
}
}
