{
  clmConfig=configuration.getCacheLoaderManagerConfig();
  if (clmConfig != null) {
    try {
      loader=createCacheLoader();
      if (loader != null)       loader.start();
      purgeLoaders(false);
    }
 catch (    Exception e) {
      throw new CacheException("Unable to start cache loaders",e);
    }
  }
}
