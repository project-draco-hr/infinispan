{
  Collection<?> keys=InfinispanCollections.emptyList();
  if (delta.hasClearOperation()) {
    AtomicHashMap<?,?> map=(AtomicHashMap<?,?>)cache.get(deltaMapKey);
    if (map != null) {
      keys=new ArrayList(map.keySet());
    }
  }
 else {
    keys=delta.getKeys();
  }
  cache.applyDelta(deltaMapKey,delta,keys);
}
