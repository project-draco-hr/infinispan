{
  return ShrinkWrap.create(WebArchive.class).addPackage(Config.class.getPackage()).addClass(TestCacheManagerFactory.class).addAsWebInfResource(new File("src/test/webapp/WEB-INF/beans.xml"),"beans.xml").addAsLibraries(DependencyResolvers.use(MavenDependencyResolver.class).loadReposFromPom("pom.xml").artifact("javax.cache:cache-api").artifact("org.infinispan:infinispan-cdi").artifact("org.infinispan:infinispan-jcache").resolveAs(GenericArchive.class));
}
