{
  return new LevelDBCacheStoreConfiguration(location,expiredLocation,compressionType,blockSize,cacheSize,expiryQueueSize,clearThreshold,lockAcquistionTimeout,lockConcurrencyLevel,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
}
