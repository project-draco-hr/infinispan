{
  Class<?> clazz=o.getClass();
  return clazz.equals(String.class) || clazz.equals(byte[].class) || clazz.equals(Boolean.class)|| clazz.equals(Integer.class)|| clazz.equals(Long.class)|| clazz.equals(Float.class)|| clazz.equals(Double.class)|| clazz.equals(String[].class)|| clazz.equals(Integer[].class)|| clazz.equals(Long[].class)|| clazz.equals(Float[].class)|| clazz.equals(Double[].class)|| clazz.equals(Boolean[].class)|| o instanceof List|| o instanceof Map|| o instanceof Set;
}
