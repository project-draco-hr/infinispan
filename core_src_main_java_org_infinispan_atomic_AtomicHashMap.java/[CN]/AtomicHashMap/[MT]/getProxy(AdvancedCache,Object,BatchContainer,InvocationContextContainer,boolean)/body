{
  if (proxy == null) {
synchronized (this) {
      if (proxy == null)       if (fineGrained) {
        proxy=new FineGrainedAtomicHashMapProxy<K,V>(cache,mapKey,batchContainer,icc);
      }
 else {
        proxy=new AtomicHashMapProxy<K,V>(cache,mapKey,batchContainer,icc);
      }
    }
  }
  return proxy;
}
