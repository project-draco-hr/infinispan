{
  boolean isLocal=isOwner(cache,key);
  Long actual;
  if (readAccesses) {
    actual=isLocal ? getTopKey(cache).getLocalTopGets().get(key) : getTopKey(cache).getRemoteTopGets().get(key);
  }
 else {
    actual=isLocal ? getTopKey(cache).getLocalTopPuts().get(key) : getTopKey(cache).getRemoteTopPuts().get(key);
  }
  Assert.assertEquals(actual == null ? 0 : actual,expected,"Wrong number of accesses");
}
