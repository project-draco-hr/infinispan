{
  for (  Modification modification : mods) switch (modification.getType()) {
case STORE:
    Store s=(Store)modification;
  cs.store(s.getStoredEntry());
break;
case CLEAR:
cs.clear();
break;
case REMOVE:
Remove r=(Remove)modification;
cs.remove(r.getKey());
break;
case PURGE_EXPIRED:
cs.purgeExpired();
break;
default :
throw new IllegalArgumentException("Unknown modification type " + modification.getType());
}
}
