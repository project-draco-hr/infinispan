{
  loadTcp();
  if (!withFD)   tcpConfig=removeFailureDetectionTcp(tcpConfig);
  if (tcpConfig.contains("TCPPING")) {
    return getTcpConfigWithTCPPINGDiscovery();
  }
  if (tcpConfig.contains("TEST_PING")) {
    if (fullTestName == null)     return tcpConfig;
 else     return getTestPingDiscovery(fullTestName,tcpConfig);
  }
 else {
    return replaceMCastAddressAndPort(tcpConfig);
  }
}
