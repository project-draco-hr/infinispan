{
  Matcher m=TCP_START_PORT.matcher(transportCfg);
  if (m.find()) {
    newStartPort=threadTcpStartPort.get();
    return m.replaceFirst("bind_port=" + newStartPort);
  }
 else {
    System.out.println("Config is:" + transportCfg);
    Thread.dumpStack();
    throw new IllegalStateException();
  }
}
