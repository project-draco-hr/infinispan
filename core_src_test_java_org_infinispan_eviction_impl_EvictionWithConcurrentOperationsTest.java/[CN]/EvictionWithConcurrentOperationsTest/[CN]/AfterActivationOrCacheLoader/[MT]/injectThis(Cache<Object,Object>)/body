{
  SequentialInterceptorChain chain=TestingUtil.extractComponent(injectInCache,SequentialInterceptorChain.class);
  SequentialInterceptor loaderInterceptor=chain.findInterceptorExtending(org.infinispan.interceptors.impl.CacheLoaderInterceptor.class);
  injectInCache.getAdvancedCache().getSequentialInterceptorChain().addInterceptorAfter(this,loaderInterceptor.getClass());
  return this;
}
