{
  DummyInMemoryCacheStore cl=new DummyInMemoryCacheStore();
  DummyInMemoryCacheStoreConfigurationBuilder loader=TestCacheManagerFactory.getDefaultCacheConfiguration(false).loaders().addLoader(DummyInMemoryCacheStoreConfigurationBuilder.class);
  loader.storeName(getClass().getName()).purgeSynchronously(true);
  cl.init(loader.create(),getCache(),getMarshaller());
  cl.start();
  if (cl.getConfiguration() == null)   throw new NullPointerException();
  return cl;
}
