{
  if (rsps != null) {
    for (    Object rsp : rsps) {
      if (rsp != null && rsp instanceof Throwable) {
        Throwable throwable=(Throwable)rsp;
        if (trace) {
          log.trace("Received Throwable from remote cache",throwable);
        }
        throw new ReplicationException(throwable);
      }
    }
  }
}
