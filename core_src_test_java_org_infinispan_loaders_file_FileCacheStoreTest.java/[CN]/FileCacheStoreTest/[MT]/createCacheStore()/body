{
  clearTempDir();
  fcs=new FileCacheStore();
  FileCacheStoreConfigurationBuilder fileBuilder=TestCacheManagerFactory.getDefaultCacheConfiguration(false).loaders().addFileCacheStore();
  fileBuilder.fetchPersistentState(true).fsyncMode(getFsyncMode()).location(this.tmpDirectory).purgeSynchronously(true);
  fcs.init(fileBuilder.create(),getCache(),getMarshaller());
  fcs.start();
  return fcs;
}
