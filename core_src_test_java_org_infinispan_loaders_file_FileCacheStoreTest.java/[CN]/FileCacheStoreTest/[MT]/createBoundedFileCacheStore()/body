{
  FileCacheStore store=new FileCacheStore();
  FileCacheStoreConfiguration fileStoreConfiguration=TestCacheManagerFactory.getDefaultCacheConfiguration(false).loaders().addFileCacheStore().location(this.tmpDirectory).maxEntries(1).purgeSynchronously(true).create();
  store.init(fileStoreConfiguration,getCache(),getMarshaller());
  store.start();
  return store;
}
