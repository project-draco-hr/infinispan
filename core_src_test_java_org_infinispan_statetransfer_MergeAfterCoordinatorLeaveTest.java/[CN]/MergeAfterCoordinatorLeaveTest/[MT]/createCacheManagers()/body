{
  createClusteredCaches(3,"cache",getDefaultClusteredConfig(Configuration.CacheMode.DIST_SYNC),new TransportFlags().withFD(true).withMerge(true));
  c1=cache(0,"cache");
  c2=cache(1,"cache");
  c3=cache(2,"cache");
  d1=TestingUtil.getDiscardForCache(c1);
  d1.setExcludeItself(true);
  d2=TestingUtil.getDiscardForCache(c2);
  d2.setExcludeItself(true);
  d3=TestingUtil.getDiscardForCache(c3);
  d3.setExcludeItself(true);
}
