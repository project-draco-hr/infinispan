{
  super(iic.fileKey.getFileName());
  ChunkCacheKey key=new ChunkCacheKey(iic.fileKey.getIndexName(),iic.fileKey.getFileName(),0,iic.fileMetadata.getBufferSize());
  byte[] b=(byte[])iic.chunksCache.get(key);
  if (b == null) {
    buffer=new byte[0];
  }
 else {
    buffer=b;
  }
  bufferPosition=0;
}
