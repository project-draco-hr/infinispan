{
  boolean local=isKeyLocalToAddress(key);
  if (isRehashInProgress()) {
    if (local) {
      return DataLocality.LOCAL_UNCERTAIN;
    }
 else {
      return DataLocality.NOT_LOCAL_UNCERTAIN;
    }
  }
 else {
    if (local) {
      return DataLocality.LOCAL;
    }
 else {
      return DataLocality.NOT_LOCAL;
    }
  }
}
