{
  if (Util.safeEquals(joiner,this.joiner)) {
    this.joiner=null;
    if (consistentHash instanceof UnionConsistentHash)     consistentHash=((UnionConsistentHash)consistentHash).getOldConsistentHash();
  }
  if (JOINER_CAS.compareAndSet(this,joiner,null)) {
    if (consistentHash instanceof UnionConsistentHash)     consistentHash=((UnionConsistentHash)consistentHash).getOldConsistentHash();
  }
}
