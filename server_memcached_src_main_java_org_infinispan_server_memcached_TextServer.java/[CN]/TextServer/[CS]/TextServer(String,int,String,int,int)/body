{
  this(host,port,configFile == null ? new DefaultCacheManager().<String,Value>getCache() : new DefaultCacheManager(configFile).<String,Value>getCache(),masterThreads,workerThreads);
  if (configFile == null) {
    log.debug("Using cache manager using configuration defaults");
  }
 else {
    log.debug("Using cache manager configured from {0}",configFile);
  }
}
