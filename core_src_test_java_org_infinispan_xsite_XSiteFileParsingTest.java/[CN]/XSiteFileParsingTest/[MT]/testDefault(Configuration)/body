{
  assertEquals(dcc.sites().allBackups().size(),2);
  assertTrue(dcc.sites().allBackups().contains(new BackupConfiguration("NYC",BackupConfiguration.BackupStrategy.SYNC,12003l,BackupFailurePolicy.IGNORE,null,false,DEFAULT_TAKE_OFFLINE,DEFAULT_STATE_TRANSFER,true)));
  assertTrue(dcc.sites().allBackups().contains(new BackupConfiguration("SFO",BackupConfiguration.BackupStrategy.ASYNC,10000l,BackupFailurePolicy.WARN,null,false,DEFAULT_TAKE_OFFLINE,DEFAULT_STATE_TRANSFER,true)));
}
