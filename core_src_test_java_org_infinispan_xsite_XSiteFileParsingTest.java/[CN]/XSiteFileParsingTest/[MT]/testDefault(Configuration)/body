{
  assertEquals(dcc.sites().backups().size(),2);
  assertTrue(dcc.sites().backups().contains(new BackupConfiguration("NYC",BackupConfiguration.BackupStrategy.SYNC,12003l,BackupFailurePolicy.IGNORE,null)));
  assertTrue(dcc.sites().backups().contains(new BackupConfiguration("SFO",BackupConfiguration.BackupStrategy.ASYNC,10000l,BackupFailurePolicy.WARN,null)));
}
