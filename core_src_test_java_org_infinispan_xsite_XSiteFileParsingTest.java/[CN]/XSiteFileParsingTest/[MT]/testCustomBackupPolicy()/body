{
  Configuration dcc=cacheManager.getCacheConfiguration("customBackupPolicy");
  assertEquals(dcc.sites().backups().size(),1);
  assertTrue(dcc.sites().backups().contains(new BackupConfiguration("NYC2",BackupConfiguration.BackupStrategy.SYNC,160000,BackupFailurePolicy.CUSTOM,CountingCustomFailurePolicy.class.getName())));
  assertEquals(dcc.sites().backupFor().remoteCache(),null);
}
