{
  Configuration dcc=cacheManager.getCacheConfiguration("customBackupPolicy");
  assertEquals(dcc.sites().allBackups().size(),1);
  assertTrue(dcc.sites().allBackups().contains(new BackupConfiguration("NYC2",BackupConfiguration.BackupStrategy.SYNC,160000,BackupFailurePolicy.CUSTOM,CountingCustomFailurePolicy.class.getName(),new TakeOfflineConfiguration(0,0))));
  assertEquals(dcc.sites().backupFor().remoteCache(),null);
}
