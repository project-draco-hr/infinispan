{
  if (jcacheLoader == null || !configuration.isReadThrough()) {
    return null;
  }
  V value=null;
  try {
    value=jcacheLoader.load(key);
  }
 catch (  Exception ex) {
    throw Exceptions.launderCacheLoaderException(ex);
  }
  if (value != null) {
    delegate.put(key,value);
  }
  return value;
}
