{
  if (isValid(req)) {
    try {
      return executeCommand((RPCCommand)req_marshaller.objectFromByteBuffer(req.getBuffer(),req.getOffset(),req.getLength()),req);
    }
 catch (    Throwable x) {
      if (trace)       log.trace("Problems invoking command.",x);
      return x;
    }
  }
 else {
    return null;
  }
}
