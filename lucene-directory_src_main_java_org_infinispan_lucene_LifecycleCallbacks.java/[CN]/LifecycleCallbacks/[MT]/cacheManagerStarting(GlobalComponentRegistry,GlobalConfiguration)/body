{
  globalCfg.fluent().serialization().addExternalizer().addExternalizer(new ChunkCacheKey.Externalizer()).addExternalizer(new FileCacheKey.Externalizer()).addExternalizer(new FileListCacheKey.Externalizer()).addExternalizer(new FileMetadata.Externalizer()).addExternalizer(new FileReadLockKey.Externalizer()).build();
}
