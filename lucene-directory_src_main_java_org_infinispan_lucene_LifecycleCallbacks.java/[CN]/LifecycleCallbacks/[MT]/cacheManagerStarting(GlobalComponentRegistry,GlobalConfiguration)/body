{
  globalCfg.fluent().serialization().addAdvancedExternalizer(new ChunkCacheKey.Externalizer()).addAdvancedExternalizer(new FileCacheKey.Externalizer()).addAdvancedExternalizer(new FileListCacheKey.Externalizer()).addAdvancedExternalizer(new FileMetadata.Externalizer()).addAdvancedExternalizer(new FileReadLockKey.Externalizer()).build();
}
