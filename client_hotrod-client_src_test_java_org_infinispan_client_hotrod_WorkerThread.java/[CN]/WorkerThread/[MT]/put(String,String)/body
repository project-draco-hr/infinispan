{
  this.key=key;
  this.value=value;
  try {
    trace("::put::send contains: " + send.peek());
    send.put(PUT_SYNC);
  }
 catch (  InterruptedException e) {
    throw new IllegalStateException(e);
  }
  try {
    String result=(String)receive.take();
    trace("::put::took out of receive: " + result);
    return result == NULL ? null : result;
  }
 catch (  InterruptedException e) {
    throw new IllegalStateException(e);
  }
}
