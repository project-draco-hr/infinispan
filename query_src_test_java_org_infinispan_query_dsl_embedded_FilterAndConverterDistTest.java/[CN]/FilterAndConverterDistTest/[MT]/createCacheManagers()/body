{
  ConfigurationBuilder cfgBuilder=new ConfigurationBuilder();
  cfgBuilder.clustering().cacheMode(CacheMode.DIST_SYNC).transaction().transactionMode(TransactionMode.NON_TRANSACTIONAL).clustering().hash().numOwners(2).stateTransfer().chunkSize(50);
  createClusteredCaches(NUM_NODES,CACHE_NAME,cfgBuilder);
}
