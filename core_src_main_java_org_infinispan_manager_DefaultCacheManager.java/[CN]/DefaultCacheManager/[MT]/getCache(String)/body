{
  assertIsNotTerminated();
  if (cacheName == null)   throw new NullPointerException("Null arguments not allowed");
  CacheWrapper cw=caches.get(cacheName);
  if (cw != null) {
    return cw.getCache();
  }
  if (caches.size() > 0) {
    log.shouldBeUsingStartCache(cacheName);
  }
  return createCache(cacheName);
}
