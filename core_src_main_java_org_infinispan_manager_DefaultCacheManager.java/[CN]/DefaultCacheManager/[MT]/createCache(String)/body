{
  Configuration c=null;
  if (cacheName.equals(DEFAULT_CACHE_NAME) || !configurationOverrides.containsKey(cacheName))   c=globalConfiguration.getDefaultConfiguration().clone();
 else   c=configurationOverrides.get(cacheName);
  c.assertValid();
  Cache cache=new InternalCacheFactory().createCache(c,globalComponentRegistry,cacheName);
  Cache other=caches.putIfAbsent(cacheName,cache);
  if (other == null) {
    cache.start();
    return cache;
  }
 else {
    return other;
  }
}
