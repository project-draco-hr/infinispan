{
  log.tracef("TopologyChangedEvent received: %s",vce);
  maxNumberInvariant.writeLock().lock();
  try {
    address2key.clear();
    addQueuesForAddresses(vce.getConsistentHashAtEnd().getCaches());
    resetNumberOfKeys();
    keyProducerStartLatch.open();
  }
  finally {
    maxNumberInvariant.writeLock().unlock();
  }
}
