{
  if (log.isTraceEnabled()) {
    log.tracef("ViewChange received: %s",vce);
  }
  maxNumberInvariant.writeLock().lock();
  try {
    address2key.clear();
    addQueuesForAddresses(vce.getNewMembers());
    resetNumberOfKeys();
    keyProducerStartLatch.open();
  }
  finally {
    maxNumberInvariant.writeLock().unlock();
  }
}
