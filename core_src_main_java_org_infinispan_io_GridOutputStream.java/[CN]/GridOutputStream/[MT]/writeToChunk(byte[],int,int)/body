{
  int remaining=getBytesRemainingInChunk();
  if (remaining == 0) {
    flush();
    localIndex=0;
    remaining=getChunkSize();
  }
  int bytesToWrite=Math.min(remaining,len);
  System.arraycopy(b,off,currentBuffer,localIndex,bytesToWrite);
  localIndex+=bytesToWrite;
  index+=bytesToWrite;
  return bytesToWrite;
}
