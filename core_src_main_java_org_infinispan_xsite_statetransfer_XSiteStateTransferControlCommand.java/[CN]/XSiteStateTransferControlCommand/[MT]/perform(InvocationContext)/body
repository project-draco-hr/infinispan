{
switch (control) {
case START_SEND:
    provider.startStateTransfer(siteName,getOrigin());
  break;
case START_RECEIVE:
consumer.startStateTransfer();
break;
case FINISH_RECEIVE:
consumer.endStateTransfer();
break;
case FINISH_SEND:
stateTransferManager.notifyStatePushFinished(siteName,getOrigin());
break;
case CANCEL_SEND:
provider.cancelStateTransfer(siteName);
break;
}
return null;
}
