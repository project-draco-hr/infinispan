{
  if (constantValue instanceof String) {
    return "\"" + constantValue + "\"";
  }
  if (constantValue instanceof Character) {
    return "'" + constantValue + "'";
  }
  if (constantValue instanceof Date) {
    return "'" + DateHelper.getJpaDateFormat().format((Date)constantValue) + "'";
  }
  return "" + constantValue;
}
