{
  if (fqn.isRoot())   return false;
  startAtomic();
  boolean result;
  try {
    if (trace)     log.trace("About to remove node " + fqn);
    Node<K,V> n=getNode(fqn.getParent());
    result=n != null && n.removeChild(fqn.getLastElement());
  }
  finally {
    endAtomic();
  }
  if (trace)   log.trace("Node successfully removed");
  return result;
}
