{
  Map<Object,String> values=putValuesInCache();
  EntryRetriever<MagicKey,String> retriever=cache(1,CACHE_NAME).getAdvancedCache().getComponentRegistry().getComponent(EntryRetriever.class);
  CloseableIterator<CacheEntry<MagicKey,String>> iterator=retriever.retrieveEntries(null,null,null);
  Map<MagicKey,String> results=mapFromIterator(iterator);
  assertEquals(values,results);
}
