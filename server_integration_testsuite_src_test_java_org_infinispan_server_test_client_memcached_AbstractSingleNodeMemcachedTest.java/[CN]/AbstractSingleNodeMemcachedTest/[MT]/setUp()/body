{
  mc=new MemcachedClient(MemcachedSingleNodeTest.ENCODING,getServer().getMemcachedEndpoint().getInetAddress().getHostName(),getServer().getMemcachedEndpoint().getPort(),10000);
  mc.delete(KEY_A);
  mc.delete(KEY_B);
  mc.delete(KEY_C);
  assertNull(mc.get(KEY_A));
  assertNull(mc.get(KEY_B));
  assertNull(mc.get(KEY_C));
}
