{
  if (ctx.getLookedUpEntries().isEmpty()) {
    return container.size();
  }
  int size=container.size();
  for (  CacheEntry e : ctx.getLookedUpEntries().values()) {
    if (container.containsKey(e.getKey())) {
      if (e.isRemoved()) {
        size--;
      }
    }
 else     if (!e.isRemoved()) {
      size++;
    }
  }
  return Math.max(size,0);
}
