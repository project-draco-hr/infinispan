{
  return new Operation<String,Integer>(store,"GET"){
    @Override public boolean call(    String key,    long run){
      try {
        InternalCacheEntry ice=store.load(key);
        if (trace)         log.tracef("Loaded key=%s, value=%s",key,ice != null ? ice.getValue() : "null");
        return ice != null;
      }
 catch (      CacheLoaderException e) {
        e.printStackTrace();
        return false;
      }
    }
  }
;
}
