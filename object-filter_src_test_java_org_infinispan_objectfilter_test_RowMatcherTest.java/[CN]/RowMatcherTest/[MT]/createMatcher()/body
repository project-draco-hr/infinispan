{
  RowPropertyHelper.ColumnMetadata[] columns=new RowPropertyHelper.ColumnMetadata[]{new RowPropertyHelper.ColumnMetadata(0,"id",Integer.class),new RowPropertyHelper.ColumnMetadata(1,"name",String.class),new RowPropertyHelper.ColumnMetadata(2,"surname",String.class),new RowPropertyHelper.ColumnMetadata(3,"age",Integer.class),new RowPropertyHelper.ColumnMetadata(4,"gender",Person.Gender.class),new RowPropertyHelper.ColumnMetadata(5,"license",String.class)};
