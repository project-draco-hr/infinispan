{
  QueryFactory qf=Search.getQueryFactory(cache);
  Query q=qf.from(User.class).having("gender").eq(User.Gender.MALE).or().having("name").eq("Spider").or().having("gender").eq(User.Gender.FEMALE).and().having("surname").like("%oe%").toBuilder().build();
  List<User> list=q.list();
  assertEquals(1,list.size());
  assertEquals("John",list.get(0).getName());
}
