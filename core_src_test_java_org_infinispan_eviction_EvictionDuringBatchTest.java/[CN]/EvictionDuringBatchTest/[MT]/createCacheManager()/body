{
  Configuration cfg=new Configuration().fluent().eviction().strategy(EvictionStrategy.LRU).maxEntries(128).expiration().wakeUpInterval(100L).locking().useLockStriping(false).invocationBatching().build();
  EmbeddedCacheManager cm=TestCacheManagerFactory.createCacheManager(cfg);
  cache=cm.getCache();
  cache.addListener(new BaseEvictionFunctionalTest.EvictionListener());
  return cm;
}
