{
  Random r=new Random();
  TopologyInfo topology=new TopologyInfo();
  int i=0;
  for (  Address address : nodes) {
    String machineId="m" + i;
    NodeTopologyInfo nti=new NodeTopologyInfo(machineId,null,null,address);
    topology.addNodeTopologyInfo(address,nti);
    i++;
  }
  return topology;
}
