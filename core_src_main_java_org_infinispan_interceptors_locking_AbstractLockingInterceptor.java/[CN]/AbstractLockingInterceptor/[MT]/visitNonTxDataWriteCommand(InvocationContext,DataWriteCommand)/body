{
  try {
    if (hasSkipLocking(command) || !shouldLockKey(command.getKey())) {
      return invokeNextInterceptor(ctx,command);
    }
    lockAndRecord(ctx,command.getKey(),getLockTimeoutMillis(command));
    return invokeNextInterceptor(ctx,command);
  }
  finally {
    lockManager.unlockAll(ctx);
  }
}
