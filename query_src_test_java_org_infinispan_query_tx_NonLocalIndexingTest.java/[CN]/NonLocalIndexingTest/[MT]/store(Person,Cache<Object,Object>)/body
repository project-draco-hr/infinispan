{
  Callable<Void> callable=new Callable<Void>(){
    @Override public Void call() throws Exception {
      cache.put(person.getName(),person);
      return null;
    }
  }
;
  if (transactionsEnabled()) {
    withTx(cache.getAdvancedCache().getTransactionManager(),callable);
  }
 else {
    callable.call();
  }
}
