{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getUserImplClass()).not(qf.not(qf.having("name").eq("John").or(qf.having("surname").eq("Man")))).toBuilder().build();
  List<User> list=q.list();
  assertEquals(2,list.size());
  assertNotEquals("Woman",list.get(0).getSurname());
}
