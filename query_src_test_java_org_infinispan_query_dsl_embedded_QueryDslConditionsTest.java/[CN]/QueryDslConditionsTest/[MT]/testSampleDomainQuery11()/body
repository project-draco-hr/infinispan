{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(User.class).having("name").eq("John").and().having("addresses.postCode").eq("X1234").and(qf.having("accountIds").eq(1)).toBuilder().build();
  List<User> list=q.list();
  assertEquals(1,list.size());
  assertEquals("Doe",list.get(0).getSurname());
}
