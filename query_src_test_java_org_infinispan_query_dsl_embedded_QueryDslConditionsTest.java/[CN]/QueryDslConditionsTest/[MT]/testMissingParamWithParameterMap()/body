{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getUserImplClass()).having("name").eq(Expression.param("param1")).and().having("gender").eq(Expression.param("param2")).toBuilder().build();
  Map<String,Object> parameterMap=new HashMap<>(1);
  parameterMap.put("param1","John");
  q.setParameters(parameterMap);
  q.list();
}
