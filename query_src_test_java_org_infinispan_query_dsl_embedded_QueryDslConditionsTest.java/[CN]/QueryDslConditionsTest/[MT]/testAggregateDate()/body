{
  QueryFactory qf=getQueryFactory();
  Query q=qf.from(getModelFactory().getTransactionImplClass()).select(Expression.count("date"),Expression.min("date")).having("description").eq("Hotel").toBuilder().groupBy("id").build();
  List<Object[]> list=q.list();
  assertEquals(1,list.size());
  assertEquals(2,list.get(0).length);
  assertEquals(1L,list.get(0)[0]);
  assertEquals(java.util.Date.class,list.get(0)[1].getClass());
  assertTrue(((Date)list.get(0)[1]).compareTo(makeDate("2013-02-27")) == 0);
}
