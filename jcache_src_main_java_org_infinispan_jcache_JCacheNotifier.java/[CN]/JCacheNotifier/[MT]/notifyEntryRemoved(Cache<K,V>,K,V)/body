{
  if (!removedListeners.isEmpty()) {
    List<CacheEntryEvent<? extends K,? extends V>> events=createEvent(cache,key,value);
    for (    CacheEntryListenerRegistration<? super K,? super V> reg : removedListeners) {
      ((CacheEntryRemovedListener<K,V>)reg.getCacheEntryListener()).onRemoved(getEntryIterable(events,reg));
    }
  }
}
