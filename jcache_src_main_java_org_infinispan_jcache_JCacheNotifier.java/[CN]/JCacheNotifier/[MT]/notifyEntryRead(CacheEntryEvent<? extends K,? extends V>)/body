{
  List<CacheEntryEvent<? extends K,? extends V>> events=Collections.<CacheEntryEvent<? extends K,? extends V>>singletonList(event);
  for (  CacheEntryListenerRegistration<? super K,? super V> reg : readListeners) {
    ((CacheEntryReadListener<K,V>)reg.getCacheEntryListener()).onRead(getEntryIterable(events,reg));
  }
}
