{
  List<CacheEntryEvent<? extends K,? extends V>> events=Collections.<CacheEntryEvent<? extends K,? extends V>>singletonList(event);
  for (  CacheEntryListenerRegistration<? super K,? super V> reg : createdListeners) {
    ((CacheEntryCreatedListener<K,V>)reg.getCacheEntryListener()).onCreated(getEntryIterable(events,reg));
  }
}
