{
  if (!expiredListeners.isEmpty()) {
    List<CacheEntryEvent<? extends K,? extends V>> events=createEvent(cache,key,value);
    for (    CacheEntryListenerRegistration<? super K,? super V> reg : expiredListeners) {
      ((CacheEntryExpiredListener<K,V>)reg.getCacheEntryListener()).onExpired(getEntryIterable(events,reg));
    }
  }
}
