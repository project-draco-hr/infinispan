{
  FieldDescriptor field=getField(entityType,propertyPath);
switch (field.getJavaType()) {
case INT:
    return Integer.class;
case LONG:
  return Long.class;
case FLOAT:
return Float.class;
case DOUBLE:
return Double.class;
case BOOLEAN:
return Boolean.class;
case STRING:
return String.class;
case BYTE_STRING:
return ByteString.class;
case ENUM:
return Integer.class;
}
return null;
}
