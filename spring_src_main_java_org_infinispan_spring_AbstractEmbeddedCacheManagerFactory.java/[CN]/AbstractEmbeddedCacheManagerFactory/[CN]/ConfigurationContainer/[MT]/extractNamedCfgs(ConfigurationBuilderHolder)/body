{
  Map<String,ConfigurationBuilder> ncbs=parsed.getNamedConfigurationBuilders();
  Map<String,Configuration> namedCfgs=new HashMap(ncbs.size());
  for (  Map.Entry<String,ConfigurationBuilder> entry : ncbs.entrySet())   namedCfgs.put(entry.getKey(),LegacyConfigurationAdaptor.adapt(entry.getValue().build()));
  return namedCfgs;
}
