{
  withCacheManager(new Callable<EmbeddedCacheManager>(){
    @Override public EmbeddedCacheManager call() throws Exception {
      ConfigurationBuilder cb=new ConfigurationBuilder();
      cb.transaction().use1PcForAutoCommitTransactions(true).transactionManagerLookup(new DummyTransactionManagerLookup());
      DefaultCacheManager cm=new DefaultCacheManager(cb.build());
      cm.getCache();
      return cm;
    }
  }
);
}
