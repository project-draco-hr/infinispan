def unzip_archive(version):
    os.chdir('./target/distribution')
    shutil.rmtree(('infinispan-server-%s' % version), ignore_errors=True)
    if settings['verbose']:
        subprocess.check_call(['unzip', ('infinispan-server-%s-bin.zip' % version)])
    else:
        subprocess.check_call(['unzip', '-q', ('infinispan-server-%s-bin.zip' % version)])
