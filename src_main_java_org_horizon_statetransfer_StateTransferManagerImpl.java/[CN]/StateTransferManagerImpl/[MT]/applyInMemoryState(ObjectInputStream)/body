{
  dataContainer.clear();
  try {
    Set<StoredEntry> set=(Set<StoredEntry>)marshaller.objectFromObjectStream(i);
    for (    StoredEntry se : set)     cache.put(se.getKey(),se.getValue(),se.getLifespan(),MILLISECONDS,Options.CACHE_MODE_LOCAL);
  }
 catch (  Exception e) {
    dataContainer.clear();
    throw new StateTransferException(e);
  }
}
