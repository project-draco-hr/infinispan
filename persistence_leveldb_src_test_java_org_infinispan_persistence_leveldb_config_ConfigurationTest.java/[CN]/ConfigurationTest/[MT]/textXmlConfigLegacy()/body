{
  EmbeddedCacheManager cacheManager=new DefaultCacheManager("config/leveldb-config-legacy-" + LevelDBStoreConfiguration.ImplementationType.AUTO.toString().toLowerCase() + ".xml");
  Cache<String,String> cache=cacheManager.getCache("testCache");
  cache.put("hello","there legacy xml");
  cache.stop();
  cacheManager.stop();
  TestingUtil.recursiveFileRemove("/tmp/leveldb/legacy");
}
