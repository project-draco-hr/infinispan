{
  String key="key", value="value";
  Map<String,String> data=new HashMap<String,String>();
  data.put(key,value);
  assertNull(cache.get(key));
  cache.put(key,value);
  assertEquals(value,cache.get(key));
  cache.remove(key);
  assertNull(cache.get(key));
  cache.putAll(data);
  assertEquals(value,cache.get(key));
}
