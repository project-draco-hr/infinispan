{
  Class<?> subjectType=classExt.readClass(input);
  Map subject=null;
  try {
    subject=(Map)Util.getInstance(subjectType);
  }
 catch (  Exception e) {
    throw new CacheException("Unable to create new instance of ReplicableCommand",e);
  }
  MarshallUtil.unmarshallMap(subject,input);
  return subject;
}
