{
  if (flags != null && flags.length > 0) {
    if (flagsOnCache == null)     tcc.setFlags(flags);
 else {
      EnumSet<Flag> tmp=flagsOnCache.clone();
      tmp.addAll(Arrays.asList(flags));
      tcc.setFlags(tmp);
    }
  }
 else   if (flagsOnCache != null) {
    tcc.setFlags(flagsOnCache);
  }
}
