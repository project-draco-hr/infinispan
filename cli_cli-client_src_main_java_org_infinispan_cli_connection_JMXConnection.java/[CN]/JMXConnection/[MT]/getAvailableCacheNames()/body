{
  ObjectInstance manager=cacheManagers.get(activeCacheManager);
  try {
    String[] cacheNames=(String[])mbsc.invoke(manager.getObjectName(),"getCacheNames",new Object[0],new String[0]);
    List<String> cacheList=Arrays.asList(cacheNames);
    Collections.sort(cacheList);
    return cacheList;
  }
 catch (  Exception e) {
    return Collections.emptyList();
  }
}
