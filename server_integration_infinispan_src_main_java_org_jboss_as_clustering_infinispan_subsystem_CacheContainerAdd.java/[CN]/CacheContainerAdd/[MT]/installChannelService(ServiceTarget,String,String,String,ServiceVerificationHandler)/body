{
  final InjectedValue<ChannelFactory> channelFactory=new InjectedValue<ChannelFactory>();
  return target.addService(ChannelService.getServiceName(containerName),new ChannelService(cluster,channelFactory)).addDependency(ChannelFactoryService.getServiceName(stack),ChannelFactory.class,channelFactory).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
}
