{
  for (  Cache c : caches) {
    try {
      if (c != null && c.getStatus() == ComponentStatus.RUNNING) {
        TransactionManager tm=getTransactionManager(c);
        if (tm != null) {
          try {
            tm.rollback();
          }
 catch (          Exception e) {
          }
        }
        c.stop();
      }
    }
 catch (    Throwable t) {
    }
  }
}
