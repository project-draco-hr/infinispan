{
  EntryBinding<Long> expiryKeyBinding=new SerialBinding<Long>(classCatalog,Long.class);
  EntryBinding<Object> expiryValueBinding=new SerialBinding<Object>(classCatalog,Object.class);
  try {
    return new StoredSortedMap<Long,Object>(database,expiryKeyBinding,expiryValueBinding,true);
  }
 catch (  Exception caught) {
    caught=ExceptionUnwrapper.unwrap(caught);
    throw new DatabaseException("error opening stored map",caught);
  }
}
