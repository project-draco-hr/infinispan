{
  clearTempDir();
  fcs=new LevelDBStore();
  ConfigurationBuilder cb=new ConfigurationBuilder();
  LevelDBStoreConfiguration cfg=createCacheStoreConfig(cb.persistence());
  fcs.init(new DummyInitializationContext(cfg,getCache(),getMarshaller(),new ByteBufferFactoryImpl(),new MarshalledEntryFactoryImpl(getMarshaller())));
  fcs.start();
  return fcs;
}
