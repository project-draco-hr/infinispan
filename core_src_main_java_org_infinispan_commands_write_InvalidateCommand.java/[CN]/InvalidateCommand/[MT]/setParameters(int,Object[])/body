{
  if (commandId != COMMAND_ID)   throw new IllegalStateException("Invalid method id");
  int i=0;
  commandInvocationId=(CommandInvocationId)args[i++];
  int size=(Integer)args[i++];
  keys=new Object[size];
  if (size == 1) {
    keys[0]=args[i];
  }
 else   if (size > 0) {
    System.arraycopy(args,i,keys,0,size);
  }
}
