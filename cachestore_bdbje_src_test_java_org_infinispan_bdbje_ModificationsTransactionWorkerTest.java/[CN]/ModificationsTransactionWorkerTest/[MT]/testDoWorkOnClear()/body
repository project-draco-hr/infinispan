{
  CacheStore cs=createMock(CacheStore.class);
  Clear clear=createMock(Clear.class);
  expect(clear.getType()).andReturn(Modification.Type.CLEAR);
  cs.clear();
  replay(cs);
  replay(clear);
  ModificationsTransactionWorker worker=new ModificationsTransactionWorker(cs,Collections.singletonList(clear));
  worker.doWork();
  verify(cs);
  verify(clear);
}
