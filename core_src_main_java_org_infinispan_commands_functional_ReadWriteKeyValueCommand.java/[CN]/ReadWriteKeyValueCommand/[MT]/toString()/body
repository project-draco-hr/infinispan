{
  return new StringBuilder(getClass().getSimpleName()).append(" {key=").append(toStr(key)).append(", value=").append(toStr(value)).append(", prevValue=").append(toStr(prevValue)).append(", prevMetadata=").append(toStr(prevMetadata)).append(", flags=").append(printFlags()).append(", valueMatcher=").append(valueMatcher).append(", successful=").append(successful).append("}").toString();
}
