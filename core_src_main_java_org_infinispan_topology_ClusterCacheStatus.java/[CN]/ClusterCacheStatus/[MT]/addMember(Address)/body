{
synchronized (this) {
    if (members.contains(joiner)) {
      if (trace)       log.tracef("Trying to add node %s to cache %s, but it is already a member: " + "members = %s, joiners = %s",joiner,cacheName,members,joiners);
      return false;
    }
    members=immutableAdd(members,joiner);
    joiners=immutableAdd(joiners,joiner);
    if (trace)     log.tracef("Added joiner %s to cache %s: members = %s, joiners = %s",joiner,cacheName,members,joiners);
    return true;
  }
}
