{
  CountDownLatch latch=new CountDownLatch(1);
  ReplSyncDistributedExecutorTestCancelCounter.incrementAndGet();
  try {
    latch.await(5000,TimeUnit.MILLISECONDS);
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    ReplSyncDistributedExecutorTestCancelCounter.incrementAndGet();
  }
  return 1;
}
