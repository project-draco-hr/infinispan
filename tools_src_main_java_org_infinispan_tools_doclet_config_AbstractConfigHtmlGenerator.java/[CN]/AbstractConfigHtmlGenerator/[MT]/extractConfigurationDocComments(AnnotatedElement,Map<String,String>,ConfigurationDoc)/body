{
  if (cd != null) {
    if (validString(cd.desc())) {
      m.put(cd.name(),cd.desc());
    }
 else {
      if (e instanceof Class<?>) {
        Class<?> clazz=(Class<?>)e;
        ClassDoc classDoc=rootDoc.classNamed(clazz.getName());
        m.put(cd.name(),classDoc.commentText());
      }
    }
  }
}
