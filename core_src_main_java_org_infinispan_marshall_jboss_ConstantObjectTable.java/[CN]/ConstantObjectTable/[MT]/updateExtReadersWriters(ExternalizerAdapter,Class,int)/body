{
  writers.put(typeClass,adapter);
  ExternalizerAdapter prev=readers.put(readerIndex,adapter);
  if (prev != null)   throw new CacheException(String.format("Duplicate id found! id=%d in %s is shared by another marshallable class (%s). Reader index is %d",adapter.id,adapter.externalizer.getClass().getName(),prev.externalizer.getClass().getName(),readerIndex));
  if (log.isTraceEnabled())   log.trace("Loaded externalizer {0} for {1} with id {2} and reader index {3}",adapter.externalizer.getClass().getName(),typeClass,adapter.id,readerIndex);
}
