{
  String config=INFINISPAN_START_TAG + "<cache-container name=\"container-extra-modules\" default-cache=\"extra-module\">" + "   <local-cache name=\"extra-module\">\n"+ "     <modules>\n"+ "       <sample-element xmlns=\"urn:infinispan:config:mymodule\" sample-attribute=\"test-value\" />\n"+ "     </modules>\n"+ "   </local-cache>\n"+ "</cache-container>"+ INFINISPAN_END_TAG;
  assertCacheConfiguration(config);
}
