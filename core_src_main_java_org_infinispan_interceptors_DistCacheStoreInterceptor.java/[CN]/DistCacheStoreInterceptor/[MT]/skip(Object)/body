{
  List<Address> addresses=dm.locate(key);
  if ((loaderConfig.isShared() && !isFirstOwner(addresses)) || isL1Put(addresses)) {
    if (trace)     log.trace("Passing up method call and bypassing this interceptor since the cache loader is either shared " + "and the caller is not the first owner of the key, or the put call is an L1 put");
    return true;
  }
  return false;
}
