{
  if (closed.get()) {
    throw new IllegalStateException("Iterable has been closed - cannot be reused");
  }
  CloseableIterator<Map.Entry<K,C>> iterator=entryRetriever.retrieveEntries(filter,converter,null);
  iterators.add(iterator);
  if (closed.get()) {
    try {
      iterator.close();
    }
 catch (    Exception e) {
    }
    throw new IllegalStateException("Iterable has been closed - cannot be reused");
  }
  return iterator;
}
