{
  if (retriever == null) {
    throw new NullPointerException("Retriever cannot be null!");
  }
  if (filter == null) {
    throw new NullPointerException("Filter cannot be null!");
  }
  this.entryRetriever=retriever;
  this.filter=filter;
  this.converter=converter;
  this.flags=flags;
  this.cache=cache;
}
