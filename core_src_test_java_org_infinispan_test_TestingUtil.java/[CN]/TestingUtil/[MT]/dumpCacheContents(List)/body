{
  System.out.println("**** START: Cache Contents ****");
  int count=1;
  for (  Object o : caches) {
    Cache c=(Cache)o;
    if (c == null) {
      System.out.println("  ** Cache " + count + " is null!");
    }
 else {
      System.out.println("  ** Cache " + count + " is "+ c.getCacheManager().getAddress());
    }
    count++;
  }
  System.out.println("**** END: Cache Contents ****");
}
