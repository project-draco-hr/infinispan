{
  HashSet<Integer> unfinishedSegments=new HashSet<Integer>(segments);
  unfinishedSegments.removeAll(finishedSegments);
  return "InboundTransferTask{" + "unfinishedSegments=" + unfinishedSegments + ", source="+ source+ ", isCancelled="+ isCancelled+ ", topologyId="+ topologyId+ ", timeout="+ timeout+ '}';
}
