{
switch (fieldNumber) {
case wrappedEnum:
    numericValue=value;
  break;
case wrappedBool:
numericValue=Boolean.TRUE.equals(value) ? IndexingTagHandler.TRUE_INT : IndexingTagHandler.FALSE_INT;
break;
case wrappedBytes:
case wrappedString:
stringValue=(String)value;
break;
case wrappedDouble:
case wrappedFloat:
case wrappedInt64:
case wrappedUInt64:
case wrappedInt32:
case wrappedFixed64:
case wrappedFixed32:
case wrappedUInt32:
case wrappedSFixed32:
case wrappedSFixed64:
case wrappedSInt32:
case wrappedSInt64:
numericValue=value;
break;
case wrappedDescriptorFullName:
messageDescriptor=serCtx.getMessageDescriptor((String)value);
break;
case wrappedMessageBytes:
bytes=(byte[])value;
break;
default :
throw new IllegalStateException("Unexpected field : " + fieldNumber);
}
}
