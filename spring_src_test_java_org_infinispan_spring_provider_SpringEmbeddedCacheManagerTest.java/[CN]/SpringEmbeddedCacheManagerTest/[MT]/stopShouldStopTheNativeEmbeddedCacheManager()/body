{
  final EmbeddedCacheManager nativeCacheManager=new DefaultCacheManager();
  nativeCacheManager.getCache();
  final SpringEmbeddedCacheManager objectUnderTest=new SpringEmbeddedCacheManager(nativeCacheManager);
  objectUnderTest.stop();
  assertEquals("Calling stop() on SpringEmbeddedCacheManager should stop the enclosed " + "Infinispan EmbeddedCacheManager. However, it is still running.",ComponentStatus.TERMINATED,nativeCacheManager.getStatus());
}
