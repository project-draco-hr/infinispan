{
  Cache<String,String> cache1=cache(0), cache2=cache(1);
  cache1.put("k1","v");
  cache1.put("k2","v");
  cache1.put("k3","v");
  tm(1).begin();
  cache2.put("k3","v2");
  Transaction t=tm(1).suspend();
  tm(0).begin();
  cache1.getAdvancedCache().lock(Arrays.asList("k1","k2","k3"));
  tm(0).rollback();
}
