{
  Query query=Search.getQueryFactory(remoteCache).from(UserPB.class).select(Expression.max("age")).having("age").gte(20).toBuilder().build();
  ClientEvents.addContinuousQueryListener(remoteCache,new ContinuousQueryListener<String,Object[]>(){
    @Override public void resultJoining(    String key,    Object[] value){
    }
    @Override public void resultLeaving(    String key){
    }
  }
,query);
}
