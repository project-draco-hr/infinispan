{
  if (serializing) {
    if (bytes == null)     bindings=new HashMap<String,Object>();
 else {
      try {
        ObjectInputStream ois=new ObjectInputStream(new ByteArrayInputStream(bytes));
        bindings=(HashMap<String,Object>)ois.readObject();
        ois.close();
        bytes=null;
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
}
