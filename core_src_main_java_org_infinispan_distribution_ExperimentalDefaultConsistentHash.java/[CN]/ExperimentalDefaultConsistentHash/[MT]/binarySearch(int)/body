{
  int lowerBound=0;
  int upperBound=pool.size() - 1;
  while (lowerBound <= upperBound) {
    int mid=(lowerBound + upperBound) >>> 1;
    int currentHash=pool.get(mid).hash;
    if (currentHash < hash) {
      lowerBound=mid + 1;
    }
 else     if (currentHash > hash) {
      upperBound=mid - 1;
    }
 else {
      return mid;
    }
  }
  return -(lowerBound + 1);
}
