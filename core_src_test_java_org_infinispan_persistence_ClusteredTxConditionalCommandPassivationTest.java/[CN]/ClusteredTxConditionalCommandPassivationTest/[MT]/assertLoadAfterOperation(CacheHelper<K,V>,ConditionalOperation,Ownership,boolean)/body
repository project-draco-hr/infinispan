{
  if (operation == ConditionalOperation.PUT_IF_ABSENT && !skipLoad) {
switch (ownership) {
case PRIMARY_OWNER:
      assertLoad(cacheHelper,1,0,0);
    break;
case BACKUP_OWNER:
  assertLoad(cacheHelper,0,1,0);
break;
}
}
 else {
if (ownership != Ownership.NON_OWNER) {
assertLoad(cacheHelper,skipLoad ? 0 : 1,skipLoad ? 0 : 1,0);
}
}
}
