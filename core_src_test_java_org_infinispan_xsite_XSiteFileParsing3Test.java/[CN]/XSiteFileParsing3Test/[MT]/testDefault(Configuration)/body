{
  TakeOfflineConfiguration toc=new TakeOfflineConfiguration(123,5673);
  assertTrue(dcc.sites().allBackups().contains(new BackupConfiguration("NYC",BackupConfiguration.BackupStrategy.SYNC,12003l,BackupFailurePolicy.IGNORE,null,false,toc,DEFAULT_STATE_TRANSFER,true)));
