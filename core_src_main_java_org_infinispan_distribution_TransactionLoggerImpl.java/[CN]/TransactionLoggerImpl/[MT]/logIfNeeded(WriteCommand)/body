{
  if (enabled) {
    loggingLock.readLock().lock();
    try {
      if (enabled) {
        try {
          commandQueue.put(command);
        }
 catch (        InterruptedException e) {
          Thread.currentThread().interrupt();
        }
        return true;
      }
    }
  finally {
      loggingLock.readLock().unlock();
    }
  }
  return false;
}
