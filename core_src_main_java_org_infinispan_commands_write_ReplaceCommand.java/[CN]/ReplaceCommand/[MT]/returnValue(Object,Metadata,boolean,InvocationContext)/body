{
  this.successful=successful;
  Object previousValue=oldValue == null ? beingReplaced : oldValue;
  if (successful) {
    notifier.notifyCacheEntryModified(key,newValue,previousValue,previousMetadata,true,ctx,this);
  }
  if (oldValue == null) {
    return beingReplaced;
  }
 else {
    return successful;
  }
}
