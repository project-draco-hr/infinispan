{
  for (  LoaderConfiguration c : template.cacheLoaders()) {
    if (c instanceof LegacyStoreConfiguration)     this.addStore().read((LegacyStoreConfiguration)c);
    if (c instanceof LegacyLoaderConfiguration)     this.addLoader().read((LegacyLoaderConfiguration)c);
 else     if (c instanceof FileCacheStoreConfiguration)     this.addFileCacheStore().read((FileCacheStoreConfiguration)c);
  }
  this.passivation=template.passivation();
  this.preload=template.preload();
  this.shared=template.shared();
  return this;
}
