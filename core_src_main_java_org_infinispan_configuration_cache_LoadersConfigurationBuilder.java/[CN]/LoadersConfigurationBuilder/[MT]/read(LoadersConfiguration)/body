{
  for (  AbstractLoaderConfiguration c : template.cacheLoaders()) {
    if (c instanceof LoaderConfiguration)     this.addCacheLoader().read((LoaderConfiguration)c);
 else     if (c instanceof FileCacheStoreConfiguration)     this.addFileCacheStore().read((FileCacheStoreConfiguration)c);
  }
  this.passivation=template.passivation();
  this.preload=template.preload();
  this.shared=template.shared();
  return this;
}
