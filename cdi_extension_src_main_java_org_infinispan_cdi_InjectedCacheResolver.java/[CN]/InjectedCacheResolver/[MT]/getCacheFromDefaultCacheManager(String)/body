{
  Iterable<Cache<?,?>> caches=defaultJCacheManager.getCaches();
  for (  Cache<?,?> cache : caches) {
    if (cache.getName().endsWith(cacheName))     return (Cache<K,V>)cache;
  }
  return (Cache<K,V>)defaultJCacheManager.configureCache(cacheName,defaultCacheManager.getCache().getAdvancedCache());
}
