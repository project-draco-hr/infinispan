{
  ConfigurationBuilder cb=new ConfigurationBuilder();
  cb.clustering().invocationBatching().enable().locking().lockAcquisitionTimeout(1000).isolationLevel(IsolationLevel.REPEATABLE_READ).transaction().lockingMode(LockingMode.OPTIMISTIC).locking().writeSkewCheck(true).versioning().enable().scheme(VersioningScheme.SIMPLE);
  return cb;
}
