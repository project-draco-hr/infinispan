{
  assertEquals(addr1,strategy.nextServer(null));
  assertEquals(addr2,strategy.nextServer(null));
  assertEquals(addr3,strategy.nextServer(null));
  assertEquals(addr1,strategy.nextServer(null));
  assertEquals(addr2,strategy.nextServer(null));
  assertEquals(addr3,strategy.nextServer(null));
  assertEquals(addr1,strategy.nextServer(null));
  assertEquals(addr2,strategy.nextServer(null));
  assertEquals(addr3,strategy.nextServer(null));
}
