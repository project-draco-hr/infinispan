{
  int index=input.read();
  Externalizer ext;
switch (index) {
case 0:
    ext=idViaConfigObjExt;
  break;
case 1:
ext=idViaAnnotationObjExt;
break;
case 2:
ext=idViaBothObjExt;
break;
default :
throw new CacheException(String.format("Unknown index (%d) for externalizer %s",index,this.getClass().getName()));
}
return ext.readObject(input);
}
