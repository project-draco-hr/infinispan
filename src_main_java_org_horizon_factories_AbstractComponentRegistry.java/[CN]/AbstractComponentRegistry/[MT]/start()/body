{
  boolean createdInStart=false;
  if (!state.startAllowed()) {
    if (state.needToDestroyFailedCache())     destroy();
    if (state.needToInitializeBeforeStart()) {
      init();
      createdInStart=true;
    }
 else     return;
  }
  try {
    internalStart(createdInStart);
  }
 catch (  Throwable t) {
    handleLifecycleTransitionFailure(t);
  }
}
