{
  T result=null;
  final long timeout=System.currentTimeMillis() + TIMEOUT;
  while (result == null && System.currentTimeMillis() < timeout) {
    try {
      result=c.call();
    }
 catch (    Exception e) {
      try {
        Thread.sleep(RETRY_TIME);
      }
 catch (      InterruptedException ie) {
      }
    }
  }
  return result;
}
