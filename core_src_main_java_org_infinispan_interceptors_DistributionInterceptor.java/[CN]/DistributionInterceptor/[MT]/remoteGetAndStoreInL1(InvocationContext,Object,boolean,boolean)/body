{
  DataLocality locality=dm.getLocality(key);
  boolean isMappedToLocalNode=locality.isLocal();
  if (ctx.isOriginLocal() && !isMappedToLocalNode && isNotInL1(key)) {
    return realRemoteGet(ctx,key,true,isWrite);
  }
 else {
    if (isMappedToLocalNode && dmWasRehashingDuringLocalLookup) {
      if (trace)       log.tracef("Key %s is mapped to local node %s, but a rehash is in progress so may need to look elsewhere",key,rpcManager.getAddress());
      return realRemoteGet(ctx,key,false,isWrite);
    }
 else {
      if (trace)       log.tracef("Not doing a remote get for key %s since entry is mapped to current node (%s), or is in L1.  Owners are %s",key,rpcManager.getAddress(),dm.locate(key));
    }
  }
  return null;
}
