{
  try {
    Object returnValue=invokeNextInterceptor(ctx,command);
    if (isL1CacheEnabled && !ctx.isOriginLocal() && returnValue != null) {
      l1Manager.addRequestor(command.getKey(),ctx.getOrigin());
    }
    if (needsRemoteGet(ctx,command.getKey(),returnValue == null) && !dm.getLocality(command.getKey()).isLocal())     returnValue=remoteGetAndStoreInL1(ctx,command.getKey(),false);
    return returnValue;
  }
 catch (  SuspectException e) {
    return visitGetKeyValueCommand(ctx,command);
  }
}
