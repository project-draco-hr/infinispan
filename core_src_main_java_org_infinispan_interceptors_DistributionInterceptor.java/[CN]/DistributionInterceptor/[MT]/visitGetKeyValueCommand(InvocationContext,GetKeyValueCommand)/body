{
  boolean isStillRehashingOnJoin=!dm.isJoinComplete();
  Object returnValue=invokeNextInterceptor(ctx,command);
  if (!ctx.hasFlag(Flag.SKIP_REMOTE_LOOKUP) && returnValue == null && ctx.lookupEntry(command.getKey()) == null)   returnValue=remoteGetAndStoreInL1(ctx,command.getKey(),isStillRehashingOnJoin);
  return returnValue;
}
