{
  if (isNeedReliableReturnValues(command) || command.isConditional() || command.hasFlag(Flag.DELTA_WRITE)|| shouldFetchRemoteValuesForWriteSkewCheck(ctx,command)) {
    for (    Object k : keygen.getKeys()) {
      CacheEntry entry=ctx.lookupEntry(k);
      boolean skipRemoteGet=entry != null && entry.skipRemoteGet();
      if (skipRemoteGet) {
        continue;
      }
      InternalCacheEntry ice=remoteGet(ctx,k,true,command);
      if (ice == null) {
        localGet(ctx,k,true,command,false);
      }
    }
  }
}
