{
  try {
    if (ctx.isOriginLocal())     ((VersionedCommitCommand)command).setUpdatedVersions(ctx.getCacheTransaction().getUpdatedEntryVersions());
    return invokeNextInterceptor(ctx,command);
  }
  finally {
    log.fatal("Commit command is " + command);
    if (!ctx.isOriginLocal())     ctx.getCacheTransaction().setUpdatedEntryVersions(((VersionedCommitCommand)command).getUpdatedVersions());
    commitContextEntries(ctx);
  }
}
