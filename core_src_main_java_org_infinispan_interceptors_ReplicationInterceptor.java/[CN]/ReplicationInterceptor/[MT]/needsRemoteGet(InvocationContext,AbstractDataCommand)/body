{
  Object key=command.getKey();
  final CacheEntry entry;
  return !command.hasFlag(Flag.CACHE_MODE_LOCAL) && !command.hasFlag(Flag.SKIP_REMOTE_LOOKUP) && !stateTransferManager.getCacheTopology().getReadConsistentHash().isKeyLocalToNode(rpcManager.getAddress(),key)&& ((entry=ctx.lookupEntry(key)) == null || entry.isNull() || entry.isLockPlaceholder());
}
