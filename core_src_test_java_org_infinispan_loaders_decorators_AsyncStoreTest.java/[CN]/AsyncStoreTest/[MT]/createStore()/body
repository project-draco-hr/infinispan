{
  DummyInMemoryCacheStoreConfigurationBuilder dummyCfg=TestCacheManagerFactory.getDefaultCacheConfiguration(false).loaders().addStore(DummyInMemoryCacheStoreConfigurationBuilder.class).storeName(AsyncStoreTest.class.getName());
  dummyCfg.async().enable().threadPoolSize(10);
  DummyInMemoryCacheStore underlying=new DummyInMemoryCacheStore();
  store=new AsyncStore(underlying);
  store.init(dummyCfg.create(),getCache(),null);
  store.start();
}
