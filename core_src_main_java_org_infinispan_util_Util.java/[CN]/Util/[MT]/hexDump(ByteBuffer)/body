{
  byte[] data=new byte[buffer.remaining()];
  int pos=buffer.position();
  buffer.get(data);
  buffer.position(pos);
  StringBuilder buf=new StringBuilder(buffer.remaining() + 22);
  for (  byte b : data)   buf.append(HEX_VALUES.charAt((b & 0xF0) >> 4)).append(HEX_VALUES.charAt((b & 0x0F)));
  return buf.toString();
}
