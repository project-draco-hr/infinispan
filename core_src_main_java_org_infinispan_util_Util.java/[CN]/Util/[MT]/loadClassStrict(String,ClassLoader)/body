{
  ClassLoader[] cls=new ClassLoader[]{userClassLoader,Util.class.getClassLoader(),ClassLoader.getSystemClassLoader()};
  ClassNotFoundException e=null;
  NoClassDefFoundError ne=null;
  for (  ClassLoader cl : cls) {
    if (cl == null)     continue;
    try {
      return (Class<T>)Class.forName(classname,true,cl);
    }
 catch (    ClassNotFoundException ce) {
      e=ce;
    }
catch (    NoClassDefFoundError ce) {
      ne=ce;
    }
  }
  if (e != null)   throw e;
 else   if (ne != null)   throw new ClassNotFoundException(classname,ne);
 else   throw new IllegalStateException();
}
