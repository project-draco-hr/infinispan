{
  if (o instanceof byte[]) {
    byte[] array=(byte[])o;
    StringBuilder sb=new StringBuilder();
    sb.append("[B0x");
    if (IS_ARRAYS_DEBUG) {
      sb.append(toHexString(array));
    }
 else {
      sb.append(toHexString(array,8));
      sb.append("..");
    }
    return sb.toString();
  }
 else {
    return o.toString();
  }
}
