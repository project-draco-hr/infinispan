{
  if (o instanceof byte[]) {
    byte[] array=(byte[])o;
    StringBuilder sb=new StringBuilder();
    sb.append("[B0x");
    if (IS_ARRAYS_DEBUG) {
      sb.append(toHexString(array));
    }
 else {
      sb.append(toHexString(array,8));
      sb.append("..(");
      sb.append(array.length);
      sb.append(')');
    }
    return sb.toString();
  }
 else   if (o == null) {
    return "null";
  }
 else {
    return o.toString();
  }
}
