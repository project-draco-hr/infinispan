{
  if (o == null) {
    return true;
  }
 else   if (o instanceof String && "".equals(o)) {
    return true;
  }
 else   if (o instanceof List<?> && ((List<?>)o).size() < 1) {
    return true;
  }
 else   if (o instanceof Map<?,?> && ((Map<?,?>)o).size() < 1) {
    return true;
  }
 else   if (o instanceof byte[] && ((byte[])o).length < 1) {
    return true;
  }
  return false;
}
