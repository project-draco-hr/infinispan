{
  final List<Address> members=baseCH.getMembers();
  final int numOwners=baseCH.getNumOwners();
  DefaultConsistentHash rebalanced=new DefaultConsistentHash(baseCH.getHashFunction(),numOwners,1,members,null,new List[]{createOwnersCollection(members,numOwners)});
  return baseCH.equals(rebalanced) ? baseCH : rebalanced;
}
