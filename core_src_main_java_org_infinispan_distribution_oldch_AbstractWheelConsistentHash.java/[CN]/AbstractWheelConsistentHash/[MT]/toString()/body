{
  StringBuilder sb=new StringBuilder(getClass().getSimpleName());
  sb.append(" {");
  for (int i=0; i < positionKeys.length; i++) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append(positionKeys[i]).append(": ").append(positionValues[i]);
  }
  sb.append("}");
  return sb.toString();
}
