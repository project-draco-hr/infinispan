{
  super(delegate);
  this.cacheManager=cache == null ? null : cache.getCacheManager();
  this.cache=cache;
  this.config=config;
  executor=Executors.newSingleThreadExecutor(new ThreadFactory(){
    @Override public Thread newThread(    Runnable r){
      return new Thread(r,THREAD_NAME);
    }
  }
);
}
