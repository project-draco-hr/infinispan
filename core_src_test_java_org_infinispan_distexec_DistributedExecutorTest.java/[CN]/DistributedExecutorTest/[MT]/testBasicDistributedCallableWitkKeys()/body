{
  c1.put("key1","Manik");
  c1.put("key2","Mircea");
  c1.put("key3","Galder");
  c1.put("key4","Sanne");
  DistributedExecutorService des=new DefaultExecutorService(c1);
  Future<Boolean> future=des.submit(new SimpleDistributedCallable(true),new String[]{"key1","key2"});
  Boolean r=future.get();
  assert r;
}
