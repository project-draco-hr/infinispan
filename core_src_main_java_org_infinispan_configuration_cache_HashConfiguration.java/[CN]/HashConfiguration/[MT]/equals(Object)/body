{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  HashConfiguration that=(HashConfiguration)o;
  if (numOwners != that.numOwners)   return false;
  if (numVirtualNodes != that.numVirtualNodes)   return false;
  if (consistentHash != null ? !consistentHash.equals(that.consistentHash) : that.consistentHash != null)   return false;
  if (groupsConfiguration != null ? !groupsConfiguration.equals(that.groupsConfiguration) : that.groupsConfiguration != null)   return false;
  if (hash != null ? !hash.equals(that.hash) : that.hash != null)   return false;
  if (stateTransferConfiguration != null ? !stateTransferConfiguration.equals(that.stateTransferConfiguration) : that.stateTransferConfiguration != null)   return false;
  return true;
}
