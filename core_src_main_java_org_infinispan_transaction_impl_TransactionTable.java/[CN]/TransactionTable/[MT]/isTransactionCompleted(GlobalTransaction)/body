{
  if (completedTransactionsInfo == null)   return false;
  CompletedTransactionsPerNode completedTransactionsPerNode=completedTransactionsInfo.get(gtx.getAddress());
  if (completedTransactionsPerNode == null)   return false;
  if (completedTransactionsPerNode.completedTransactions.containsKey(gtx))   return true;
  return gtx.getId() <= completedTransactionsPerNode.lastPrunedTxId;
}
