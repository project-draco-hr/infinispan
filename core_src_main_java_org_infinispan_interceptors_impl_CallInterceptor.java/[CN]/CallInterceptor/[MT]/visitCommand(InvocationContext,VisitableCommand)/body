{
  if (command instanceof AbstractTransactionBoundaryCommand) {
    if (trace)     log.tracef("Suppressing invocation for %s",command.getClass().getSimpleName());
    return ctx.shortCircuit(null);
  }
  if (trace)   log.tracef("Invoking: %s",command.getClass().getSimpleName());
  return ctx.shortCircuit(command.perform(ctx));
}
