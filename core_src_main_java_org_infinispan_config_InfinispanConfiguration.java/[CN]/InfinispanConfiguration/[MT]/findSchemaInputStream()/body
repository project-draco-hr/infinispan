{
  boolean validating=!skipSchemaValidation();
  if (!validating)   return null;
  FileLookup fileLookup=new FileLookup();
  InputStream is=fileLookup.lookupFile(schemaPath());
  if (is != null)   return is;
  try {
    is=new URL(schemaURL()).openStream();
    return is;
  }
 catch (  Exception e) {
  }
  return null;
}
