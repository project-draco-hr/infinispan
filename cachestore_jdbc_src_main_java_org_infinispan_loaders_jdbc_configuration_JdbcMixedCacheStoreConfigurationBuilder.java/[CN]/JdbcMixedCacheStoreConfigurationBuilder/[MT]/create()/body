{
  return new JdbcMixedCacheStoreConfiguration(batchSize,fetchSize,databaseType,key2StringMapper,binaryTable.create(),stringTable.create(),connectionFactory.create(),lockAcquistionTimeout,lockConcurrencyLevel,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
}
