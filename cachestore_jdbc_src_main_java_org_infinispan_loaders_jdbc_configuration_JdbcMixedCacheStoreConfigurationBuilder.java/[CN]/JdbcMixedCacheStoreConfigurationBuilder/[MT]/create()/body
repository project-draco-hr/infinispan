{
  return new JdbcMixedCacheStoreConfiguration(key2StringMapper,binaryTable.create(),stringTable.create(),driverClass,connectionUrl,username,password,connectionFactoryClass,datasource,lockAcquistionTimeout,lockConcurrencyLevel,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
