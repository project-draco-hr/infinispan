{
  if (!strategy.isEnabled() && getBuilder().loaders().passivation())   log.passivationWithoutEviction();
  if (strategy == EvictionStrategy.FIFO)   log.warn("FIFO strategy is deprecated, LRU will be used instead");
  if (strategy.isEnabled() && maxEntries <= 0)   throw new ConfigurationException("Eviction maxEntries value cannot be less than or equal to zero if eviction is enabled");
  if (maxEntries > 0 && !strategy.isEnabled()) {
    strategy=EvictionStrategy.LIRS;
    log.debugf("Max entries configured (%d) without eviction strategy. Eviction strategy overriden to %s",maxEntries,strategy);
  }
}
