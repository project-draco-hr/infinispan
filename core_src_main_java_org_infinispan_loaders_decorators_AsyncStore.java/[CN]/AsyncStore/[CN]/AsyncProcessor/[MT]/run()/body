{
  clearAllReadLock.lock();
  try {
    innerRun();
  }
 catch (  Throwable t) {
    runAgainAfterWaiting=false;
    log.error("Unexpected error",t);
  }
 finally {
    clearAllReadLock.unlock();
  }
  if (runAgainAfterWaiting) {
    try {
      Thread.sleep(10);
    }
 catch (    InterruptedException e) {
    }
    ensureMoreWorkIsHandled();
  }
}
