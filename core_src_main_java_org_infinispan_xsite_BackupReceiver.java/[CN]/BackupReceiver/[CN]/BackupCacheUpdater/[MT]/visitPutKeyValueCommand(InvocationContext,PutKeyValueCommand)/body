{
  log.tracef("Processing a remote put %s",command);
  if (command.isConditional()) {
    return backupCache.putIfAbsent(command.getKey(),command.getValue(),command.getLifespanMillis(),TimeUnit.MILLISECONDS,command.getMaxIdleTimeMillis(),TimeUnit.MILLISECONDS);
  }
  return backupCache.put(command.getKey(),command.getValue(),command.getLifespanMillis(),TimeUnit.MILLISECONDS,command.getMaxIdleTimeMillis(),TimeUnit.MILLISECONDS);
}
