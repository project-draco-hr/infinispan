{
  long currentTimeMillis=-1;
  while (it.hasNext()) {
    InternalCacheEntry e=it.next();
    if (e.canExpire()) {
      if (currentTimeMillis == -1)       currentTimeMillis=System.currentTimeMillis();
      if (!e.isExpired(currentTimeMillis)) {
        next=e;
        return;
      }
    }
 else {
      next=e;
      return;
    }
  }
}
