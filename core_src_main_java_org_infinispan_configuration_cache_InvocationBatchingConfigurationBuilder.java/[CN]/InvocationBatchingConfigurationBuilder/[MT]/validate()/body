{
  if (enabled && getBuilder().transaction().transactionMode != null && getBuilder().transaction().transactionMode.equals(NON_TRANSACTIONAL))   throw log.invocationBatchingNeedsTransactionalCache();
  if (enabled && getBuilder().transaction().recovery().enabled && !getBuilder().transaction().useSynchronization())   throw log.invocationBatchingCannotBeRecoverable();
}
