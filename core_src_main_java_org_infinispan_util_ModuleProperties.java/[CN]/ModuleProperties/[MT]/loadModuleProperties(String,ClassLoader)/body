{
  Collection<URL> resources=FileLookupFactory.newInstance().lookupFileLocations(MODULE_PROPERTIES_FILENAME,cl);
  if (resources == null)   throw new IOException("Could not find " + MODULE_PROPERTIES_FILENAME + " files on classpath for module "+ moduleName);
  for (  URL url : resources) {
    ModuleProperties props=new ModuleProperties();
    InputStream inStream=url.openStream();
    try {
      props.load(inStream);
    }
  finally {
      Util.close(inStream);
    }
    props.verify();
    if (props.getName().equalsIgnoreCase(moduleName)) {
      return props;
    }
  }
  return null;
}
