{
  final Object removedValue=e.getValue();
  notify(ctx,removedValue,true);
  e.setRemoved(true);
  e.setValid(false);
  e.setChanged(true);
  if (valueMatcher != ValueMatcher.MATCH_EXPECTED_OR_NEW) {
    return isConditional() ? true : removedValue;
  }
 else {
    return isConditional() ? true : value;
  }
}
