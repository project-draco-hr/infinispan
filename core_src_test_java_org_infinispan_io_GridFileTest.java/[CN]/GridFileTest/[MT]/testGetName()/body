{
  assertEquals(fs.getFile("").getName(),"");
  assertEquals(fs.getFile("/").getName(),"");
  assertEquals(fs.getFile("file.txt").getName(),"file.txt");
  assertEquals(fs.getFile("/file.txt").getName(),"file.txt");
  assertEquals(fs.getFile("/dir/file.txt").getName(),"file.txt");
  assertEquals(fs.getFile("/dir/subdir/file.txt").getName(),"file.txt");
  assertEquals(fs.getFile("dir/subdir/file.txt").getName(),"file.txt");
}
