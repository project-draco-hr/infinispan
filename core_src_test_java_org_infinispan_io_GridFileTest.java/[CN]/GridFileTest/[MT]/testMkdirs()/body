{
  assertFalse(mkdirs(""));
  assertFalse(mkdirs("/"));
  assertTrue(mkdirs("myDir1"));
  assertTrue(mkdirs("myDir2/mySubDir"));
  assertTrue(mkdirs("/myDir3"));
  assertTrue(mkdirs("/myDir4/mySubDir"));
  assertTrue(mkdirs("/myDir5/subDir/secondSubDir"));
  createFile("/file.txt");
  assertFalse(mkdirs("/file.txt/dir"));
}
