{
  String filePath="skip.dat";
  OutputStream out=fs.getOutput(filePath);
  try {
    out.write(1);
    out.write(2);
    out.write(3);
  }
  finally {
    out.close();
  }
  InputStream in=fs.getInput(filePath);
  try {
    long skipped=in.skip(2);
    assertEquals(skipped,2);
    assertEquals(in.read(),3);
  }
  finally {
    in.close();
  }
}
