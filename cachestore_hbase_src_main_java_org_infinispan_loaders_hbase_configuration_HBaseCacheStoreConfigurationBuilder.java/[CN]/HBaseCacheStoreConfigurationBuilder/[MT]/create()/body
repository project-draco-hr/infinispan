{
  return new HBaseCacheStoreConfiguration(autoCreateTable,entryColumnFamily,entryTable,entryValueField,expirationColumnFamily,expirationTable,expirationValueField,hbaseZookeeperQuorumHost,hbaseZookeeperClientPort,keyMapper,sharedTable,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
}
