{
  checkNotClosed();
  if (lockRequired(key)) {
    return new WithProcessorLock<V>().call(key,new Callable<V>(){
      @Override public V call(){
        return put(skipCacheLoadCache,skipCacheLoadAndStatsCache,key,value,false);
      }
    }
);
  }
  return put(skipCacheLoadCache,skipCacheLoadAndStatsCache,key,value,false);
}
