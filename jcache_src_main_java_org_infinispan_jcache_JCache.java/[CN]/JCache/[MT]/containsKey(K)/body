{
  checkStarted();
  if (log.isTraceEnabled())   log.tracef("Invoke containsKey(key=%s)",key);
  if (lockRequired(key)) {
    return new WithProcessorLock<Boolean>().call(key,new Callable<Boolean>(){
      @Override public Boolean call(){
        return containsKeyCache.containsKey(key);
      }
    }
);
  }
  return containsKeyCache.containsKey(key);
}
