{
  for (  AdvancedExternalizer ext : internalExternalizers) {
    if (ext instanceof ReplicableCommandExternalizer)     ((ReplicableCommandExternalizer)ext).inject(cmdFactory);
    if (ext instanceof CacheRpcCommandExternalizer)     ((CacheRpcCommandExternalizer)ext).inject(cmdFactory,gcr);
    if (ext instanceof MarshalledValue.Externalizer)     ((MarshalledValue.Externalizer)ext).inject(gcr);
    int id=checkInternalIdLimit(ext.getId(),ext);
    updateExtReadersWritersWithTypes(new ExternalizerAdapter(id,ext));
  }
}
