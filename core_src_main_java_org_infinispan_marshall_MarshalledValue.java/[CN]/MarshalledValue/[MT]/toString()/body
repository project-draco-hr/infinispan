{
  StringBuilder sb=new StringBuilder().append("MarshalledValue(").append("instance=").append(instance != null ? instance.toString() : "<undeserialized>").append("; cachedHashCode=").append(cachedHashCode).append("; serialized=").append(raw != null).append(")@").append(Integer.toHexString(System.identityHashCode(this)));
