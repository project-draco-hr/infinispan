{
  Option<RequestParameters> params=decodeCtx.decoder().readParameters(decodeCtx.header(),in);
  if (params.isDefined()) {
    decodeCtx.params_$eq(params.get());
    if (decodeCtx.header().op().getDecoderRequirements() == DecoderRequirements.PARAMETERS) {
      out.add(decodeCtx);
      resetRequested=true;
      return false;
    }
    return true;
  }
 else {
    return false;
  }
}
