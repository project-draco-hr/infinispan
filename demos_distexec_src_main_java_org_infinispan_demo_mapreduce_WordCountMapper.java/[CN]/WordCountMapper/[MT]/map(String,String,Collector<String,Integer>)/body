{
  chunks++;
  for (  String word : value.split(" ")) {
    if (word != null) {
      String w=word.trim();
      if (w.length() > 0) {
        c.emit(word.toLowerCase(),1);
        words++;
      }
    }
  }
  if (chunks % 1000 == 0)   System.out.printf("Analyzed %s words in %s lines%n",words,chunks);
}
