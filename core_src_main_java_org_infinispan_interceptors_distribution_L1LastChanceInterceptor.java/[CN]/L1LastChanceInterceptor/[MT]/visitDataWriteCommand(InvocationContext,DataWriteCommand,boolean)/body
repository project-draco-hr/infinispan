{
  Object returnValue=ctx.forkInvocationSync(command);
  Object key;
  if (shouldUpdateOnWriteCommand(command) && command.isSuccessful() && cdl.localNodeIsOwner((key=command.getKey()))) {
    if (trace) {
      log.trace("Sending additional invalidation for requestors if necessary.");
    }
    blockOnL1FutureIfNeeded(l1Manager.flushCache(Collections.singleton(key),ctx.getOrigin(),assumeOriginKeptEntryInL1));
  }
  return ctx.shortCircuit(returnValue);
}
