{
  return "ComponentMetadata{" + "name='" + name + '\''+ ", dependencies="+ dependencies+ ", injectMetadata="+ Arrays.toString(injectMetadata)+ ", startMethods="+ Arrays.toString(startMethods)+ ", stopMethods="+ Arrays.toString(stopMethods)+ ", globalScope="+ globalScope+ ", survivesRestarts="+ survivesRestarts+ '}';
}
