{
  record("load");
  if (key == null)   return null;
  InternalCacheEntry se=deserializeEntry(store.get(key));
  if (se == null)   return null;
  if (se.isExpired(System.currentTimeMillis())) {
    log.debugf("Key %s exists, but has expired.  Entry is %s",key,se);
    store.remove(key);
    return null;
  }
  return se;
}
