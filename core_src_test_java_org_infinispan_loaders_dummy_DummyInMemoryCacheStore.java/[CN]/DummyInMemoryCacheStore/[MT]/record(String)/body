{
  boolean replaced;
  long end=System.currentTimeMillis() + 5000;
  do {
    int i=stats.get(method);
    replaced=stats.replace(method,i,i + 1);
    if (!replaced) {
      try {
        Thread.sleep(200);
      }
 catch (      InterruptedException e) {
        Thread.currentThread().interrupt();
      }
    }
  }
 while (!replaced && end < System.currentTimeMillis());
}
