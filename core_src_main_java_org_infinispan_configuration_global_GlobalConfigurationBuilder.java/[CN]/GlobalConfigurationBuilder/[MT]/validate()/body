{
  for (  AbstractGlobalConfigurationBuilder<?> validatable : asList(asyncListenerExecutor,asyncTransportExecutor,evictionScheduledExecutor,replicationQueueScheduledExecutor,globalJmxStatistics,transport,serialization,shutdown,site)) {
    validatable.validate();
  }
  for (  Builder<?> m : modules) {
    m.validate();
  }
}
