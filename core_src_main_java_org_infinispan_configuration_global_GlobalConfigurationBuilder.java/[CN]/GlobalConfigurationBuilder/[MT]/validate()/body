{
  for (  AbstractGlobalConfigurationBuilder<?> validatable : asList(asyncListenerExecutor,asyncTransportExecutor,remoteCommandsExecutor,evictionScheduledExecutor,replicationQueueScheduledExecutor,globalJmxStatistics,transport,serialization,shutdown,site,totalOrderExecutor)) {
    validatable.validate();
  }
  for (  Builder<?> m : modules) {
    m.validate();
  }
}
