{
  validate();
  List<Object> modulesConfig=new LinkedList<Object>();
  for (  Builder<?> module : modules)   modulesConfig.add(module.create());
  return new GlobalConfiguration(evictionThreadPool.create(),listenerThreadPool.create(),replicationQueueThreadPool.create(),persistenceThreadPool.create(),globalJmxStatistics.create(),transport.create(),security.create(),serialization.create(),shutdown.create(),modulesConfig,site.create(),cl.get());
}
