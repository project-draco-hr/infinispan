{
  if (fromIndex < 0 || toIndex > ImmutableListCopy.this.size || fromIndex > toIndex)   throw new IllegalArgumentException("fromIndex(" + fromIndex + "), toIndex("+ toIndex+ "), size ("+ ImmutableListCopy.this.size+ "), List="+ ImmutableListCopy.this.toString());
  offset=fromIndex;
  size=toIndex - fromIndex;
}
