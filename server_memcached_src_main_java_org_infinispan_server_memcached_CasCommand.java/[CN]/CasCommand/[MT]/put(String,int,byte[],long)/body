{
  Value old=(Value)cache.get(key);
  if (old != null) {
    if (old.getCas() == cas) {
      Value value=new Value(flags,data);
      boolean replaced=cache.replace(key,old,value);
      if (replaced)       return Reply.STORED;
 else       return Reply.EXISTS;
    }
 else {
      return Reply.EXISTS;
    }
  }
  return Reply.NOT_FOUND;
}
