{
  Set<Object> objects=container.keySet();
  Set<Object> result=new HashSet<Object>();
  result.addAll(objects);
  for (  CacheEntry ce : ctx.getLookedUpEntries().values()) {
    if (ce.isRemoved()) {
      result.remove(ce.getKey());
    }
 else {
      if (ce.isCreated()) {
        result.add(ce.getKey());
      }
    }
  }
  return result;
}
