{
  Set<HashEntry<K,V>> evicted=null;
  boolean obtainedLock=!lockedAlready ? tryLock() : true;
  if (!obtainedLock && ea.thresholdExpired()) {
    lock();
    obtainedLock=true;
  }
  if (obtainedLock) {
    try {
      evicted=ea.execute();
    }
  finally {
      if (!lockedAlready)       unlock();
    }
  }
  return evicted;
}
