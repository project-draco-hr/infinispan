{
  String message=greetingService.sayHi("Pete");
  assertNotNull(message);
  assertEquals("Hi Pete",message);
  assertEquals(customCache.size(),1);
  message=greetingService.sayHi("Pete");
  assertNotNull(message);
  assertEquals("Hi Pete",message);
