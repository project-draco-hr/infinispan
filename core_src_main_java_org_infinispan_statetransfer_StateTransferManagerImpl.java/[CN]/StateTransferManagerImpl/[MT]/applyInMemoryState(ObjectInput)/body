{
  dataContainer.clear();
  try {
    Set<InternalCacheEntry> set=(Set<InternalCacheEntry>)marshaller.objectFromObjectStream(i);
    for (    InternalCacheEntry se : set)     cache.withFlags(CACHE_MODE_LOCAL,SKIP_SHARED_CACHE_STORE).put(se.getKey(),se.getValue(),se.getLifespan(),MILLISECONDS,se.getMaxIdle(),MILLISECONDS);
  }
 catch (  Exception e) {
    dataContainer.clear();
    throw new StateTransferException(e);
  }
}
