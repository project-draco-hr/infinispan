{
  if (trace)   log.trace("Attempting to evict cached entry with key of {0}",key);
  try {
    cache.evict(key);
    log.trace("Eviction of cached entry with key of {0} successful",key);
    return true;
  }
 catch (  Exception e) {
    if (log.isDebugEnabled())     log.debug("Unable to evict {0}",e,key);
    return false;
  }
}
