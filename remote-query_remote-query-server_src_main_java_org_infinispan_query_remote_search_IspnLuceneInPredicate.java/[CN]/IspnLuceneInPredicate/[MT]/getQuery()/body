{
  LuceneDisjunctionPredicate predicate=new LuceneDisjunctionPredicate(builder);
  for (  Object element : values) {
    IspnLuceneComparisonPredicate equals=new IspnLuceneComparisonPredicate(builder,propertyName,org.hibernate.hql.ast.spi.predicate.ComparisonPredicate.Type.EQUALS,element);
    predicate.add(equals);
  }
  return predicate.getQuery();
}
