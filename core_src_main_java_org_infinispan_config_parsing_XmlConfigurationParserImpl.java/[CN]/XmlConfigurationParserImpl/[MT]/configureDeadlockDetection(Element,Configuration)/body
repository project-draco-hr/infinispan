{
  if (element == null)   return;
  String enabled=getAttributeValue(element,"enabled");
  if (existsAttribute(enabled))   c.setEnableDeadlockDetection(getBoolean(enabled));
  String spinDuration=getAttributeValue(element,"spinDuration");
  if (existsAttribute(spinDuration))   c.setDeadlockDetectionSpinDuration(getLong(spinDuration));
}
