{
  try {
    if (evictionEventQueue.size() > capacityWarnThreshold) {
      if (log.isWarnEnabled())       log.warn("Eviction event queue size is at 98% capacity of {0} on cache {1}. You should reduce the wakeUpInterval attribute.",evictionConfig.getEventQueueSize(),cache.getName());
    }
    evictionEventQueue.put(ee);
  }
 catch (  InterruptedException e) {
    if (log.isDebugEnabled())     log.debug("Interrupted on adding event",e);
    Thread.currentThread().interrupt();
  }
}
