{
  final ConfigureCache annotation=event.getAnnotatedMember().getAnnotation(ConfigureCache.class);
  String configurationName="";
  if (annotation != null) {
    configurationName=annotation.value();
  }
  configurations.add(new ConfigurationHolder((Producer<Configuration>)event.getProducer(),configurationName,Reflections.getQualifiers(beanManager,event.getAnnotatedMember().getAnnotations())));
}
