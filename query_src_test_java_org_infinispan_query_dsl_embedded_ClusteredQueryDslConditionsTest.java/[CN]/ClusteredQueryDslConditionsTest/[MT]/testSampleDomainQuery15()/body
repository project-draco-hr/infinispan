{
  QueryFactory qf=Search.getQueryFactory(cache2);
  Query q=qf.from(User.class).having("accountIds").containsAny(1,3).toBuilder().build();
  List<User> list=q.list();
  assertEquals(2,list.size());
  assertTrue(Arrays.asList(1,2).contains(list.get(0).getId()));
  assertTrue(Arrays.asList(1,2).contains(list.get(1).getId()));
}
