{
  QueryFactory qf=Search.getQueryFactory(cache2);
  Query q=qf.from(Transaction.class).having("accountId").eq(1).and().having("description").like("%rent%").toBuilder().build();
  List<Transaction> list=q.list();
  assertEquals(1,list.size());
  assertEquals(1,list.get(0).getId());
  assertEquals(1,list.get(0).getAccountId());
  assertTrue(list.get(0).getDescription().contains("rent"));
}
