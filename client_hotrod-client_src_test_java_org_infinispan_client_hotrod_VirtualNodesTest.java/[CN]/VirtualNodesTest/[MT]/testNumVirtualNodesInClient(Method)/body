{
  RemoteCacheManager client=client(0);
  client.getCache().put(1,v(m));
  TcpTransportFactory tf=(TcpTransportFactory)extractField(client,"transportFactory");
  SortedMap<Integer,SocketAddress> positions=(SortedMap<Integer,SocketAddress>)extractField(tf.getConsistentHash(),"positions");
  assertEquals(1000,positions.size());
}
