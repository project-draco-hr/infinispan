{
  super.start();
  if (config.getAwsAccessKey() == null)   throw new IllegalArgumentException("awsAccessKey must be set");
  if (config.getAwsSecretKey() == null)   throw new IllegalArgumentException("awsSecretKey must be set");
  this.connection.connect(config,marshaller);
  String s3Bucket=config.getBucket();
  if (s3Bucket == null)   throw new IllegalArgumentException("s3Bucket must be set");
  this.s3Bucket.init(this.connection,connection.verifyOrCreateBucket(s3Bucket));
}
