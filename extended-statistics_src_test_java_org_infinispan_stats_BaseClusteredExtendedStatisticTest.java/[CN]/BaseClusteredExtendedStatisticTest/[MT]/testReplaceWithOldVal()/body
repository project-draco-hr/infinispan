{
  assertEmpty(KEY_1);
  put(1,KEY_1,VALUE_1);
  assertCacheValue(KEY_1,VALUE_1);
  put(0,KEY_1,VALUE_2);
  assertCacheValue(KEY_1,VALUE_2);
  cache(0).replace(KEY_1,VALUE_3,VALUE_4);
  assertCacheValue(KEY_1,VALUE_2);
  replace(0,KEY_1,VALUE_2,VALUE_4);
  assertCacheValue(KEY_1,VALUE_4);
  assertNoTransactions();
  assertNoTxStats();
}
