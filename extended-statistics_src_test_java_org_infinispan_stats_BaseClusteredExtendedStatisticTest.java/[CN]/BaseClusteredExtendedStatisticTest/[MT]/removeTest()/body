{
  assertEmpty(KEY_1);
  cache(1).put(KEY_1,VALUE_1);
  assertCacheValue(KEY_1,VALUE_1);
  cache(0).remove(KEY_1);
  assertCacheValue(KEY_1,null);
  cache(0).put(KEY_1,VALUE_1);
  assertCacheValue(KEY_1,VALUE_1);
  cache(0).remove(KEY_1);
  assertCacheValue(KEY_1,null);
  assertNoTransactions();
  assertNoTxStats();
}
