{
  Descriptor md=messageDescriptor;
  for (  String prop : propertyPath) {
    FieldDescriptor fd=md.findFieldByName(prop);
    if (fd.isRepeated()) {
      return true;
    }
    if (fd.getJavaType() == JavaType.MESSAGE) {
      md=fd.getMessageType();
    }
 else {
      md=null;
    }
  }
  return false;
}
