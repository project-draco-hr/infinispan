{
  try {
    if (protocolServer != null) {
      ROOT_LOGGER.connectorStopping(serverName);
      try {
        protocolServer.stop();
      }
 catch (      Exception e) {
        ROOT_LOGGER.connectorStopFailed(e,serverName);
      }
    }
  }
  finally {
    ROOT_LOGGER.connectorStopped(serverName);
  }
}
