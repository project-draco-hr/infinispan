{
  Object returnValue;
  if (ctx.isOriginLocal()) {
    returnValue=invokeNextInterceptor(ctx,command);
  }
 else {
    for (    Object key : command.getKeys()) {
      l1Manager.addRequestor(key,ctx.getOrigin());
    }
    returnValue=invokeNextInterceptor(ctx,command);
  }
  return returnValue;
}
