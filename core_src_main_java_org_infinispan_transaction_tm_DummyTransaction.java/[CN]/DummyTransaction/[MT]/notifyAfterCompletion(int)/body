{
  if (syncs == null)   return;
  for (  Synchronization s : syncs) {
    if (trace) {
      log.trace("processing afterCompletion for " + s);
    }
    try {
      s.afterCompletion(status);
    }
 catch (    Throwable t) {
      log.error("afterCompletion() failed for " + s,t);
    }
  }
  syncs.clear();
}
