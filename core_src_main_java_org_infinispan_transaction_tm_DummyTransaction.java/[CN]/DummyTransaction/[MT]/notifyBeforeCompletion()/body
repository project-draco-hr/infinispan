{
  boolean retval=true;
  if (syncs == null)   return true;
  for (  Synchronization s : syncs) {
    if (trace)     log.tracef("processing beforeCompletion for %s",s);
    try {
      s.beforeCompletion();
    }
 catch (    Throwable t) {
      retval=false;
      log.beforeCompletionFailed(s,t);
    }
  }
  return retval;
}
