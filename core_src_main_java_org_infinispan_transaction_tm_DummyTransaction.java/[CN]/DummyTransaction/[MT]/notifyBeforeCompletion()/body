{
  boolean retval=true;
  if (syncs == null)   return true;
  for (  Synchronization s : syncs) {
    if (log.isDebugEnabled()) {
      log.debug("processing beforeCompletion for " + s);
    }
    try {
      s.beforeCompletion();
    }
 catch (    Throwable t) {
      retval=false;
      log.error("beforeCompletion() failed for " + s,t);
    }
  }
  return retval;
}
