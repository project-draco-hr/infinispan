{
  checkStartedStatus();
  ArrayList<Cache<?,?>> cacheList;
synchronized (caches) {
    cacheList=new ArrayList<Cache<?,?>>(caches.values());
    caches.clear();
  }
  for (  Cache<?,?> cache : cacheList) {
    try {
      cache.stop();
    }
 catch (    Exception e) {
    }
  }
  cm.stop();
  status=Status.STOPPED;
}
