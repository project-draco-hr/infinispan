{
  assertNotInCacheAndStore("k1","k2","k3");
  cache.put("k1","v1");
  cache.put("k2","v2");
  cache.evict("k2");
  assertInCacheNotInStore("k1","v1");
  assertInStoreNotInCache("k2","v2");
  Map m=new HashMap();
  m.put("k1","v1-NEW");
  m.put("k2","v2-NEW");
  m.put("k3","v3-NEW");
  cache.putAll(m);
  assertInCacheNotInStore("k1","v1-NEW");
  assertInCacheNotInStore("k2","v2-NEW");
  assertInCacheNotInStore("k3","v3-NEW");
}
