{
  if (!cfg.isLazyPurgingOnly()) {
    acquireGlobalLock(false);
    try {
      if (multiThreadedPurge) {
        purgerService.execute(new Runnable(){
          public void run(){
            try {
              purge();
            }
 catch (            Exception e) {
              log.problemsPurging(e);
            }
          }
        }
);
      }
 else {
        purge();
      }
    }
  finally {
      releaseGlobalLock(false);
    }
  }
}
