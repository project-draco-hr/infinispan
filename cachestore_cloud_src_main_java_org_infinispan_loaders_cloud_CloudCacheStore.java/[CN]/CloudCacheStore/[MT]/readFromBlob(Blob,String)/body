{
  if (blob == null)   return null;
  try {
    Bucket bucket=(Bucket)marshaller.objectFromInputStream(blob.getContent());
    if (bucket != null)     bucket.setBucketName(bucketName);
    return bucket;
  }
 catch (  Exception e) {
    throw convertToCacheLoaderException("Unable to read blob",e);
  }
}
