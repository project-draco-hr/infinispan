import memcache
import time
ip = '127.0.0.1'
port = '13211'
ipaddress = ((ip + ':') + port)
mc = memcache.Client([ipaddress], debug=1)
print 'Connecting to {0}'.format(ipaddress)
key = 'Simple_Key'
expected = 'Simple value'
print "Testing get ['{0}'] should return {1} ...".format(key, expected),
get(mc, key, expected)
key = 'Expiring_Key'
print "Testing get ['{0}'] should return nothing...".format(key),
time.sleep(5)
getNone(mc, key)
key = 'Incr_Key'
expected = '4'
print "Testing get ['{0}'] should return {1} ...".format(key, expected),
get(mc, key, expected)
key = 'Decr_Key'
expected = '3'
print "Testing get ['{0}'] should return {1} ...".format(key, expected),
get(mc, key, expected)
key = 'Multi_Decr_Key'
expected = '1'
print "Testing get ['{0}'] should return {1} ...".format(key, expected),
get(mc, key, expected)
