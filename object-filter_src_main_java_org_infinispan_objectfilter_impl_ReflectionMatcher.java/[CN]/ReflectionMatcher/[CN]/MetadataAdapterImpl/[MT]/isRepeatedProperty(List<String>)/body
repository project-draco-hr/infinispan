{
  ReflectionHelper.PropertyAccessor a=ReflectionHelper.getAccessor(clazz,propertyPath.get(0));
  if (a.isMultiple()) {
    return true;
  }
  for (int i=1; i < propertyPath.size(); i++) {
    a=a.getAccessor(propertyPath.get(i));
    if (a.isMultiple()) {
      return true;
    }
  }
  return false;
}
