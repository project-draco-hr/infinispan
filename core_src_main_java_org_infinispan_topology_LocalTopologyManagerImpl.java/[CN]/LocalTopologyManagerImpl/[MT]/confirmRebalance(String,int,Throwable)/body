{
  ReplicableCommand command=new CacheTopologyControlCommand(cacheName,CacheTopologyControlCommand.Type.REBALANCE_CONFIRM,transport.getAddress(),topologyId,throwable);
  try {
    executeOnCoordinatorAsync(command);
  }
 catch (  Exception e) {
    log.debugf(e,"Error sending the rebalance completed notification for cache %s to the coordinator",cacheName);
  }
}
