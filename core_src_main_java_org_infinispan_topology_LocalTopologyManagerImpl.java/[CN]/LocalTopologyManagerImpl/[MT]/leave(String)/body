{
  runningCaches.remove(cacheName);
  ReplicableCommand command=new CacheTopologyControlCommand(cacheName,CacheTopologyControlCommand.Type.LEAVE,transport.getAddress(),null);
  try {
    executeOnCoordinatorAsync(command);
  }
 catch (  Exception e) {
    log.debugf(e,"Error sending the leave request for cache %s to coordinator",cacheName);
  }
}
