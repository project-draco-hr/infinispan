{
  K key=entry.getKey();
  V prev=entry.getValue();
  MetaParams prevMetas=extractMetaParams(entry);
  entry.setValue(value);
  entry.setChanged(true);
  MetaParams newMetas=updateMetaParams(entry,metas);
  notifier.notifyOnModify(EntryViews.readOnly(key,prev,prevMetas),EntryViews.readOnly(key,value,newMetas));
}
