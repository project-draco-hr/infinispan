{
  final EmbeddedCacheManager infinispanEmbeddedCacheManagerWithDefaultConfiguration=this.applicationContext.getBean(INFINISPAN_EMBEDDED_CACHE_MANAGER_WITH_DEFAULT_CONFIGURATION_BEAN_NAME,EmbeddedCacheManager.class);
  assertNotNull("Spring application context should contain a EmbeddedCacheManager with default settings having bean name = \"" + INFINISPAN_EMBEDDED_CACHE_MANAGER_WITH_DEFAULT_CONFIGURATION_BEAN_NAME + "\". However, it doesn't.",infinispanEmbeddedCacheManagerWithDefaultConfiguration);
}
