{
  QueryFactory qf=Search.getQueryFactory(remoteCache);
  Query q=qf.from(User.class).orderBy("surname",SortOrder.DESC).having("gender").eq(User.Gender.MALE).or().having("name").eq("Spider").and().having("gender").eq(User.Gender.FEMALE).or().having("surname").like("%oe%").toBuilder().build();
  List<User> list=q.list();
  assertEquals(2,list.size());
  assertEquals("Woman",list.get(0).getSurname());
  assertEquals("Doe",list.get(1).getSurname());
}
