{
  if (payloadStarted) {
    if (skipping == 0) {
      AttributeNode<Integer> attrNode=currentNode.getChild(fieldNumber);
      if (attrNode != null) {
        currentNode=attrNode;
        return;
      }
    }
    skipping++;
  }
 else {
    throw new IllegalStateException("No nested message is expected");
  }
}
