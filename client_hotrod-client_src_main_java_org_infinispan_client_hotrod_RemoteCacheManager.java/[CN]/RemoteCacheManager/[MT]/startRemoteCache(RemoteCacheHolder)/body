{
  RemoteCacheImpl<?,?> remoteCache=remoteCacheHolder.remoteCache;
  OperationsFactory operationsFactory=new OperationsFactory(transportFactory,remoteCache.getName(),topologyId,remoteCacheHolder.forceReturnValue,codec);
  remoteCache.init(marshaller,asyncExecutorService,operationsFactory,configuration.keySizeEstimate(),configuration.valueSizeEstimate());
}
