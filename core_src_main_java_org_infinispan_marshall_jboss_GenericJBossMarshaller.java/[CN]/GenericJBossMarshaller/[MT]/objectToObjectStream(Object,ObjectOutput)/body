{
  ClassLoader toUse=defaultCl;
  Thread current=Thread.currentThread();
  ClassLoader old=current.getContextClassLoader();
  if (old == null) {
    try {
      current.setContextClassLoader(toUse);
      out.writeObject(obj);
    }
  finally {
      current.setContextClassLoader(old);
    }
  }
 else {
    out.writeObject(obj);
  }
}
