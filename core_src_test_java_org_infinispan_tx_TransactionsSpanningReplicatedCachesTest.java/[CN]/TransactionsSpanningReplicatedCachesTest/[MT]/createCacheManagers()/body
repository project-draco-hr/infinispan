{
  Configuration c=getConfiguration();
  cm1=addClusterEnabledCacheManager(c);
  cm2=addClusterEnabledCacheManager();
  defineConfigurationOnAllManagers("c1",c);
  defineConfigurationOnAllManagers("c2",c);
  cache(0,"c1");
  cache(0,"c2");
  cache(1,"c1");
  cache(1,"c2");
  cache(0,"cache1");
  cache(0,"cache2");
  cache(1,"cache1");
  cache(1,"cache2");
  cache(0);
  cache(1);
}
