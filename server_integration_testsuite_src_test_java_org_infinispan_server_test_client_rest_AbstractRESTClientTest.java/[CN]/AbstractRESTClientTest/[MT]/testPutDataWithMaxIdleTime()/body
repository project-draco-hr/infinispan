{
  URI fullPathKey=fullPathKey(KEY_A);
  post(fullPathKey,"data","application/text",HttpServletResponse.SC_OK,"Content-Type","application/text","maxIdleTimeSeconds","2");
  get(fullPathKey,"data");
  for (int i=1; i < 3; i++) {
    Thread.sleep(1000);
    head(fullPathKey);
  }
  Thread.sleep(2100);
  head(fullPathKey,HttpServletResponse.SC_NOT_FOUND);
}
