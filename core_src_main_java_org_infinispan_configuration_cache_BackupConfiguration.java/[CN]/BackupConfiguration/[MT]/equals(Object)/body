{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  BackupConfiguration other=(BackupConfiguration)obj;
  if (attributes == null) {
    if (other.attributes != null)     return false;
  }
 else   if (!attributes.equals(other.attributes))   return false;
  if (takeOfflineConfiguration == null) {
    if (other.takeOfflineConfiguration != null)     return false;
  }
 else   if (!takeOfflineConfiguration.equals(other.takeOfflineConfiguration))   return false;
  if (xSiteStateTransferConfiguration == null) {
    if (other.xSiteStateTransferConfiguration != null)     return false;
  }
 else   if (!xSiteStateTransferConfiguration.equals(other.xSiteStateTransferConfiguration))   return false;
  return true;
}
