{
  ClusteredQueryInvoker invoker=new ClusteredQueryInvoker(cache);
  Object value=null;
  try {
    value=invoker.getValue(scoreDoc.index,topDoc.getNodeAddress(),queryId);
  }
 catch (  Exception e) {
    log.error("Error while trying to remoting fetch next value: " + e.getMessage());
  }
  return value;
}
