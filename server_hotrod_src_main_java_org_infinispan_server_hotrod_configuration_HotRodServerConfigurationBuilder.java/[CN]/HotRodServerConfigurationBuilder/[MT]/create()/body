{
  return new HotRodServerConfiguration(defaultCacheName,proxyHost == null ? host : proxyHost,proxyPort < 0 ? port : proxyPort,topologyLockTimeout,topologyReplTimeout,topologyAwaitInitialTransfer,topologyStateTransfer,name,host,port,idleTimeout,recvBufSize,sendBufSize,ssl.create(),tcpNoDelay,workerThreads,authentication.create(),ignoredCaches);
}
