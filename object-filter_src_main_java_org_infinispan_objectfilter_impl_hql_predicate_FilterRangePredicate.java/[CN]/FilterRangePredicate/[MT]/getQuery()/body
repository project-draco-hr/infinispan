{
  return new AndExpr(new ComparisonExpr(new PropertyValueExpr(propertyName,isRepeatedProperty),new ConstantValueExpr((Comparable)lower),ComparisonExpr.Type.GREATER_OR_EQUAL),new ComparisonExpr(new PropertyValueExpr(propertyName,isRepeatedProperty),new ConstantValueExpr((Comparable)upper),ComparisonExpr.Type.LESS_OR_EQUAL));
}
