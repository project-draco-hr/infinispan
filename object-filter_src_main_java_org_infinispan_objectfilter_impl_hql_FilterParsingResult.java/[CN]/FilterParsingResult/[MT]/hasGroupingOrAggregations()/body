{
  if (groupBy != null || havingBuilder.build() != null) {
    return true;
  }
  if (projectedPaths != null) {
    for (    PropertyPath p : projectedPaths) {
      if (p.getAggregationType() != null) {
        return true;
      }
    }
  }
  if (sortFields != null) {
    for (    SortField s : sortFields) {
      if (s.getPath().getAggregationType() != null) {
        return true;
      }
    }
  }
  return false;
}
