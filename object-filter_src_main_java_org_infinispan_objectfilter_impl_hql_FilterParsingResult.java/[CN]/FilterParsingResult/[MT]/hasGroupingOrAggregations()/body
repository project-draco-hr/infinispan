{
  if (havingFilter != null || !groupBy.isEmpty()) {
    return true;
  }
  for (  PropertyPath p : projectedPaths) {
    if (p.getAggregationType() != null) {
      return true;
    }
  }
  for (  SortField s : sortFields) {
    if (s.getPath().getAggregationType() != null) {
      return true;
    }
  }
  return false;
}
