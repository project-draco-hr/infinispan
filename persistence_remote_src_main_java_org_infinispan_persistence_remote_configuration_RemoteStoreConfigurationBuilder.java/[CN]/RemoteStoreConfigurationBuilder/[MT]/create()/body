{
  List<RemoteServerConfiguration> remoteServers=new ArrayList<RemoteServerConfiguration>();
  for (  RemoteServerConfigurationBuilder server : servers) {
    remoteServers.add(server.create());
  }
  return new RemoteStoreConfiguration(purgeOnStartup,fetchPersistentState,ignoreModifications,async.create(),singletonStore.create(),preload,shared,properties,asyncExecutorFactory.create(),balancingStrategy,connectionPool.create(),connectionTimeout,forceReturnValues,hotRodWrapping,keySizeEstimate,marshaller,pingOnStartup,protocolVersion,rawValues,remoteCacheName,remoteServers,socketTimeout,tcpNoDelay,transportFactory,valueSizeEstimate);
}
