{
  this.cacheName=cacheName;
  this.keys=null;
  this.singleKey=null;
  if (keys != null && !keys.isEmpty()) {
    if (keys.size() == 1) {
      for (      Object k : keys)       this.singleKey=k;
    }
 else {
      this.keys=new HashSet<Object>(keys);
    }
  }
  this.flags=flags;
  this.implicit=implicit;
}
