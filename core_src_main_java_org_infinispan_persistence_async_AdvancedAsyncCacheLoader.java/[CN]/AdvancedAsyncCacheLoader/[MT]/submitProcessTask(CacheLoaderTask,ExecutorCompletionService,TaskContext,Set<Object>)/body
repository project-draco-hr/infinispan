{
  ecs.submit(new Callable(){
    @Override public Object call() throws Exception {
      for (      Object k : batch) {
        if (taskContext.isStopped())         break;
        MarshalledEntry load=load(k);
        if (load != null)         cacheLoaderTask.processEntry(load,taskContext);
      }
      return null;
    }
  }
);
}
