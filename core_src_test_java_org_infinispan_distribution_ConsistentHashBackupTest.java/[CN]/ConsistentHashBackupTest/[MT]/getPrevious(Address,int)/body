{
  List<Address> addressList=ch.getAddressOnTheWheel();
  int pos=addressList.indexOf(a);
  List<Address> result=new ArrayList<Address>();
  for (int i=1; i <= count; i++) {
    int index;
    if (pos - i < 0) {
      index=addressList.size() + (pos - i);
    }
 else {
      index=pos - i;
    }
    result.add(addressList.get((index)));
  }
  return result;
}
