{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (ChunkCacheKey.class != obj.getClass())   return false;
  ChunkCacheKey other=(ChunkCacheKey)obj;
  if (chunkId != other.chunkId)   return false;
  if (fileName == null) {
    if (other.fileName != null)     return false;
  }
 else   if (!fileName.equals(other.fileName))   return false;
  if (indexName == null) {
    if (other.indexName != null)     return false;
  }
 else   if (!indexName.equals(other.indexName))   return false;
  return true;
}
