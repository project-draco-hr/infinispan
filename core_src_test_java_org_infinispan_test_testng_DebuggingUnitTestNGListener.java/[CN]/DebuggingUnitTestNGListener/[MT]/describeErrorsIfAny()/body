{
  if (!failedTestDescriptions.isEmpty()) {
    log("~~~~~~~~~~~~~~~~~~~~~~~~~ TEST HEALTH INFO ~~~~~~~~~~~~~~~~~~~~~~~~~~");
    log("Some tests didn't properly shutdown the CacheManager:");
    for (    String errorMsg : failedTestDescriptions) {
      System.out.println("\t" + errorMsg);
    }
    log("~~~~~~~~~~~~~~~~~~~~~~~~~ TEST HEALTH INFO ~~~~~~~~~~~~~~~~~~~~~~~~~~");
  }
}
