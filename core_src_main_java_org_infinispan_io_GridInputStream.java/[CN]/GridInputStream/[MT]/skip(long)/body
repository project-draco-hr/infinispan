{
  checkClosed();
  if (len <= 0)   return 0;
  int bytesToSkip=Math.min((int)len,getBytesRemainingInStream());
  index+=bytesToSkip;
  if (bytesToSkip <= getBytesRemainingInChunk()) {
    localIndex+=bytesToSkip;
  }
 else {
    fetchChunk();
    localIndex=index % getChunkSize();
  }
  return bytesToSkip;
}
