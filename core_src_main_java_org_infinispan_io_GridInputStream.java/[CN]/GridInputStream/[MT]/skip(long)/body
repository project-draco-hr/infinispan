{
  assertOpen();
  if (length <= 0)   return 0;
  int bytesToSkip=Math.min((int)length,getBytesRemainingInStream());
  index+=bytesToSkip;
  if (bytesToSkip <= getBytesRemainingInChunk()) {
    localIndex+=bytesToSkip;
  }
 else {
    getChunk();
    localIndex=index % getChunkSize();
  }
  return bytesToSkip;
}
