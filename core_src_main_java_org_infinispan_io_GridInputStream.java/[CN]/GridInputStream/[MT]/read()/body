{
  checkClosed();
  int remaining=getBytesRemainingInChunk();
  if (remaining == 0) {
    if (endReached)     return -1;
    fetchNextChunk();
    if (currentBuffer == null)     return -1;
 else     if (isLastChunk())     endReached=true;
  }
  int retval=0x0ff & currentBuffer[localIndex++];
  index++;
  return retval;
}
