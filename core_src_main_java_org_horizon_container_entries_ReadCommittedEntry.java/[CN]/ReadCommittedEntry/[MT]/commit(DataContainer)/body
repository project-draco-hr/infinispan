{
  if (isChanged()) {
    if (trace)     log.trace("Updating entry (key={0} removed={1} valid={2} changed={3} created={4} value={5}]",getKey(),isRemoved(),isValid(),isChanged(),isCreated(),value);
    if (isRemoved()) {
      container.remove(key);
    }
 else {
      container.put(key,value,lifespan,maxIdle);
    }
    reset();
  }
}
