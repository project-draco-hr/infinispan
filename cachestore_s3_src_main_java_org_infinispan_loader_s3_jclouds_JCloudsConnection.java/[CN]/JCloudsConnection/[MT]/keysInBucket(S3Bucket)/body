{
  try {
    Set<String> keys=new HashSet<String>();
    for (    S3Object object : s3Service.getBucket(bucket).get().getContents()) {
      keys.add(object.getKey());
    }
    return keys;
  }
 catch (  Exception ex) {
    throw convertToS3ConnectionException("Exception while listing bucket " + bucket,ex);
  }
}
