{
  Status convertedStatus;
switch (status) {
case FAILED:
case TERMINATED:
case STOPPING:
    convertedStatus=Status.STOPPED;
  break;
case INITIALIZING:
case INSTANTIATED:
convertedStatus=Status.UNINITIALISED;
break;
case RUNNING:
convertedStatus=Status.STARTED;
break;
default :
convertedStatus=Status.UNINITIALISED;
break;
}
return convertedStatus;
}
