{
  StringBuilder sb=new StringBuilder();
  sb.append('[');
  Iterator<?> it=iterator();
  if (it.hasNext()) {
    for (; ; ) {
      Object e=it.next();
      String eStr=this instanceof KeySetView ? map.keyEq.toString(e) : this instanceof ValuesView ? map.valueEq.toString(e) : e.toString();
      sb.append(e == this ? "(this Collection)" : eStr);
      if (!it.hasNext())       break;
      sb.append(',').append(' ');
    }
  }
  return sb.append(']').toString();
}
