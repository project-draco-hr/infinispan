{
  if (cache.getCacheConfiguration().indexing().enabled()) {
    try {
      return executeQuery(cache,query);
    }
 catch (    IOException e) {
      throw new CacheException("An exception has occurred during query execution",e);
    }
  }
  try {
    return executeNonIndexedQuery(cache,query);
  }
 catch (  IOException e) {
    throw new CacheException("An exception has occurred during query execution",e);
  }
}
