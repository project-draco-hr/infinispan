{
  for (int i=0; i < numElements; i++) {
    try {
      Object e=queue.poll(5,TimeUnit.SECONDS);
      assertNotNull(e);
    }
 catch (    InterruptedException e) {
      throw new AssertionError("Interrupted while waiting for condition",e);
    }
  }
  try {
    Object e=queue.poll(5,TimeUnit.SECONDS);
    assertNull(e);
  }
 catch (  InterruptedException e) {
    throw new AssertionError("Interrupted while waiting for condition",e);
  }
}
