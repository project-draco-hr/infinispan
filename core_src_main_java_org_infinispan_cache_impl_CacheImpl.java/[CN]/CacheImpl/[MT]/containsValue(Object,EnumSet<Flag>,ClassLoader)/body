{
  assertValueNotNull(value);
  try (CloseableIterable<CacheEntry<K,V>> iterable=filterEntries(AcceptAllKeyValueFilter.getInstance(),explicitFlags,explicitClassLoader)){
    for (    CacheEntry<K,V> entry : iterable) {
      if (value.equals(entry.getValue())) {
        return true;
      }
    }
  }
   return false;
}
