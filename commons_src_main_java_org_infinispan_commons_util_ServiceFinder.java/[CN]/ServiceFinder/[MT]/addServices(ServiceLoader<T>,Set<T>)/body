{
  Iterator<T> i=loadedServices.iterator();
  while (i.hasNext()) {
    try {
      T service=i.next();
      LOG.debugf("Loading service impl: %s",service.getClass().getSimpleName());
      services.add(service);
    }
 catch (    ServiceConfigurationError e) {
      LOG.debugf("Skipping service impl",e);
    }
  }
}
