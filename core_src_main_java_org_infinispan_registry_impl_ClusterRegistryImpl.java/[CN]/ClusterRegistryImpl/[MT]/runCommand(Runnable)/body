{
  while (true) {
    try {
      runnable.run();
      break;
    }
 catch (    CacheException e) {
      if (SuspectException.isSuspectExceptionInChain(e)) {
        log.trace("Ignoring suspect exception and retrying operation for ClusterRegistry.");
      }
 else {
        throw e;
      }
    }
  }
}
