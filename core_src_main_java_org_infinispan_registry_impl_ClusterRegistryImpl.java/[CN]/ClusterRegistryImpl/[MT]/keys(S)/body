{
  startRegistryCache();
  Set<K> result=new HashSet<K>();
  Transaction tx=suspendTx();
  try {
    for (    ScopedKey<S,K> key : clusterRegistryCache.keySet()) {
      if (key.hasScope(scope)) {
        result.add(key.getKey());
      }
    }
    return result;
  }
  finally {
    resumeTx(tx);
  }
}
