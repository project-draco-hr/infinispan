{
  int h=hash(k.hashCode());
  Segment s=segmentFor(h);
  LinkedEntry le=s.get(k,h);
  InternalCacheEntry ice=null;
  if (le != null) {
    ice=le.e;
    if (isMarkedForRemoval(le))     unlink(le);
  }
  if (ice != null) {
    if (ice.isExpired()) {
      remove(k);
      ice=null;
    }
  }
  return ice != null;
}
