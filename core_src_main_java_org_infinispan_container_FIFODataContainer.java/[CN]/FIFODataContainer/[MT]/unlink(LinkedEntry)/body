{
  if (entry == head || entry == tail)   return;
  for (; ; ) {
    LinkedEntry next=entry.n;
    if (isMarkedForRemoval(next))     return;
    LinkedEntry prev;
    if (markNextReference(entry)) {
      next=entry.n;
      while (true) {
        prev=entry.p;
        if (isMarkedForRemoval(prev) || markPrevReference(entry)) {
          prev=entry.p;
          break;
        }
      }
      prev=correctPrev(prev.p,next.n);
    }
  }
}
