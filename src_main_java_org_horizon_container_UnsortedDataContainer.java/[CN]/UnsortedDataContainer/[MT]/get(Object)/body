{
  CachedValue cv=immortalData.get(k);
  if (cv != null) {
    cv.touch();
    return cv.getValue();
  }
 else {
    ExpirableCachedValue ecv=expirableData.get(k);
    if (ecv != null) {
      if (ecv.isExpired()) {
        expire(k);
      }
 else {
        ecv.touch();
        return ecv.getValue();
      }
    }
  }
  return null;
}
