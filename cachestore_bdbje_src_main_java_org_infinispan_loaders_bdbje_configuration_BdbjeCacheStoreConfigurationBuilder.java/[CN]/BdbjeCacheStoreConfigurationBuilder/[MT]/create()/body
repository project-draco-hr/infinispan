{
  return new BdbjeCacheStoreConfiguration(location,lockAcquistionTimeout,maxTxRetries,cacheDbNamePrefix,catalogDbName,expiryDbPrefix,environmentPropertiesFile,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
