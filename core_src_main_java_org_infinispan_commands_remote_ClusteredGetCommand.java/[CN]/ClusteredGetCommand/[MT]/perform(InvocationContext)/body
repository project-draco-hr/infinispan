{
  if (key != null) {
    InternalCacheEntry cacheEntry=dataContainer.get(key);
    if (cacheEntry == null) {
      context.setOriginLocal(false);
      if (cacheLoaderManager != null && cacheLoaderManager.getCacheLoader() != null)       cacheEntry=cacheLoaderManager.getCacheLoader().load(key);
    }
    return cacheEntry;
  }
 else {
    throw new CacheException("Invalid command. Missing key!");
  }
}
