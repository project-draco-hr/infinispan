{
  int l=name.length();
  int s=name.charAt(0) == '-' ? 1 : 0;
  if (l - s > 10) {
    log.cacheLoaderIgnoringUnexpectedFile(dir,name);
    return false;
  }
  for (int i=s; i < l; i++) {
    char c=name.charAt(i);
    if (c < '0' || c > '9') {
      log.cacheLoaderIgnoringUnexpectedFile(dir,name);
      return false;
    }
  }
  return true;
}
