{
  try {
    InternalCacheEntry ice=unmarshall(tree.get(key),key);
    if (ice != null && ice.isExpired(System.currentTimeMillis())) {
      remove(key);
      return null;
    }
    return ice;
  }
 catch (  IOException e) {
    throw new CacheLoaderException(e);
  }
catch (  ClassNotFoundException e) {
    throw new CacheException(e);
  }
}
