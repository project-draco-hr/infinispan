{
  if (!totalOrder) {
    asyncTransportExecutor.submit(new Runnable(){
      @Override public void run(){
        gcr.wireDependencies(command);
        try {
          command.perform(null);
        }
 catch (        Throwable throwable) {
        }
      }
    }
);
  }
  transport.invokeRemotely(null,command,ResponseMode.ASYNCHRONOUS_WITH_SYNC_MARSHALLING,timeout,true,null,totalOrder,isDistributed);
}
