{
  boolean notWrapped=false;
  for (  Object key : command.getMap().keySet()) {
    if (cll.localNodeIsOwner(key)) {
      lockKey(ctx,key);
      entryFactory.wrapEntryForPut(ctx,key,null,true);
      notWrapped=true;
    }
  }
  if (notWrapped)   invokeNextInterceptor(ctx,command);
  return null;
}
