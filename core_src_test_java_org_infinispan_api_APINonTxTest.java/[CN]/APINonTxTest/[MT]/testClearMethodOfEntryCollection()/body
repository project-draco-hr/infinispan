{
  final String key1="1", value1="one", key2="2", value2="two", key3="3", value3="three";
  Map<String,String> m=new HashMap<String,String>();
  m.put(key1,value1);
  m.put(key2,value2);
  m.put(key3,value3);
  cache.putAll(m);
  Set<Map.Entry<Object,Object>> entries=cache.entrySet();
  entries.clear();
  assertEquals(0,cache.size());
}
