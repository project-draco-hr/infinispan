{
  while (server1.getCacheManager("clustered").getDefaultCache().getNumberOfEntries() != 0 || server2.getCacheManager("clustered").getDefaultCache().getNumberOfEntries() != 0) {
    try {
      distCache.clear();
      Thread.sleep(200);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
}
