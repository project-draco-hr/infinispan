{
  return getDefaultClusteredConfig(Configuration.CacheMode.DIST_SYNC,true).fluent().transaction().transactionManagerLookupClass(RecoveryDummyTransactionManagerLookup.class).recovery().locking().useLockStriping(false).clustering().hash().numOwners(2).rehashEnabled(false).clustering().l1().disable().stateRetrieval().fetchInMemoryState(false).build();
