{
  return getDefaultClusteredConfig(Configuration.CacheMode.DIST_SYNC,false).fluent().transaction().transactionManagerLookupClass(DummyTransactionManagerLookup.class).recovery().locking().useLockStriping(false).clustering().hash().numOwners(2).rehashEnabled(false).clustering().l1().disable().stateRetrieval().fetchInMemoryState(false).build();
