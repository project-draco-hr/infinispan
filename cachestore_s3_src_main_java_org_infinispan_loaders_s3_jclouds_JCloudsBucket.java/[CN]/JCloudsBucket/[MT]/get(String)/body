{
  InputStream input=null;
  try {
    input=map.get(key);
    if (input != null) {
      return bucketFromStream(key,input);
    }
    return null;
  }
 catch (  Exception e) {
    throw connection.convertToS3ConnectionException("Error while reading from object: " + key,e);
  }
 finally {
    safeClose(input);
  }
}
