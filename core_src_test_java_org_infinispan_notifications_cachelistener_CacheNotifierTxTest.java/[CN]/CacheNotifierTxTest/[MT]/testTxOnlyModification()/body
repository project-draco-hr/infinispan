{
  initCacheData("key","value");
  tm.begin();
  cache.put("key","value2");
  tm.commit();
  expectTransactionBoundaries(true);
  verify(mockNotifier).notifyCacheEntryModified(eq("key"),eq("value"),eq(false),eq(true),isA(InvocationContext.class),isA(PutKeyValueCommand.class));
  verify(mockNotifier).notifyCacheEntryModified(eq("key"),eq("value2"),eq(false),eq(false),isA(InvocationContext.class),((FlagAffectedCommand)isNull()));
}
