def guess_view(fn):
    all_views_re = re.compile('.*Received new cluster view.*\\|([0-9]+). \\[(.*)\\].*')
    views = {}
    with open(fn) as f:
        for l in f:
            m = all_views_re.match(l)
            if m:
                view_num = m.group(1)
                members = m.group(2)
                views[view_num] = as_list(members)
    return views
