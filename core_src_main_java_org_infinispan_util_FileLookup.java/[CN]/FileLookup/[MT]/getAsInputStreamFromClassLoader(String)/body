{
  ClassLoader cl=Thread.currentThread().getContextClassLoader();
  InputStream is;
  try {
    is=cl == null ? null : cl.getResourceAsStream(filename);
  }
 catch (  RuntimeException re) {
    is=null;
  }
  if (is == null) {
    try {
      is=getClass().getClassLoader().getResourceAsStream(filename);
    }
 catch (    RuntimeException re) {
      is=null;
    }
  }
  return is;
}
