{
  URL u;
  try {
    u=cl == null ? null : cl.getResource(filename);
  }
 catch (  RuntimeException re) {
    u=null;
  }
  if (u == null) {
    try {
      u=getClass().getClassLoader().getResource(filename);
    }
 catch (    RuntimeException re) {
      u=null;
    }
  }
  if (u == null) {
    File f=new File(filename);
    if (f.exists())     try {
      u=f.toURI().toURL();
    }
 catch (    MalformedURLException e) {
    }
  }
  return u;
}
