{
  ClassLoader[] cls=new ClassLoader[]{userClassLoader,Util.class.getClassLoader(),ClassLoader.getSystemClassLoader()};
  for (  ClassLoader cl : cls) {
    if (cl == null)     continue;
    try {
      return cl.getResourceAsStream(filename);
    }
 catch (    RuntimeException e) {
    }
  }
  return null;
}
