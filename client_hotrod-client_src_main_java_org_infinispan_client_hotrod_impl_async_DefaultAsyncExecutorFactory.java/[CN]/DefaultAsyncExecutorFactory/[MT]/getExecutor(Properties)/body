{
  readParams(p);
  ThreadFactory tf=new ThreadFactory(){
    public Thread newThread(    Runnable r){
      Thread th=new Thread(r,THREAD_NAME + "-" + counter.getAndIncrement());
      th.setDaemon(true);
      return th;
    }
  }
;
  return new ThreadPoolExecutor(poolSize,poolSize,0L,TimeUnit.MILLISECONDS,new LinkedBlockingQueue<Runnable>(queueSize),tf);
}
