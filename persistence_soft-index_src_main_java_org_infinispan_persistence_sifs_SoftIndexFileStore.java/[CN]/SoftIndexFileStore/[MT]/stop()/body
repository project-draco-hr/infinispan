{
  try {
    logAppender.stopOperations();
    logAppender=null;
    compactor.stopOperations();
    compactor=null;
    index.stopOperations();
    index=null;
    fileProvider.stop();
    fileProvider=null;
    temporaryTable=null;
    indexQueue=null;
    storeQueue=null;
  }
 catch (  InterruptedException e) {
    throw new PersistenceException("Cannot stop cache store",e);
  }
}
