{
  if (dldEnabled) {
    if (recoveryEnabled) {
      return new RecoveryAwareDldGlobalTransaction();
    }
 else {
      return new DldGlobalTransaction();
    }
  }
 else {
    if (recoveryEnabled) {
      return new RecoveryAwareGlobalTransaction();
    }
 else {
      return new GlobalTransaction();
    }
  }
}
