{
  if (value != null && !deltas.isEmpty()) {
    for (    Delta delta : deltas) {
      delta.merge(value);
    }
    value.commit();
    if (wrappedEntry != null) {
      wrappedEntry.setChanged(true);
    }
  }
  reset();
  if (wrappedEntry != null) {
    wrappedEntry.commit(container,metadata);
  }
}
