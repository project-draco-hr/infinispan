{
  for (  Entry<KOut,VOut> entry : mapReceived.entrySet()) {
    List<VOut> l=null;
    if (!finalReduced.containsKey(entry.getKey())) {
      l=new LinkedList<VOut>();
      finalReduced.put(entry.getKey(),l);
    }
 else {
      l=finalReduced.get(entry.getKey());
    }
    l.add(entry.getValue());
  }
}
