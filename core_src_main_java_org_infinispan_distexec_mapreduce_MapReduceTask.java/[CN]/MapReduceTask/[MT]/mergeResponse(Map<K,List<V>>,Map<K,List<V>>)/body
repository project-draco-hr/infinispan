{
  for (  Entry<K,List<V>> entry : m.entrySet()) {
synchronized (result) {
      List<V> list=result.get(entry.getKey());
      if (list != null) {
        list.addAll(entry.getValue());
      }
 else {
        list=new ArrayList<V>();
        list.addAll(entry.getValue());
      }
      result.put(entry.getKey(),list);
    }
  }
}
