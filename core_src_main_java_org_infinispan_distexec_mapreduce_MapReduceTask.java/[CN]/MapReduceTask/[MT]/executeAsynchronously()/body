{
  final Callable<Map<KOut,VOut>> call=new Callable<Map<KOut,VOut>>(){
    @Override public Map<KOut,VOut> call() throws Exception {
      return execute();
    }
  }
;
  return new AbstractInProcessFuture<Map<KOut,VOut>>(){
    @Override public Map<KOut,VOut> get() throws InterruptedException, ExecutionException {
      try {
        return call.call();
      }
 catch (      Exception e) {
        throw new ExecutionException(e);
      }
    }
  }
;
}
