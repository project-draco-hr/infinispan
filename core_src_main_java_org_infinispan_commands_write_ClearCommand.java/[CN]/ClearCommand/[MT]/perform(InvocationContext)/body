{
  for (  CacheEntry e : ctx.getLookedUpEntries().values()) {
    if (e instanceof MVCCEntry) {
      MVCCEntry me=(MVCCEntry)e;
      Object k=me.getKey(), v=me.getValue();
      notifier.notifyCacheEntryRemoved(k,v,v,true,ctx,this);
      me.setRemoved(true);
      me.setValid(false);
    }
  }
  return null;
}
