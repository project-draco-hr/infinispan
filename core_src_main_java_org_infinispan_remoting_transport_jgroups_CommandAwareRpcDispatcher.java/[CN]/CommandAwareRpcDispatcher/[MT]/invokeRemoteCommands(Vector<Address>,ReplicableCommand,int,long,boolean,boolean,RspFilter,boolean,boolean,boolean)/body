{
  ReplicationTask task=new ReplicationTask(command,oob,dests,mode,timeout,anycasting,filter,supportReplay,broadcast);
  if (asyncMarshalling) {
    asyncExecutor.submit(task);
    return null;
  }
 else {
    RspList response;
    try {
      response=task.call();
    }
 catch (    Exception e) {
      throw new CacheException(e);
    }
    if (mode == GroupRequest.GET_NONE)     return null;
    if (response.isEmpty() || containsOnlyNulls(response))     return null;
 else     return response;
  }
}
