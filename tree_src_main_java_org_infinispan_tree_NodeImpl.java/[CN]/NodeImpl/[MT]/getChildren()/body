{
  startAtomic();
  try {
    Set<Node<K,V>> result=new HashSet<Node<K,V>>();
    for (    Fqn f : getStructure().values()) {
      NodeImpl<K,V> n=new NodeImpl<K,V>(f,cache,batchContainer,icc);
      result.add(n);
    }
    return Immutables.immutableSetWrap(result);
  }
  finally {
    endAtomic();
  }
}
