{
  ConfigurationBuilder cb=getDefaultClusteredCacheConfig(CacheMode.REPL_SYNC,true);
  cb.indexing().enable().indexLocalOnly(false).addProperty("default.directory_provider","filesystem").addProperty("default.indexBase",TMP_DIR + File.separator + indexName).addProperty("lucene_version","LUCENE_CURRENT");
  return cb;
}
