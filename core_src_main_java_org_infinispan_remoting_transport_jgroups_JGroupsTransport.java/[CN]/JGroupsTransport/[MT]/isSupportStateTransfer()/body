{
  ProtocolStack stack;
  if (channel != null && (stack=channel.getProtocolStack()) != null) {
    if (stack.findProtocol(STREAMING_STATE_TRANSFER.class) == null) {
      log.streamingStateTransferNotPresent();
      return false;
    }
  }
 else {
    log.channelNotSetUp();
    return false;
  }
  return true;
}
