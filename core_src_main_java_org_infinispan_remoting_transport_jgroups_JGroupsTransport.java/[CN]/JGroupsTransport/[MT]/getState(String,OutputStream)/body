{
  if (trace)   log.tracef("Received request to generate state for cache named '%s'.  Attempting to generate state.",cacheName);
  try {
    inboundInvocationHandler.generateState(cacheName,ostream);
  }
 catch (  StateTransferException e) {
    log.errorGeneratingState(e);
  }
 finally {
    Util.flushAndCloseStream(ostream);
  }
}
