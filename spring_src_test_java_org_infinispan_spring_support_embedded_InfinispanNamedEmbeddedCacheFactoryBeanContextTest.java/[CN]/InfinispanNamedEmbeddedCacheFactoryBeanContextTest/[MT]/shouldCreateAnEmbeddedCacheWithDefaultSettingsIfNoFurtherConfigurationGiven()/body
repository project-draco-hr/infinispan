{
  final Cache<Object,Object> infinispanNamedEmbeddedCacheWithoutFurtherConfiguration=this.applicationContext.getBean(Infinispan_NAMED_EMBEDDED_CACHE_WITHOUT_FURTHER_CONFIGURATION_BEAN_NAME,Cache.class);
  assertNotNull("Spring application context should contain a named Infinispan cache having bean name = \"" + Infinispan_NAMED_EMBEDDED_CACHE_WITHOUT_FURTHER_CONFIGURATION_BEAN_NAME + "\". However, it doesn't.",infinispanNamedEmbeddedCacheWithoutFurtherConfiguration);
}
