{
  DummyInMemoryStore cl=new DummyInMemoryStore();
  final DummyInMemoryStoreConfigurationBuilder loader=TestCacheManagerFactory.getDefaultCacheConfiguration(false).persistence().addStore(DummyInMemoryStoreConfigurationBuilder.class);
  loader.storeName(getClass().getName());
  cl.init(new DummyLoaderContext(loader.create(),getCache(),getMarshaller()));
  cl.start();
  csc=loader.create();
  return cl;
}
