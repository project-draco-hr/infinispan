{
  File[] files=dir.listFiles();
  if (files == null)   return 0;
  int count=0;
  for (  File file : files) {
    if (file.isDirectory())     count+=removeDir(file);
 else     if (file.isFile()) {
      String key=file.getPath();
      cache.remove(key);
      count++;
    }
  }
  return count;
}
