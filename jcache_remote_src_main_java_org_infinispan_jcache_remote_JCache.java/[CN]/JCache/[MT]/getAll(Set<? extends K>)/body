{
  checkNotClosed();
  checkNotNullOrNullElement(keys,"keys");
  if (keys.isEmpty()) {
    return InfinispanCollections.emptyMap();
  }
  Map<K,V> result=new HashMap<K,V>(keys.size());
  for (  K key : keys) {
    V value=get(key);
    if (value != null) {
      result.put(key,value);
    }
  }
  return result;
}
