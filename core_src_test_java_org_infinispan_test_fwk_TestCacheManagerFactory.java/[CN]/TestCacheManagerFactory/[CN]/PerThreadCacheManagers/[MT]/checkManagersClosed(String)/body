{
  for (  String cmName : cacheManagers.keySet()) {
    EmbeddedCacheManager cm=cacheManagers.get(cmName);
    if (cm.getStatus().allowInvocations()) {
      String thName=Thread.currentThread().getName();
      String errorMessage='\n' + "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!\n" + "!!!!!! ("+ thName+ ") Exiting because "+ testName+ " has NOT shut down all the cache managers it has started !!!!!!!\n"+ "!!!!!! ("+ thName+ ") The still-running cacheManager was created here: "+ cmName+ " !!!!!!!\n"+ "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!\n";
      log.error(errorMessage);
      System.err.println(errorMessage);
      System.exit(9);
    }
  }
  cacheManagers.clear();
}
