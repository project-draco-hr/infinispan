{
  if (!transactional) {
    builder.transaction().transactionMode(TransactionMode.NON_TRANSACTIONAL);
  }
 else {
    builder.transaction().transactionMode(TransactionMode.TRANSACTIONAL).transactionManagerLookup((TransactionManagerLookup)Util.getInstance(TransactionSetup.getManagerLookup(),TestCacheManagerFactory.class.getClassLoader()));
    if (!Util.isOSGiContext()) {
      JBossTransactionsUtils.setVolatileStores();
    }
  }
}
