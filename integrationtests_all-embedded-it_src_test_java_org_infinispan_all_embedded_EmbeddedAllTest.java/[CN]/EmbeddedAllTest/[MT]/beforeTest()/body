{
  GlobalConfiguration globalConfiguration=GlobalConfigurationBuilder.defaultClusteredBuilder().globalJmxStatistics().allowDuplicateDomains(true).transport().nodeName("node1").build();
  GlobalConfiguration globalConfiguration2=GlobalConfigurationBuilder.defaultClusteredBuilder().globalJmxStatistics().allowDuplicateDomains(true).transport().nodeName("node2").build();
