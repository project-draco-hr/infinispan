{
  return new JdbcStringBasedCacheStoreConfiguration(key2StringMapper,table.create(),driverClass,connectionUrl,username,password,connectionFactoryClass,datasource,lockAcquistionTimeout,lockConcurrencyLevel,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
