{
  return new JdbcStringBasedCacheStoreConfiguration(key2StringMapper,table.create(),connectionFactory.create(),lockAcquistionTimeout,lockConcurrencyLevel,purgeOnStartup,purgeSynchronously,purgerThreads,fetchPersistentState,ignoreModifications,TypedProperties.toTypedProperties(properties),async.create(),singletonStore.create());
