{
  ArrayList<RpcDispatcherServer> servers=new ArrayList<RpcDispatcherServer>();
  for (int i=0; i < 5; i++) {
    RpcDispatcherServer server=createChannel(i);
    servers.add(server);
  }
  for (int i=0; i < 5; i++) {
    RpcDispatcherServer server=servers.get(i);
    server.ch.connect("ConcurrentChannelStartTest");
  }
}
