{
  int size=ImmutableListCopy.this.elements.length;
  if (fromIndex < 0 || toIndex > size || fromIndex > toIndex)   throw new IllegalArgumentException("fromIndex(" + fromIndex + "), toIndex("+ toIndex+ "), size ("+ size+ "), List="+ ImmutableListCopy.this.toString());
  offset=fromIndex;
  this.size=toIndex - fromIndex;
}
