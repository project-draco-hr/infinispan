{
  ClassLoader cl=Thread.currentThread().getContextClassLoader();
  URL u=cl == null ? null : cl.getResource(filename);
  if (u == null) {
    u=getClass().getClassLoader().getResource(filename);
  }
  if (u == null) {
    File f=new File(filename);
    if (f.exists())     try {
      u=f.toURI().toURL();
    }
 catch (    MalformedURLException e) {
    }
  }
  return u;
}
