{
  if (keys != null) {
    Set<Cache<?,?>> cachesInTx=new HashSet<Cache<?,?>>();
    for (    Object k : keys) {
      cachesInTx.addAll(Arrays.asList(getOwners(k)));
    }
    for (    Cache<?,?> c : cachesInTx) {
      listenerLookup.get(c).expectAnyWithTx();
      listenerLookup.get(c).waitForRpc();
    }
  }
}
