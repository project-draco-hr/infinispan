{
  List<MagicKey> keys=new ArrayList<MagicKey>(Arrays.asList(new MagicKey(c1,"k1"),new MagicKey(c2,"k2"),new MagicKey(c3,"k3"),new MagicKey(c4,"k4")));
  int i=0;
  for (  Cache<Object,String> c : caches)   c.put(keys.get(i++),"v" + i);
  i=0;
  for (  MagicKey key : keys)   assertOnAllCachesAndOwnership(key,"v" + ++i);
  log.info("Initialized with keys %s",keys);
  return keys;
}
