{
  if (callbackHandle != null) {
    callbackHandle.remove();
  }
  try {
    hostInjector.getValue().getHost().removeChild(context);
    context.stop();
  }
 catch (  LifecycleException e) {
    ROOT_LOGGER.contextStopFailed(e);
  }
  try {
    context.destroy();
  }
 catch (  Exception e) {
    ROOT_LOGGER.contextDestroyFailed(e);
  }
}
