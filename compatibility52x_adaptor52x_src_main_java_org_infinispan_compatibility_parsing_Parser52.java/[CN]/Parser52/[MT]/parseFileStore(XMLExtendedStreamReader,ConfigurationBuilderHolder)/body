{
  ConfigurationBuilder builder=holder.getCurrentConfigurationBuilder();
  SingleFileStoreConfigurationBuilder fcscb=builder.persistence().addSingleFileStore();
  for (int i=0; i < reader.getAttributeCount(); i++) {
    ParseUtils.requireNoNamespaceAttribute(reader,i);
    String value=replaceProperties(reader.getAttributeValue(i));
    Attribute attribute=Attribute.forName(reader.getAttributeLocalName(i));
switch (attribute) {
case LOCATION:
      fcscb.location(value);
    break;
case FSYNC_INTERVAL:
  log.trace(Attribute.FSYNC_INTERVAL + " is deprecated and ignored in this version of the product.");
break;
case FSYNC_MODE:
log.trace(Attribute.FSYNC_MODE + " is deprecated and ignored in this version of the product.");
break;
case STREAM_BUFFER_SIZE:
log.trace(Attribute.STREAM_BUFFER_SIZE + " is deprecated and ignored in this version of the product.");
break;
default :
parseLockSupportStoreAttributes(reader,i,fcscb);
break;
}
}
parseStoreChildren(reader,fcscb,builder.persistence());
}
