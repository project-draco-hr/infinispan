{
  if (isFlagSet(CHANGED)) {
    if (trace)     log.trace("Updating entry [" + getKey() + "].  deleted="+ isDeleted()+ " valid="+ isValid()+ " changed="+ isChanged()+ " created="+ isFlagSet(CREATED)+ " value="+ value);
    if (isFlagSet(DELETED)) {
      container.remove(key);
    }
 else {
      container.put(key,value,lifespan);
    }
    reset();
  }
}
