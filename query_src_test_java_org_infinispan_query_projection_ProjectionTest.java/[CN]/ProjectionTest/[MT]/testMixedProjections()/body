{
  Foo foo=new Foo("bar1","baz4");
  cache.put("1",foo);
  CacheQuery cacheQuery=createProjectionQuery(ProjectionConstants.KEY,ProjectionConstants.VALUE,ProjectionConstants.VALUE,org.hibernate.search.ProjectionConstants.OBJECT_CLASS,"baz","bar");
  assertQueryReturns(cacheQuery,new Object[]{"1",foo,foo,foo.getClass(),foo.baz,foo.bar});
}
