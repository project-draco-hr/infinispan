{
  ((BoundedEquivalentConcurrentHashMapV8<K,InternalCacheEntry<K,V>>)entries).compute(newEntry.getKey(),new BoundedEquivalentConcurrentHashMapV8.BiFun<K,InternalCacheEntry<K,V>,InternalCacheEntry<K,V>>(){
    @Override public InternalCacheEntry<K,V> apply(    K key,    InternalCacheEntry<K,V> entry){
      activator.onUpdate(key,entry == null);
      return newEntry;
    }
  }
);
}
