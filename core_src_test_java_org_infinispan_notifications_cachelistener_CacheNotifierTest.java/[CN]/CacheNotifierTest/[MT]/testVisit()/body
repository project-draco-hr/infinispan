{
  initCacheData("key","value");
  mockNotifier.notifyCacheEntryVisited(eq("key"),eq(true),isA(InvocationContext.class));
  expectLastCall().once();
  mockNotifier.notifyCacheEntryVisited(eq("key"),eq(false),isA(InvocationContext.class));
  expectLastCall().once();
  replay(mockNotifier);
  cache.get("key");
  verify(mockNotifier);
}
