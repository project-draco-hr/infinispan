{
  initCacheData(Collections.singletonMap("key","value"));
  cache.get("key");
  verify(mockNotifier).notifyCacheEntryVisited(eq("key"),eq("value"),eq(true),isA(InvocationContext.class),argThat(matcher));
  verify(mockNotifier).notifyCacheEntryVisited(eq("key"),eq("value"),eq(false),isA(InvocationContext.class),argThat(matcher));
}
