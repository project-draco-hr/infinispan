{
  verify(getMockNotifier(cache)).notifyCacheEntryCreated(eq(key),eq(value),isNotNull(Metadata.class),eq(true),isA(InvocationContext.class),argThat(matcher));
  verify(getMockNotifier(cache)).notifyCacheEntryCreated(eq(key),eq(value),isNotNull(Metadata.class),eq(false),isA(InvocationContext.class),argThat(matcher));
