{
  Map<String,String> data=new HashMap<String,String>();
  data.put("key","value");
  data.put("key2","value2");
  initCacheData(data);
  cache.remove("key2");
  verify(mockNotifier).notifyCacheEntryRemoved(eq("key2"),eq("value2"),any(Metadata.class),eq(true),isA(InvocationContext.class),argThat(matcher));
  verify(mockNotifier).notifyCacheEntryRemoved(eq("key2"),eq("value2"),any(Metadata.class),eq(false),isA(InvocationContext.class),argThat(matcher));
}
