{
  initCacheData(Collections.singletonMap("key","value"));
  cache.put("key","value2");
  verify(mockNotifier).notifyCacheEntryModified(eq("key"),eq("value"),eq(false),eq(true),isA(InvocationContext.class),argThat(matcher));
  cache.put("key","value2");
  verify(mockNotifier).notifyCacheEntryModified(eq("key"),eq("value2"),eq(false),eq(true),isA(InvocationContext.class),argThat(matcher));
  verify(mockNotifier,times(2)).notifyCacheEntryModified(eq("key"),eq("value2"),eq(false),eq(false),isA(InvocationContext.class),argThat(matcher));
}
