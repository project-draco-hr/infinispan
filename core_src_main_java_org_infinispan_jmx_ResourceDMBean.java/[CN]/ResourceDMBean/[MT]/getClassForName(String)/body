{
  try {
    Class<?> c=Util.loadClass(name);
    return c;
  }
 catch (  ClassNotFoundException cnfe) {
    for (int i=0; i < primitives.length; i++) {
      if (name.equals(primitives[i].getName())) {
        return primitives[i];
      }
    }
  }
  throw new ClassNotFoundException("Class " + name + " cannot be found");
}
