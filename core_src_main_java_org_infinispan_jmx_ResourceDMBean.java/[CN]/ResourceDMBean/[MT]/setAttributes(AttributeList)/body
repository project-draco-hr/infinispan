{
  AttributeList results=new AttributeList();
  for (  Object aList : list) {
    Attribute attr=(Attribute)aList;
    try {
      setNamedAttribute(attr);
      results.add(attr);
    }
 catch (    Exception e) {
      log.failedToUpdateAtribute(attr.getName(),attr.getValue());
    }
  }
  return results;
}
