{
  InetAddress mcast_addr=InetAddress.getByName("228.10.10.5");
  SocketAddress saddr=new InetSocketAddress(mcast_addr,43589);
  MulticastSocket retval=null;
  try {
    success=false;
    retval=new MulticastSocket(saddr);
    success=true;
  }
  finally {
    if (retval != null) {
      try {
        retval.close();
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
}
