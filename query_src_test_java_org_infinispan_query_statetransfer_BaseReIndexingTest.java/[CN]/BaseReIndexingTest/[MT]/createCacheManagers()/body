{
  builder=getDefaultClusteredCacheConfig(CacheMode.REPL_SYNC,false);
  builder.indexing().enable().indexLocalOnly(false).addProperty("hibernate.search.default.directory_provider","ram").addProperty("hibernate.search.lucene_version","LUCENE_CURRENT");
  configureCache(builder);
  createClusteredCaches(2,builder);
}
