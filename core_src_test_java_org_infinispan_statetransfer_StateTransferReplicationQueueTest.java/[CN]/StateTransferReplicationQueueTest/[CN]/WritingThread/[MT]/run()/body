{
  int c=0;
  while (!stop) {
    try {
      if (tx)       tm.begin();
      cache.put("test" + c,new PojoValue(c));
      cache.remove("test" + c);
      c++;
      if (tx)       tm.commit();
      if (c % 1000 == 0)       TestingUtil.sleepThread(1);
    }
 catch (    Exception e) {
      stopThread();
    }
  }
  result=c;
}
