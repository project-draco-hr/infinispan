{
  if (children == null) {
    throw new IllegalArgumentException("No child found : " + attribute);
  }
  AttributeNode<AttributeId> child=children.get(attribute);
  if (child == null) {
    throw new IllegalArgumentException("No child found : " + attribute);
  }
  if (--child.refCount == 0) {
    children.remove(attribute);
    if (children.isEmpty() && parent != null) {
      parent.removeChild(attribute);
    }
  }
}
