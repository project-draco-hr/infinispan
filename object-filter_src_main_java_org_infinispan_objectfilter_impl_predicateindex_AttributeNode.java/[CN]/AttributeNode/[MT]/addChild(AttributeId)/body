{
  AttributeNode<AttributeId> child;
  if (children == null) {
    children=new HashMap<AttributeId,AttributeNode<AttributeId>>();
    child=new AttributeNode<AttributeId>(attribute,this);
    children.put(attribute,child);
  }
 else {
    child=children.get(attribute);
    if (child == null) {
      child=new AttributeNode<AttributeId>(attribute,this);
      children.put(attribute,child);
    }
  }
  return child;
}
