{
  if (validate) {
    validate();
  }
  return new Configuration(clustering.create(),customInterceptors.create(),dataContainer.create(),deadlockDetection.create(),eviction.create(),expiration.create(),indexing.create(),invocationBatching.create(),jmxStatistics.create(),loaders.create(),locking.create(),storeAsBinary.create(),transaction.create(),unsafe.create(),versioning.create(),classLoader);
}
