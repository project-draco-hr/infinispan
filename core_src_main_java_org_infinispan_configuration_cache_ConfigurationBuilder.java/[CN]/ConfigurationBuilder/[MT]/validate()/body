{
  for (  AbstractConfigurationChildBuilder<?> validatable : asList(clustering,dataContainer,deadlockDetection,eviction,expiration,indexing,invocationBatching,jmxStatistics,loaders,locking,storeAsBinary,transaction,versioning,unsafe)) {
    validatable.validate();
  }
  for (  Builder<?> m : modules) {
    m.validate();
  }
}
