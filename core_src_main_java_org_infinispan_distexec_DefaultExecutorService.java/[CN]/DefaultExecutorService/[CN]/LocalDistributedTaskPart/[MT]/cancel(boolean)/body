{
  if (!isCancelled()) {
    CancelCommand ccc=factory.buildCancelCommandCommand(distCommand.getUUID());
    ccc.init(cancellationService);
    try {
      ccc.perform(null);
    }
 catch (    Throwable e) {
      log.couldNotExecuteCancellationLocally(e.getLocalizedMessage());
    }
    setCancelled();
    return future.cancel(true);
  }
 else {
    return false;
  }
}
