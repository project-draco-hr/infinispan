{
  if (isLocalNodeExecutionTarget()) {
    invokeLocally();
  }
 else {
    if (trace)     log.tracef("Sending %s to remote execution at node %s",f,getExecutionTarget());
    try {
      rpc.invokeRemotelyInFuture(Collections.singletonList(getExecutionTarget()),getCommand(),rpc.getRpcOptionsBuilder(ResponseMode.SYNCHRONOUS).timeout(getOwningTask().timeout(),TimeUnit.MILLISECONDS).build(),(DistributedTaskPart<Object>)this);
    }
 catch (    Throwable e) {
      log.remoteExecutionFailed(getExecutionTarget(),e);
    }
  }
}
