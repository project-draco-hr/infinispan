{
  Matcher matcher=createMatcher();
  Object obj=createPerson1();
  final int[] matchCount=new int[1];
  for (int k=0; k < NUM_FILTERS; k++) {
    matcher.registerFilter(query,new FilterCallback(){
      @Override public void onFilterResult(      Object userContext,      Object instance,      Object eventType,      Object[] projection,      Comparable[] sortProjection){
        matchCount[0]++;
      }
    }
);
  }
  long stime=System.nanoTime();
  for (int i=0; i < ITERATIONS; i++) {
    matchCount[0]=0;
    matcher.match(null,obj,null);
    assertEquals(NUM_FILTERS,matchCount[0]);
  }
  return System.nanoTime() - stime;
}
