{
  StackTraceElement[] stackTraceElements=Thread.getAllStackTraces().get(Thread.currentThread());
  StringBuilder sb=new StringBuilder(initialLine);
  for (int i=3; i < 10; i++) {
    sb.append("\n\t");
    sb.append(stackTraceElements[i]);
  }
  sb.append("\n");
  System.out.println(sb.toString());
}
