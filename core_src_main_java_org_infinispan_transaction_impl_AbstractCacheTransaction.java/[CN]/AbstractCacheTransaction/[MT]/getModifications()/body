{
  if (hasLocalOnlyModifications) {
    List<WriteCommand> mods=new ArrayList<WriteCommand>();
    for (    WriteCommand cmd : modifications) {
      if (!cmd.hasFlag(Flag.CACHE_MODE_LOCAL)) {
        mods.add(cmd);
      }
    }
    return mods;
  }
 else {
    return getAllModifications();
  }
}
