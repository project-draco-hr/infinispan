{
  int size=keySet.size();
  for (  Object k : keySet) {
    if (!container.containsKey(k))     size--;
  }
  for (  CacheEntry e : lookedUpEntries.values()) {
    if (container.containsKey(e.getKey())) {
      if (e.isRemoved()) {
        size--;
      }
    }
 else     if (!e.isRemoved()) {
      size++;
    }
  }
  return Math.max(size,0);
}
