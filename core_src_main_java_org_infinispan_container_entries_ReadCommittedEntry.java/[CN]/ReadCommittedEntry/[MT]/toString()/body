{
  return getClass().getSimpleName() + "(" + Util.hexIdHashCode(this)+ "){"+ "key="+ toStr(key)+ ", value="+ toStr(value)+ ", oldValue="+ toStr(oldValue)+ ", isCreated="+ isCreated()+ ", isChanged="+ isChanged()+ ", isRemoved="+ isRemoved()+ ", isValid="+ isValid()+ ", skipRemoteGet="+ skipLookup()+ ", metadata="+ metadata+ '}';
}
