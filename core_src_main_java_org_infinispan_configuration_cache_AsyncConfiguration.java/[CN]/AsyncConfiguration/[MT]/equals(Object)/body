{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  AsyncConfiguration that=(AsyncConfiguration)o;
  if (asyncMarshalling != that.asyncMarshalling)   return false;
  if (replicationQueueInterval != that.replicationQueueInterval)   return false;
  if (replicationQueueMaxElements != that.replicationQueueMaxElements)   return false;
  if (useReplicationQueue != that.useReplicationQueue)   return false;
  if (replicationQueue != null ? !replicationQueue.equals(that.replicationQueue) : that.replicationQueue != null)   return false;
  return true;
}
