{
  Class<?> result=entityClass;
  for (  String propName : propertyPath) {
    Class elementType=ReflectionHelper.getElementType(result,propName);
    if (elementType != null) {
      result=elementType;
    }
 else {
      result=ReflectionHelper.getPropertyType(result,propName);
    }
    if (result == null) {
      break;
    }
  }
  return result;
}
