{
  int fetched=0;
  while (orderedValues.size() <= index || fetched < fetchSize) {
    ClusteredDoc scoreDoc=(ClusteredDoc)hq.pop();
    if (scoreDoc == null) {
      return;
    }
    ClusteredTopDocs topDoc=rechargeQueue(scoreDoc);
    Object value=fetchValue(scoreDoc,topDoc);
    orderedValues.add(value);
    fetched++;
  }
}
