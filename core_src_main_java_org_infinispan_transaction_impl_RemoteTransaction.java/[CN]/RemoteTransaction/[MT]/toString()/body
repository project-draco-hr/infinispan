{
  return "RemoteTransaction{" + "modifications=" + modifications + ", lookedUpEntries="+ lookedUpEntries+ ", lockedKeys="+ toStr(getLockedKeys())+ ", backupKeyLocks="+ toStr(getBackupLockedKeys())+ ", lookedUpEntriesTopology="+ lookedUpEntriesTopology+ ", isMarkedForRollback="+ isMarkedForRollback()+ ", tx="+ tx+ ", state="+ transactionState+ '}';
}
