{
  int dotIndex=cn.lastIndexOf(".");
  int dotIndexPlusOne=dotIndex + 1;
  String cname=cn;
  if (dotIndexPlusOne == cn.length())   cname=format(cn.substring(0,cn.length() - 1));
 else {
    if (dotIndex > -1 && cn.length() > dotIndexPlusOne) {
      cname=cn.substring(dotIndexPlusOne);
    }
    cname+="-thread";
  }
  return cname;
}
