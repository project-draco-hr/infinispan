{
  if (loadAllNonExpiredRowsSql == null) {
    loadAllNonExpiredRowsSql="SELECT " + config.dataColumnName() + ","+ config.idColumnName()+ ", "+ config.timestampColumnName()+ " FROM "+ getTableName()+ " WHERE "+ config.timestampColumnName()+ " > ? OR "+ config.timestampColumnName()+ " < 0";
  }
  return loadAllNonExpiredRowsSql;
}
