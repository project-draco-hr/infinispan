{
  if (deleteRowSql == null) {
    if (DatabaseType.SYBASE == getDatabaseType()) {
      deleteRowSql="DELETE FROM " + getTableName() + " WHERE "+ idColumnName+ " = convert("+ idColumnType+ ","+ "?)";
    }
 else {
      deleteRowSql="DELETE FROM " + getTableName() + " WHERE "+ idColumnName+ " = ?";
    }
  }
  return deleteRowSql;
}
