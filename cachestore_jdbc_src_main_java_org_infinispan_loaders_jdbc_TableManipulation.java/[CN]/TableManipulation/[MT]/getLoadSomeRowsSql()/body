{
  if (loadSomeRowsSql == null) {
switch (getDatabaseType()) {
case ORACLE:
      loadSomeRowsSql="SELECT " + dataColumnName + ","+ idColumnName+ " FROM "+ getTableName()+ " LIMIT ?";
    break;
case DB2:
  loadSomeRowsSql="SELECT " + dataColumnName + ","+ idColumnName+ " FROM "+ getTableName()+ " LIMIT ?";
break;
case INFORMIX:
case INTERBASE:
case FIREBIRD:
loadSomeRowsSql="SELECT " + dataColumnName + ","+ idColumnName+ " FROM "+ getTableName()+ " LIMIT ?";
break;
case SQL_SERVER:
case ACCESS:
loadSomeRowsSql="SELECT " + dataColumnName + ","+ idColumnName+ " FROM "+ getTableName()+ " LIMIT ?";
break;
default :
loadSomeRowsSql="SELECT " + dataColumnName + ","+ idColumnName+ " FROM "+ getTableName()+ " LIMIT ?";
break;
}
}
return loadSomeRowsSql;
}
